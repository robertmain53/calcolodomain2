    <script>
      // Core conversion logic
      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function getSelectedSize() {
        const radios = document.querySelectorAll(".size-radio");
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "medium";
      }

      function dogToHumanAge(dogYears, size) {
        const D = Math.max(0, dogYears);
        if (D <= 0) return 0;

        // First year: fast growth
        let human;
        if (D <= 1) {
          // Slightly size-adjusted first year
          const firstYearMap = {
            "toy-small": 15,
            medium: 16,
            large: 15,
            giant: 14,
          };
          const base = firstYearMap[size] || 16;
          human = base * D;
        } else if (D <= 2) {
          // Second year: still fast
          const secondYearAdd = {
            "toy-small": 9,
            medium: 9,
            large: 9,
            giant: 9,
          };
          const add = secondYearAdd[size] || 9;
          human =
            (size === "toy-small" ? 15 : size === "giant" ? 14 : 16) +
            add * (D - 1);
        } else {
          // After 2 years: size-dependent slope
          let base;
          if (size === "toy-small") base = 24; // ≈ 15 + 9
          else if (size === "giant") base = 23; // ≈ 14 + 9
          else base = 25; // medium/large

          const slope =
            {
              "toy-small": 4,
              medium: 5,
              large: 6,
              giant: 7,
            }[size] || 5;

          human = base + slope * (D - 2);
        }
        return human;
      }

      function dogToHumanSimpleSeven(dogYears) {
        return Math.max(0, dogYears) * 7;
      }

      function dogToHumanTwoStage(dogYears) {
        const D = Math.max(0, dogYears);
        if (D <= 0) return 0;
        if (D <= 1) return 15 * D;
        if (D <= 2) return 15 + 9 * (D - 1);
        return 24 + 4 * (D - 2);
      }

      function getLifeStage(humanAge) {
        if (humanAge < 15) return "Puppy";
        if (humanAge < 28) return "Young adult";
        if (humanAge < 50) return "Adult";
        return "Senior";
      }

      function getVetFrequency(humanAge) {
        if (humanAge < 15)
          return {
            freq: "Every 3–6 months",
            note: "Puppy vaccines and growth checks.",
          };
        if (humanAge < 50)
          return {
            freq: "Once a year",
            note: "Annual wellness exam and preventive care.",
          };
        return {
          freq: "Every 6 months",
          note: "Senior screening and closer monitoring.",
        };
      }

      function updateTimeline(size) {
        const el = document.getElementById("timeline-list");
        if (!el) return;
        let text;
        if (size === "toy-small") {
          text = `
        <li>Puppy: 0–1 years (≈ 0–15 human years)</li>
        <li>Young adult: 1–4 years (≈ 15–32 human years)</li>
        <li>Adult: 4–8 years (≈ 32–52 human years)</li>
        <li>Senior: 8+ years (≈ 52+ human years)</li>
      `;
        } else if (size === "giant") {
          text = `
        <li>Puppy: 0–1 years (≈ 0–14 human years)</li>
        <li>Young adult: 1–3 years (≈ 14–30 human years)</li>
        <li>Adult: 3–6 years (≈ 30–55 human years)</li>
        <li>Senior: 6+ years (≈ 55+ human years)</li>
      `;
        } else if (size === "large") {
          text = `
        <li>Puppy: 0–1 years (≈ 0–15 human years)</li>
        <li>Young adult: 1–3 years (≈ 15–30 human years)</li>
        <li>Adult: 3–7 years (≈ 30–55 human years)</li>
        <li>Senior: 7+ years (≈ 55+ human years)</li>
      `;
        } else {
          text = `
        <li>Puppy: 0–1 years (≈ 0–16 human years)</li>
        <li>Young adult: 1–3 years (≈ 16–30 human years)</li>
        <li>Adult: 3–7 years (≈ 30–50 human years)</li>
        <li>Senior: 7+ years (≈ 50+ human years)</li>
      `;
        }
        el.innerHTML = text;
      }

      function performCalculation() {
        const yearsInput = document.getElementById("dog-age-years");
        const monthsInput = document.getElementById("dog-age-months");
        if (!yearsInput || !monthsInput) return;

        let years = parseFloat(yearsInput.value || "0");
        let months = parseFloat(monthsInput.value || "0");
        if (isNaN(years)) years = 0;
        if (isNaN(months)) months = 0;
        months = clamp(months, 0, 11);

        const dogYears = years + months / 12;
        const size = getSelectedSize();

        const human = dogToHumanAge(dogYears, size);
        const humanRounded = Math.round(human);

        const oursEl = document.getElementById("human-age-main");
        const detailEl = document.getElementById("human-age-detail");
        if (oursEl) oursEl.textContent = `≈ ${humanRounded} years`;

        if (detailEl) {
          const sizeLabel =
            {
              "toy-small": "small",
              medium: "medium",
              large: "large",
              giant: "giant",
            }[size] || "medium";
          const dogAgeLabel = dogYears.toFixed(1).replace(/\.0$/, "");
          detailEl.textContent = `Based on a ${dogAgeLabel}-year-old ${sizeLabel} dog.`;
        }

        const lifeStage = getLifeStage(human);
        const lifeStageEl = document.getElementById("life-stage");
        const lifeStageNoteEl = document.getElementById("life-stage-note");
        if (lifeStageEl) lifeStageEl.textContent = lifeStage;
        if (lifeStageNoteEl) {
          let note = "";
          if (lifeStage === "Puppy") note = "Rapid growth and development.";
          else if (lifeStage === "Young adult")
            note = "High energy, finishing growth.";
          else if (lifeStage === "Adult") note = "Prime adult years.";
          else note = "More frequent monitoring recommended.";
          lifeStageNoteEl.textContent = note;
        }

        const vet = getVetFrequency(human);
        const vetFreqEl = document.getElementById("vet-frequency");
        const vetNoteEl = document.getElementById("vet-frequency-note");
        if (vetFreqEl) vetFreqEl.textContent = vet.freq;
        if (vetNoteEl) vetNoteEl.textContent = vet.note;

        // Comparison table
        const seven = dogToHumanSimpleSeven(dogYears);
        const twoStage = dogToHumanTwoStage(dogYears);
        const compareOurs = document.getElementById("compare-ours");
        const compareSeven = document.getElementById("compare-seven");
        const compareTwoStage = document.getElementById("compare-two-stage");
        if (compareOurs) compareOurs.textContent = humanRounded;
        if (compareSeven) compareSeven.textContent = Math.round(seven);
        if (compareTwoStage) compareTwoStage.textContent = Math.round(twoStage);

        updateTimeline(size);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculate-btn");
        if (calcBtn) calcBtn.addEventListener("click", performCalculation);

        // Recalculate on size change
        document.querySelectorAll(".size-radio").forEach((r) => {
          r.addEventListener("change", performCalculation);
        });

        // Toggle age input mode
        const toggleBtn = document.getElementById("toggle-age-mode");
        const ageYM = document.getElementById("age-input-ym");
        const ageBD = document.getElementById("age-input-birthdate");
        if (toggleBtn && ageYM && ageBD) {
          toggleBtn.addEventListener("click", () => {
            const usingBirthdate = !ageBD.classList.contains("hidden");
            if (usingBirthdate) {
              ageBD.classList.add("hidden");
              ageYM.classList.remove("hidden");
              toggleBtn.textContent = "Use birth date instead";
            } else {
              ageYM.classList.add("hidden");
              ageBD.classList.remove("hidden");
              toggleBtn.textContent = "Use years & months instead";
            }
          });
        }

        // Age from dates
        const calcFromDatesBtn = document.getElementById("calc-age-from-dates");
        if (calcFromDatesBtn) {
          calcFromDatesBtn.addEventListener("click", () => {
            const birthEl = document.getElementById("dog-birthdate");
            const asOfEl = document.getElementById("as-of-date");
            const infoEl = document.getElementById("age-from-dates-info");
            if (!birthEl || !asOfEl || !infoEl) return;

            const birthVal = birthEl.value;
            const asOfVal =
              asOfEl.value || new Date().toISOString().slice(0, 10);
            if (!birthVal) {
              infoEl.textContent = "Please enter your dog’s birth date.";
              infoEl.classList.remove("text-green-600");
              infoEl.classList.add("text-red-600");
              return;
            }

            const birth = new Date(birthVal);
            const asOf = new Date(asOfVal);
            if (
              isNaN(birth.getTime()) ||
              isNaN(asOf.getTime()) ||
              asOf <= birth
            ) {
              infoEl.textContent = "As-of date must be after the birth date.";
              infoEl.classList.remove("text-green-600");
              infoEl.classList.add("text-red-600");
              return;
            }

            const diffMs = asOf - birth;
            const diffDays = diffMs / (1000 * 60 * 60 * 24);
            const years = diffDays / 365.25;
            const wholeYears = Math.floor(years);
            const months = Math.round((years - wholeYears) * 12);

            const yearsInput = document.getElementById("dog-age-years");
            const monthsInput = document.getElementById("dog-age-months");
            if (yearsInput && monthsInput) {
              yearsInput.value = wholeYears;
              monthsInput.value = clamp(months, 0, 11);
            }

            infoEl.textContent = `Calculated age: about ${wholeYears} years and ${clamp(
              months,
              0,
              11
            )} months.`;
            infoEl.classList.remove("text-red-600");
            infoEl.classList.add("text-green-600");

            performCalculation();
          });
        }

        // Suggest size from weight
        const suggestBtn = document.getElementById("suggest-size");
        if (suggestBtn) {
          suggestBtn.addEventListener("click", () => {
            const weightEl = document.getElementById("dog-weight");
            const unitEl = document.getElementById("weight-unit");
            const infoEl = document.getElementById("suggest-size-info");
            if (!weightEl || !unitEl || !infoEl) return;

            let w = parseFloat(weightEl.value || "0");
            if (isNaN(w) || w <= 0) {
              infoEl.textContent = "Enter your dog’s weight first.";
              infoEl.classList.remove("text-green-600");
              infoEl.classList.add("text-red-600");
              return;
            }

            if (unitEl.value === "kg") {
              w = w * 2.20462; // convert to lb
            }

            let size = "medium";
            if (w < 20) size = "toy-small";
            else if (w < 50) size = "medium";
            else if (w < 90) size = "large";
            else size = "giant";

            document.querySelectorAll(".size-radio").forEach((r) => {
              r.checked = r.value === size;
            });

            const label = {
              "toy-small": "Toy / Small",
              medium: "Medium",
              large: "Large",
              giant: "Giant",
            }[size];

            infoEl.textContent = `Suggested size category: ${label}.`;
            infoEl.classList.remove("text-red-600");
            infoEl.classList.add("text-green-600");

            updateTimeline(size);
            performCalculation();
          });
        }

        // Basic FAQ toggle (in addition to global script_faq)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              if (!answer) return;
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              if (isHidden) answer.classList.remove("hidden");
            });
          });

        // Initial calculation
        performCalculation();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
