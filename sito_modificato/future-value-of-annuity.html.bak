    <script>
      // Utility formatters
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0.00";
        return value.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }
      function formatNumber(value, decimals = 0) {
        if (!isFinite(value)) return "0";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function calculateFutureValueAnnuity() {
        const payment = parseFloat(
          document.getElementById("paymentAmount").value
        );
        const annualRate = parseFloat(
          document.getElementById("annualRate").value
        );
        const years = parseFloat(document.getElementById("years").value);
        const paymentFreq = parseInt(
          document.getElementById("paymentFrequency").value,
          10
        );
        const compFreq = parseInt(
          document.getElementById("compoundingFrequency").value,
          10
        );
        const annuityType =
          document.querySelector('input[name="annuityType"]:checked')?.value ||
          "ordinary";

        if (
          !(payment > 0) ||
          !(annualRate >= 0) ||
          !(years > 0) ||
          !(paymentFreq > 0) ||
          !(compFreq > 0)
        ) {
          alert(
            "Please enter a positive payment, years, and non-negative interest rate."
          );
          return;
        }

        const totalYears = years;
        const totalCompPeriods = Math.round(totalYears * compFreq);
        const ratePerComp = annualRate / 100 / compFreq;

        // Effective rate per payment period using equivalent rate conversion
        const effRatePerYear = Math.pow(1 + ratePerComp, compFreq) - 1;
        const ratePerPayment =
          Math.pow(1 + effRatePerYear, 1 / paymentFreq) - 1;

        const totalPayments = Math.round(totalYears * paymentFreq);

        // Use closed-form formula with ratePerPayment
        let fvOrdinary;
        if (ratePerPayment === 0) {
          fvOrdinary = payment * totalPayments;
        } else {
          fvOrdinary =
            payment *
            ((Math.pow(1 + ratePerPayment, totalPayments) - 1) /
              ratePerPayment);
        }
        const fv =
          annuityType === "due"
            ? fvOrdinary * (1 + ratePerPayment)
            : fvOrdinary;

        const totalContrib = payment * totalPayments;
        const totalInterest = fv - totalContrib;

        document.getElementById("fvResult").textContent = formatCurrency(fv);
        document.getElementById("totalContributions").textContent =
          formatCurrency(totalContrib);
        document.getElementById("totalInterest").textContent =
          formatCurrency(totalInterest);
        document.getElementById("totalPayments").textContent = formatNumber(
          totalPayments,
          0
        );
        document.getElementById("fvTypeLabel").textContent =
          annuityType === "due"
            ? "Annuity due (payments at beginning of period)"
            : "Ordinary annuity (payments at end of period)";

        // Build year-by-year table using simulation for clarity
        const tbody = document.getElementById("growthTableBody");
        tbody.innerHTML = "";
        let balance = 0;
        let contribSoFar = 0;
        const paymentsPerYear = paymentFreq;
        const compPerYear = compFreq;
        const ratePerCompounding = ratePerComp;

        for (let year = 1; year <= Math.ceil(totalYears); year++) {
          const startYear = year - 1;
          const endYear = Math.min(year, totalYears);
          const yearDuration = endYear - startYear;
          const yearCompPeriods = Math.round(yearDuration * compPerYear);
          const yearPayments = Math.round(yearDuration * paymentsPerYear);

          // Simulate within the year at compounding steps
          const steps = yearCompPeriods;
          const paymentsPerComp = paymentsPerYear / compPerYear;

          for (let i = 0; i < steps; i++) {
            // Interest first
            balance *= 1 + ratePerCompounding;

            // Then payments within this compounding interval
            const paymentsThisStep = paymentsPerComp;
            if (paymentsThisStep > 0) {
              for (let p = 0; p < paymentsThisStep; p++) {
                if (annuityType === "due") {
                  // payment at beginning of sub-period: approximate as immediate
                  balance += payment;
                  contribSoFar += payment;
                } else {
                  // ordinary: approximate as end of sub-period, but within comp interval effect is small
                  balance += payment;
                  contribSoFar += payment;
                }
              }
            }
          }

          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2 text-gray-700">${year}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          contribSoFar
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          balance
        )}</td>
      `;
          tbody.appendChild(row);
        }

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const yearsInput = document.getElementById("years");
        const yearsSlider = document.getElementById("yearsSlider");
        const yearsSliderValue = document.getElementById("yearsSliderValue");
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        // Sync slider and numeric input
        function syncFromSlider() {
          yearsInput.value = yearsSlider.value;
          yearsSliderValue.textContent = yearsSlider.value;
        }
        function syncFromInput() {
          const val = parseFloat(yearsInput.value);
          if (isFinite(val)) {
            const clamped = Math.min(
              Math.max(val, parseFloat(yearsSlider.min)),
              parseFloat(yearsSlider.max)
            );
            yearsSlider.value = clamped;
            yearsSliderValue.textContent = clamped;
          }
        }

        yearsSlider.addEventListener("input", syncFromSlider);
        yearsInput.addEventListener("input", syncFromInput);

        // Default values
        document.getElementById("paymentAmount").value = 500;
        document.getElementById("annualRate").value = 6;
        yearsInput.value = 20;
        syncFromInput();

        calcBtn.addEventListener("click", calculateFutureValueAnnuity);

        resetBtn.addEventListener("click", function () {
          document.getElementById("paymentAmount").value = 500;
          document.getElementById("annualRate").value = 6;
          document.getElementById("years").value = 20;
          document.getElementById("paymentFrequency").value = "12";
          document.getElementById("compoundingFrequency").value = "12";
          document.querySelector(
            'input[name="annuityType"][value="ordinary"]'
          ).checked = true;
          yearsSlider.value = 20;
          yearsSliderValue.textContent = "20";
          document.getElementById("resultsSection").classList.add("hidden");
          document.getElementById("growthTableBody").innerHTML = "";
        });
      });
    </script>

