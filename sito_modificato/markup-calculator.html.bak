    <script>
      // Utility
      function round2(val) {
        return (Math.round((val + Number.EPSILON) * 100) / 100).toFixed(2);
      }

      function formatCurrency(val) {
        if (isNaN(val)) return "-";
        return "$" + round2(val);
      }

      function formatPercent(val) {
        if (isNaN(val)) return "-";
        return round2(val) + "%";
      }

      // Mode toggle
      const modeCostBtn = document.getElementById("mode-cost");
      const modePriceBtn = document.getElementById("mode-price");

      function setMode(mode) {
        if (mode === "cost") {
          modeCostBtn.classList.add("bg-blue-600", "text-white");
          modeCostBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          modePriceBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          modePriceBtn.classList.remove("bg-blue-600", "text-white");
        } else {
          modePriceBtn.classList.add("bg-blue-600", "text-white");
          modePriceBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          modeCostBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          modeCostBtn.classList.remove("bg-blue-600", "text-white");
        }
        document.getElementById("markup-form").dataset.mode = mode;
      }

      modeCostBtn.addEventListener("click", () => setMode("cost"));
      modePriceBtn.addEventListener("click", () => setMode("price"));
      setMode("cost");

      // Main calculation
      document.getElementById("calculate-btn").addEventListener("click", () => {
        const costInput = document.getElementById("cost");
        const priceInput = document.getElementById("price");
        const markupInput = document.getElementById("markup");
        const marginInput = document.getElementById("margin");
        const qtyInput = document.getElementById("quantity");
        const errorMsg = document.getElementById("error-msg");

        let cost = parseFloat(costInput.value);
        let price = parseFloat(priceInput.value);
        let markup = parseFloat(markupInput.value);
        let margin = parseFloat(marginInput.value);
        let qty = parseInt(qtyInput.value, 10);

        errorMsg.textContent = "";

        if (isNaN(qty) || qty <= 0) qty = 1;

        const mode =
          document.getElementById("markup-form").dataset.mode || "cost";

        try {
          if (mode === "cost") {
            if (isNaN(cost) || cost < 0) {
              throw new Error("Please enter a valid cost.");
            }

            if (!isNaN(markup)) {
              const m = markup / 100;
              price = cost * (1 + m);
            } else if (!isNaN(margin)) {
              const g = margin / 100;
              if (g >= 1) throw new Error("Margin % must be less than 100%.");
              price = cost / (1 - g);
            } else if (!isNaN(price)) {
              // derive from cost & price
            } else {
              throw new Error(
                "Enter either markup %, margin %, or selling price."
              );
            }
          } else {
            // mode = price
            if (isNaN(price) || price <= 0) {
              throw new Error("Please enter a valid selling price.");
            }

            if (!isNaN(markup)) {
              const m = markup / 100;
              cost = price / (1 + m);
            } else if (!isNaN(margin)) {
              const g = margin / 100;
              if (g >= 1) throw new Error("Margin % must be less than 100%.");
              cost = price * (1 - g);
            } else if (!isNaN(cost)) {
              // derive from cost & price
            } else {
              throw new Error("Enter either markup %, margin %, or cost.");
            }
          }

          if (isNaN(cost) || isNaN(price)) {
            throw new Error(
              "Not enough information to calculate. Please fill at least two fields."
            );
          }
          if (cost < 0 || price < 0) {
            throw new Error("Cost and price must be non-negative.");
          }
          if (cost === 0 && price === 0) {
            throw new Error("Cost and price cannot both be zero.");
          }

          const profit = price - cost;

          // Compute markup and margin if missing
          const markupPct = cost === 0 ? NaN : (profit / cost) * 100;
          const marginPct = price === 0 ? NaN : (profit / price) * 100;

          // Totals
          const totalCost = cost * qty;
          const totalRevenue = price * qty;
          const totalProfit = profit * qty;

          // Update inputs with normalized values
          costInput.value = isNaN(cost) ? "" : round2(cost);
          priceInput.value = isNaN(price) ? "" : round2(price);
          markupInput.value = isNaN(markupPct) ? "" : round2(markupPct);
          marginInput.value = isNaN(marginPct) ? "" : round2(marginPct);
          qtyInput.value = qty;

          // Update results
          document.getElementById("res-cost").textContent =
            formatCurrency(cost);
          document.getElementById("res-price").textContent =
            formatCurrency(price);
          document.getElementById("res-profit").textContent =
            formatCurrency(profit);
          document.getElementById("res-markup").textContent =
            formatPercent(markupPct);
          document.getElementById("res-margin").textContent =
            formatPercent(marginPct);

          document.getElementById("res-qty").textContent = qty.toString();
          document.getElementById("res-total-cost").textContent =
            formatCurrency(totalCost);
          document.getElementById("res-total-revenue").textContent =
            formatCurrency(totalRevenue);
          document.getElementById("res-total-profit").textContent =
            formatCurrency(totalProfit);

          // Conversion explanation
          const mRate = isNaN(markupPct) ? NaN : markupPct / 100;
          const gRate = isNaN(marginPct) ? NaN : marginPct / 100;
          let convMarkupText = "-";
          let convMarginText = "-";
          if (!isNaN(mRate) && mRate > -1) {
            const gFromM = (mRate / (1 + mRate)) * 100;
            convMarkupText = formatPercent(markupPct);
            convMarginText = formatPercent(gFromM);
          } else if (!isNaN(gRate) && gRate < 1) {
            const mFromG = (gRate / (1 - gRate)) * 100;
            convMarkupText = formatPercent(mFromG);
            convMarginText = formatPercent(marginPct);
          }
          document.getElementById("conv-markup").textContent = convMarkupText;
          document.getElementById("conv-margin").textContent = convMarginText;

          document.getElementById("results-section").classList.remove("hidden");
        } catch (e) {
          errorMsg.textContent = e.message;
          document.getElementById("results-section").classList.add("hidden");
        }
      });

      // Clear
      document.getElementById("clear-btn").addEventListener("click", () => {
        ["cost", "price", "markup", "margin", "quantity"].forEach((id) => {
          const el = document.getElementById(id);
          if (id === "quantity") {
            el.value = 1;
          } else {
            el.value = "";
          }
        });
        document.getElementById("error-msg").textContent = "";
        document.getElementById("results-section").classList.add("hidden");
      });

      // FAQ toggle (simple, in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const symbol = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question")
              .forEach((b) => {
                b.nextElementSibling.classList.add("hidden");
                b.querySelector("span:last-child").textContent = "+";
              });
            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
