    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function getRegionFactor() {
        const state = document.getElementById("state").value;
        if (state === "US-avg") return 1.0;
        if (state === "US-high") return 1.4;
        if (state === "US-low") return 0.8;
        if (state === "custom") {
          const custom =
            parseFloat(document.getElementById("customRegionFactor").value) ||
            1;
          return clamp(custom, 0.3, 3);
        }
        return 1.0;
      }

      function getConstructionFactor() {
        const type = document.getElementById("constructionType").value;
        if (type === "frame") return 1.0;
        if (type === "masonry") return 1.1; // slightly higher rebuild cost
        return 1.0;
      }

      function getDeductibleFactor(deductible) {
        const baseDed = 500;
        const ratio = deductible / baseDed;
        if (ratio <= 0) return 1;
        const k = 0.12;
        const factor = 1 - k * Math.log10(ratio);
        return clamp(factor, 0.6, 1.1);
      }

      function getRiskFactor() {
        let factor = 1.0;
        if (document.getElementById("hasSecurity").checked) factor *= 0.95;
        if (document.getElementById("hasSmokeAlarms").checked) factor *= 0.97;
        if (document.getElementById("newRoof").checked) factor *= 0.93;
        if (document.getElementById("claimsHistory").checked) factor *= 1.25;
        if (document.getElementById("bundleAuto").checked) factor *= 0.9;
        return clamp(factor, 0.6, 1.6);
      }

      function recalcDwellingFromRebuild() {
        const size = parseFloat(document.getElementById("homeSize").value) || 0;
        const cpsf =
          parseFloat(document.getElementById("costPerSqFt").value) || 0;
        const constructionFactor = getConstructionFactor();
        const rebuildCost = size * cpsf * constructionFactor;
        const dwellingInput = document.getElementById("dwellingCoverage");
        if (
          !dwellingInput.dataset.userEdited ||
          dwellingInput.dataset.userEdited === "false"
        ) {
          dwellingInput.value = Math.round(rebuildCost / 1000) * 1000;
        }
        return rebuildCost;
      }

      function calculatePremium() {
        const dwelling =
          parseFloat(document.getElementById("dwellingCoverage").value) || 0;
        const personalPct = clamp(
          parseFloat(document.getElementById("personalPropertyPct").value) ||
            50,
          20,
          100
        );
        const liability =
          parseFloat(document.getElementById("liabilityLimit").value) || 300000;
        const deductible =
          parseFloat(document.getElementById("deductible").value) || 1000;

        const rebuildCost = recalcDwellingFromRebuild();

        // Base rate per $1,000 of dwelling coverage (approximate national average)
        const baseRatePerThousand = 3.5; // $3.50 per $1,000 of Coverage A
        const regionFactor = getRegionFactor();

        let basePremium =
          (dwelling / 1000) * baseRatePerThousand * regionFactor;

        // Coverage factor: adjust for personal property and liability relative to typical levels
        const typicalPersonalPct = 50;
        const typicalLiability = 300000;
        const personalFactor = 0.5 + 0.5 * (personalPct / typicalPersonalPct);
        const liabilityFactor = 0.5 + 0.5 * (liability / typicalLiability);
        const coverageFactor = personalFactor * 0.6 + liabilityFactor * 0.4;

        // Deductible factor
        const deductibleFactor = getDeductibleFactor(deductible);

        // Risk & discount factor
        const riskFactor = getRiskFactor();

        const premiumBeforeRisk =
          basePremium * coverageFactor * deductibleFactor;
        const finalPremium = clamp(premiumBeforeRisk * riskFactor, 150, 20000);

        // Breakdown approximations
        const baseComponent = basePremium;
        const coverageComponent = premiumBeforeRisk - basePremium;
        const riskComponent = finalPremium - premiumBeforeRisk;

        // Update UI
        document.getElementById("annualPremium").textContent = formatCurrency(
          Math.round(finalPremium)
        );
        document.getElementById("monthlyPremium").textContent = formatCurrency(
          Math.round(finalPremium / 12)
        );
        document.getElementById("rebuildCost").textContent = formatCurrency(
          Math.round(rebuildCost)
        );

        document.getElementById("summaryDwelling").textContent = formatCurrency(
          Math.round(dwelling)
        );
        const personalProperty = dwelling * (personalPct / 100);
        document.getElementById("summaryPersonalProperty").textContent =
          formatCurrency(Math.round(personalProperty));
        document.getElementById("summaryLiability").textContent =
          formatCurrency(Math.round(liability));
        document.getElementById("summaryDeductible").textContent =
          formatCurrency(Math.round(deductible));

        document.getElementById("breakdownBase").textContent = formatCurrency(
          Math.round(baseComponent)
        );
        document.getElementById("breakdownCoverage").textContent =
          (coverageComponent >= 0 ? "+" : "") +
          formatCurrency(Math.round(coverageComponent));
        document.getElementById("breakdownRisk").textContent =
          (riskComponent >= 0 ? "+" : "") +
          formatCurrency(Math.round(riskComponent));
      }

      function initHomeInsuranceCalculator() {
        const stateEl = document.getElementById("state");
        const customRegionWrapper = document.getElementById(
          "custom-region-wrapper"
        );
        const constructionEl = document.getElementById("constructionType");
        const costPerSqFtWrapper =
          document.getElementById("costPerSqFtWrapper");
        const dwellingInput = document.getElementById("dwellingCoverage");

        function handleStateChange() {
          if (stateEl.value === "custom") {
            customRegionWrapper.classList.remove("hidden");
          } else {
            customRegionWrapper.classList.add("hidden");
          }
          calculatePremium();
        }

        function handleConstructionChange() {
          if (constructionEl.value === "custom") {
            costPerSqFtWrapper.querySelector("label").textContent =
              "Custom rebuild cost per sq ft ($)";
          } else {
            costPerSqFtWrapper.querySelector("label").textContent =
              "Estimated rebuild cost per sq ft ($)";
          }
          recalcDwellingFromRebuild();
          calculatePremium();
        }

        // Mark dwelling as user-edited when changed manually
        dwellingInput.addEventListener("input", () => {
          dwellingInput.dataset.userEdited = "true";
          calculatePremium();
        });

        // Attach listeners
        stateEl.addEventListener("change", handleStateChange);
        document
          .getElementById("customRegionFactor")
          .addEventListener("input", calculatePremium);
        constructionEl.addEventListener("change", handleConstructionChange);
        document
          .getElementById("yearBuilt")
          .addEventListener("input", calculatePremium);
        document.getElementById("homeSize").addEventListener("input", () => {
          recalcDwellingFromRebuild();
          calculatePremium();
        });
        document.getElementById("costPerSqFt").addEventListener("input", () => {
          recalcDwellingFromRebuild();
          calculatePremium();
        });
        document
          .getElementById("personalPropertyPct")
          .addEventListener("input", calculatePremium);
        document
          .getElementById("liabilityLimit")
          .addEventListener("change", calculatePremium);
        document
          .getElementById("deductible")
          .addEventListener("change", calculatePremium);

        [
          "hasSecurity",
          "hasSmokeAlarms",
          "newRoof",
          "claimsHistory",
          "bundleAuto",
        ].forEach((id) => {
          document
            .getElementById(id)
            .addEventListener("change", calculatePremium);
        });

        document.getElementById("resetBtn").addEventListener("click", () => {
          document.getElementById("state").value = "US-avg";
          document.getElementById("customRegionFactor").value = 1;
          customRegionWrapper.classList.add("hidden");

          document.getElementById("yearBuilt").value = 1995;
          document.getElementById("homeSize").value = 2000;
          document.getElementById("constructionType").value = "frame";
          document.getElementById("costPerSqFt").value = 200;

          dwellingInput.dataset.userEdited = "false";
          recalcDwellingFromRebuild();

          document.getElementById("personalPropertyPct").value = 50;
          document.getElementById("liabilityLimit").value = 300000;
          document.getElementById("deductible").value = 1000;

          document.getElementById("hasSecurity").checked = false;
          document.getElementById("hasSmokeAlarms").checked = true;
          document.getElementById("newRoof").checked = false;
          document.getElementById("claimsHistory").checked = false;
          document.getElementById("bundleAuto").checked = false;

          calculatePremium();
        });

        // Initial setup
        dwellingInput.dataset.userEdited = "false";
        recalcDwellingFromRebuild();
        calculatePremium();
      }

      document.addEventListener(
        "DOMContentLoaded",
        initHomeInsuranceCalculator
      );

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
