    <script>
      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "-";
        return value.toFixed(decimals);
      }

      function calculateShear() {
        const errorCard = document.getElementById("error-card");
        const summaryCard = document.getElementById("summary-card");
        const detailsCard = document.getElementById("details-card");
        errorCard.classList.add("hidden");
        summaryCard.classList.add("hidden");
        detailsCard.classList.add("hidden");
        errorCard.textContent = "";

        const length = parseFloat(document.getElementById("length").value);
        const thickness = parseFloat(
          document.getElementById("thickness").value
        );
        const height = parseFloat(document.getElementById("height").value);
        const fk = parseFloat(document.getElementById("fk").value);
        const fvko = parseFloat(document.getElementById("fvko").value);
        const alpha = parseFloat(document.getElementById("alpha").value);
        const gammaM = parseFloat(document.getElementById("gamma_m").value);
        const ned = parseFloat(document.getElementById("ned").value);
        const ved = parseFloat(document.getElementById("ved").value);
        const psi = parseFloat(document.getElementById("psi").value);
        const beta = parseFloat(document.getElementById("beta").value);

        const inputs = [
          length,
          thickness,
          height,
          fk,
          fvko,
          alpha,
          gammaM,
          ned,
          ved,
          psi,
          beta,
        ];
        if (inputs.some((v) => isNaN(v) || v <= 0)) {
          errorCard.textContent =
            "Please enter positive numeric values for all inputs.";
          errorCard.classList.remove("hidden");
          return;
        }

        const A = length * thickness; // m2
        const sigma_d = ned / 1000 / A; // MPa (ned in kN -> MN)
        const fvd = ((fvko + alpha * sigma_d) / gammaM) * psi; // MPa
        const Av = thickness * height; // m2
        let Vrd = fvd * Av * 1000; // kN (MPa * m2 * 1000)

        Vrd = Vrd * beta;

        const utilisation = ved / Vrd;

        document.getElementById("vrd-display").textContent = formatNumber(
          Vrd,
          1
        );
        document.getElementById("ved-display").textContent = formatNumber(
          ved,
          1
        );
        document.getElementById("utilisation-display").textContent =
          formatNumber(utilisation, 2);

        document.getElementById("area-display").textContent = formatNumber(
          A,
          3
        );
        document.getElementById("sigma-display").textContent = formatNumber(
          sigma_d,
          3
        );
        document.getElementById("fvd-display").textContent = formatNumber(
          fvd,
          3
        );
        document.getElementById("av-display").textContent = formatNumber(Av, 3);

        const badge = document.getElementById("utilisation-badge");
        badge.textContent = utilisation <= 1 ? "PASS" : "FAIL";
        badge.className =
          "text-xs font-semibold px-2 py-1 rounded-full " +
          (utilisation <= 1 ? "result-badge-ok" : "result-badge-ng");

        const modeText =
          utilisation <= 1
            ? "Shear capacity adequate for given actions"
            : "Shear capacity exceeded – revise design";
        document.getElementById("mode-display").textContent = modeText;

        summaryCard.classList.remove("hidden");
        detailsCard.classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateShear);
      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("ec6-form").reset();
        document.getElementById("length").value = 4.0;
        document.getElementById("thickness").value = 0.24;
        document.getElementById("height").value = 2.7;
        document.getElementById("fk").value = 5.0;
        document.getElementById("fvko").value = 0.15;
        document.getElementById("alpha").value = 0.4;
        document.getElementById("gamma_m").value = 2.7;
        document.getElementById("ned").value = 400;
        document.getElementById("ved").value = 80;
        document.getElementById("psi").value = 0.8;
        document.getElementById("beta").value = 1.0;

        document.getElementById("summary-card").classList.add("hidden");
        document.getElementById("details-card").classList.add("hidden");
        document.getElementById("error-card").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
