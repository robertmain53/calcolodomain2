    <script>
      // Mode toggle
      const modeSymbolBtn = document.getElementById("mode-symbol");
      const modeRtpBtn = document.getElementById("mode-rtp");
      const symbolSection = document.getElementById("symbol-mode-section");
      const rtpSection = document.getElementById("rtp-mode-section");

      function setMode(mode) {
        if (mode === "symbol") {
          symbolSection.classList.remove("hidden");
          rtpSection.classList.add("hidden");
          modeSymbolBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeSymbolBtn.classList.remove("border-gray-300", "text-gray-700");
          modeRtpBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeRtpBtn.classList.add("border-gray-300", "text-gray-700");
        } else {
          symbolSection.classList.add("hidden");
          rtpSection.classList.remove("hidden");
          modeRtpBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeRtpBtn.classList.remove("border-gray-300", "text-gray-700");
          modeSymbolBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeSymbolBtn.classList.add("border-gray-300", "text-gray-700");
        }
      }

      modeSymbolBtn.addEventListener("click", () => setMode("symbol"));
      modeRtpBtn.addEventListener("click", () => setMode("rtp"));

      // Symbol rows management
      const symbolTbody = document.getElementById("symbol-rows");
      const addRowBtn = document.getElementById("add-symbol-row");
      const resetRowsBtn = document.getElementById("reset-symbol-rows");

      function createSymbolRow(symbol = "", count = 1, payout = 5) {
        const tr = document.createElement("tr");
        tr.className = "border-t";

        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" value="${symbol}" placeholder="e.g. Cherry" class="w-full border rounded-md px-2 py-1 text-sm symbol-name-input" />
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" value="${count}" class="w-24 border rounded-md px-2 py-1 text-sm text-right symbol-count-input" />
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.1" value="${payout}" class="w-24 border rounded-md px-2 py-1 text-sm text-right symbol-payout-input" />
      </td>
      <td class="px-3 py-2 text-center">
        <button type="button" class="text-red-600 text-xs hover:underline remove-row-btn">Remove</button>
      </td>
    `;
        symbolTbody.appendChild(tr);
      }

      function resetExampleRows() {
        symbolTbody.innerHTML = "";
        // Simple 3-reel example: BAR, Seven, Cherry
        createSymbolRow("BAR", 2, 20);
        createSymbolRow("Seven", 1, 50);
        createSymbolRow("Cherry", 3, 5);
      }

      addRowBtn.addEventListener("click", () => createSymbolRow("", 1, 5));
      resetRowsBtn.addEventListener("click", resetExampleRows);

      symbolTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          const row = e.target.closest("tr");
          if (row && symbolTbody.children.length > 1) {
            row.remove();
          }
        }
      });

      // Initial example
      resetExampleRows();

      // Calculation logic for symbol-based mode
      function formatMoney(value) {
        if (!isFinite(value)) return "–";
        const sign = value < 0 ? "-" : "";
        return sign + "$" + Math.abs(value).toFixed(2);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }

      document
        .getElementById("calculate-symbol")
        .addEventListener("click", () => {
          const numReels = parseInt(
            document.getElementById("num-reels").value,
            10
          );
          const stopsPerReel = parseInt(
            document.getElementById("stops-per-reel").value,
            10
          );
          const betPerSpin = parseFloat(
            document.getElementById("bet-per-spin").value
          );
          const spinsPerHour = parseFloat(
            document.getElementById("spins-per-hour").value
          );
          const sessionHours = parseFloat(
            document.getElementById("session-hours").value
          );

          if (
            !(
              numReels > 0 &&
              stopsPerReel > 0 &&
              betPerSpin > 0 &&
              spinsPerHour > 0 &&
              sessionHours > 0
            )
          ) {
            alert(
              "Please enter positive values for reels, stops, bet, spins per hour and session length."
            );
            return;
          }

          const rows = Array.from(symbolTbody.querySelectorAll("tr"));
          if (rows.length === 0) {
            alert("Please define at least one winning combination.");
            return;
          }

          let totalProb = 0;
          let expectedPayoutUnits = 0;
          let expectedPayoutSquared = 0;

          rows.forEach((row) => {
            const countInput = row.querySelector(".symbol-count-input");
            const payoutInput = row.querySelector(".symbol-payout-input");
            const count = parseFloat(countInput.value);
            const payout = parseFloat(payoutInput.value);

            if (!(count >= 0 && payout >= 0)) return;
            if (stopsPerReel === 0) return;

            const p = Math.pow(count / stopsPerReel, numReels);
            if (!isFinite(p) || p <= 0) return;

            totalProb += p;
            expectedPayoutUnits += p * payout;
            expectedPayoutSquared += p * payout * payout;
          });

          if (expectedPayoutUnits < 0 || !isFinite(expectedPayoutUnits)) {
            alert(
              "The current configuration does not produce valid probabilities. Check symbol counts and stops per reel."
            );
            return;
          }

          const rtp = expectedPayoutUnits * 100;
          const houseEdge = 100 - rtp;
          const varianceUnits =
            expectedPayoutSquared - expectedPayoutUnits * expectedPayoutUnits;
          const stdDevUnits = varianceUnits > 0 ? Math.sqrt(varianceUnits) : 0;

          const spinsPerSession = spinsPerHour * sessionHours;
          const expectedReturnPerSpinMoney = expectedPayoutUnits * betPerSpin;
          const expectedLossPerSpinMoney =
            betPerSpin - expectedReturnPerSpinMoney;
          const expectedLossPerHour = expectedLossPerSpinMoney * spinsPerHour;
          const expectedLossPerSession =
            expectedLossPerSpinMoney * spinsPerSession;

          const stdDevPerSpinMoney = stdDevUnits * betPerSpin;
          const stdDevPerSessionMoney =
            stdDevPerSpinMoney * Math.sqrt(spinsPerSession);
          const lowerSession = -expectedLossPerSession - stdDevPerSessionMoney;
          const upperSession = -expectedLossPerSession + stdDevPerSessionMoney;

          // Update outputs
          document.getElementById("rtp-output").textContent =
            formatPercent(rtp);
          document.getElementById("house-edge-output").textContent =
            formatPercent(houseEdge);
          document.getElementById("hit-frequency-output").textContent =
            formatPercent(totalProb * 100);
          document.getElementById("volatility-output").textContent =
            stdDevUnits.toFixed(3) + " × bet";

          document.getElementById("session-spins-output").textContent =
            isFinite(spinsPerSession) ? spinsPerSession.toFixed(0) : "–";
          document.getElementById("loss-per-hour-output").textContent =
            formatMoney(expectedLossPerHour);
          document.getElementById("loss-per-session-output").textContent =
            formatMoney(expectedLossPerSession);
          document.getElementById("session-range-output").textContent =
            formatMoney(lowerSession) + " to " + formatMoney(upperSession);

          const interp = [];
          interp.push(
            `With your current layout, the slot has an RTP of approximately ${formatPercent(
              rtp
            )} (${formatPercent(houseEdge)} house edge).`
          );
          interp.push(
            `You will see a win on about ${formatPercent(
              totalProb * 100
            )} of spins.`
          );
          interp.push(
            `At ${spinsPerHour.toFixed(
              0
            )} spins per hour and a $${betPerSpin.toFixed(
              2
            )} bet, your long‑run expected loss is about ${formatMoney(
              expectedLossPerHour
            )} per hour.`
          );
          document.getElementById("interpretation-text").textContent =
            interp.join(" ");
        });

      // Quick RTP mode calculation
      document
        .getElementById("calculate-rtp-mode")
        .addEventListener("click", () => {
          const rtp = parseFloat(document.getElementById("known-rtp").value);
          const bet = parseFloat(document.getElementById("quick-bet").value);
          const spinsPerHour = parseFloat(
            document.getElementById("quick-spins-per-hour").value
          );
          const hours = parseFloat(
            document.getElementById("quick-hours").value
          );

          if (
            !(rtp > 0 && rtp <= 100 && bet > 0 && spinsPerHour > 0 && hours > 0)
          ) {
            alert(
              "Please enter valid positive values for RTP, bet, spins per hour and session length."
            );
            return;
          }

          const houseEdge = 100 - rtp;
          const totalSpins = spinsPerHour * hours;
          const totalWagered = bet * totalSpins;
          const expectedLoss = totalWagered * (houseEdge / 100);
          const lossPerHour = expectedLoss / hours;

          document.getElementById("quick-house-edge").textContent =
            formatPercent(houseEdge);
          document.getElementById("quick-loss-hour").textContent =
            formatMoney(lossPerHour);
          document.getElementById("quick-loss-session").textContent =
            formatMoney(expectedLoss);
          document.getElementById("quick-total-wagered").textContent =
            formatMoney(totalWagered);
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
