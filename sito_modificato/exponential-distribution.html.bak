    <script>
      (function () {
        const lambdaInput = document.getElementById("lambda");
        const meanInput = document.getElementById("mean");
        const xInput = document.getElementById("xValue");
        const pInput = document.getElementById("pValue");
        const aInput = document.getElementById("aValue");
        const bInput = document.getElementById("bValue");
        const tInput = document.getElementById("memorylessT");
        const errorBox = document.getElementById("errorBox");

        const pdfOut = document.getElementById("pdfOut");
        const cdfOut = document.getElementById("cdfOut");
        const sfOut = document.getElementById("sfOut");
        const intervalOut = document.getElementById("intervalOut");
        const quantileOut = document.getElementById("quantileOut");
        const medianOut = document.getElementById("medianOut");
        const meanOut = document.getElementById("meanOut");
        const varOut = document.getElementById("varOut");
        const sdOut = document.getElementById("sdOut");
        const memorylessCondOut = document.getElementById("memorylessCondOut");
        const memorylessPlainOut =
          document.getElementById("memorylessPlainOut");

        const computeBtn = document.getElementById("computeBtn");
        const resetBtn = document.getElementById("resetBtn");

        const paramRadios = document.querySelectorAll(
          'input[name="paramMode"]'
        );
        const rateGroup = document.getElementById("rate-input-group");
        const meanGroup = document.getElementById("mean-input-group");

        const plotLambdaSlider = document.getElementById("plotLambda");
        const plotLambdaLabel = document.getElementById("plotLambdaLabel");
        const pdfCanvas = document.getElementById("pdfCanvas");
        const cdfCanvas = document.getElementById("cdfCanvas");

        function getParamMode() {
          return Array.from(paramRadios).find((r) => r.checked).value;
        }

        function getLambda() {
          const mode = getParamMode();
          if (mode === "rate") {
            return parseFloat(lambdaInput.value);
          } else {
            const mu = parseFloat(meanInput.value);
            return mu > 0 ? 1 / mu : NaN;
          }
        }

        function formatNumber(x) {
          if (!isFinite(x)) return "–";
          if (Math.abs(x) >= 1e4 || (Math.abs(x) > 0 && Math.abs(x) < 1e-4)) {
            return x.toExponential(4);
          }
          return x.toFixed(6).replace(/0+$/, "").replace(/\.$/, "");
        }

        function showError(msg) {
          errorBox.textContent = msg;
          errorBox.classList.remove("hidden");
        }

        function clearError() {
          errorBox.classList.add("hidden");
          errorBox.textContent = "";
        }

        function compute() {
          clearError();
          let lambda = getLambda();
          const x = parseFloat(xInput.value);
          const p = parseFloat(pInput.value);
          const a = parseFloat(aInput.value);
          const b = parseFloat(bInput.value);
          const t = parseFloat(tInput.value);
          const s = a; // use 'a' as s for memoryless demonstration

          if (!(lambda > 0)) {
            showError("Rate λ (or mean μ) must be positive.");
            return;
          }
          if (!(x >= 0)) {
            showError("x must be ≥ 0.");
            return;
          }
          if (!(a >= 0 && b >= a)) {
            showError("Interval must satisfy 0 ≤ a ≤ b.");
            return;
          }
          if (!(p > 0 && p < 1)) {
            showError("Probability p must be between 0 and 1 (exclusive).");
            return;
          }
          if (!(t >= 0)) {
            showError("t must be ≥ 0 for the memoryless property.");
            return;
          }

          // PDF, CDF, Survival
          const pdf = lambda * Math.exp(-lambda * x);
          const cdf = 1 - Math.exp(-lambda * x);
          const sf = Math.exp(-lambda * x);

          pdfOut.textContent = formatNumber(pdf);
          cdfOut.textContent = formatNumber(cdf);
          sfOut.textContent = formatNumber(sf);

          // Interval probability
          const Pa = Math.exp(-lambda * a);
          const Pb = Math.exp(-lambda * b);
          const Pint = Pa - Pb;
          intervalOut.textContent = formatNumber(Pint);

          // Quantile and median
          const quantile = -Math.log(1 - p) / lambda;
          const median = Math.log(2) / lambda;
          quantileOut.textContent = formatNumber(quantile);
          medianOut.textContent = formatNumber(median);

          // Moments
          const mean = 1 / lambda;
          const variance = 1 / (lambda * lambda);
          const sd = Math.sqrt(variance);
          meanOut.textContent = formatNumber(mean);
          varOut.textContent = formatNumber(variance);
          sdOut.textContent = formatNumber(sd);

          // Memoryless property
          const cond = Math.exp(-lambda * t); // equals P(X>t)
          const plain = Math.exp(-lambda * t);
          memorylessCondOut.textContent = formatNumber(cond);
          memorylessPlainOut.textContent = formatNumber(plain);
        }

        function reset() {
          lambdaInput.value = 0.5;
          meanInput.value = 2;
          xInput.value = 1;
          pInput.value = 0.9;
          aInput.value = 1;
          bInput.value = 3;
          tInput.value = 2;
          plotLambdaSlider.value = 0.5;
          plotLambdaLabel.textContent = "0.5";
          clearError();
          compute();
          drawPlots();
        }

        // Parameterization toggle
        paramRadios.forEach((r) => {
          r.addEventListener("change", () => {
            const mode = getParamMode();
            if (mode === "rate") {
              // sync mean from lambda
              const lam = parseFloat(lambdaInput.value);
              if (lam > 0) meanInput.value = (1 / lam).toFixed(4);
              rateGroup.classList.remove("hidden");
              meanGroup.classList.add("hidden");
            } else {
              const mu = parseFloat(meanInput.value);
              if (mu > 0) lambdaInput.value = (1 / mu).toFixed(4);
              rateGroup.classList.add("hidden");
              meanGroup.classList.remove("hidden");
            }
            compute();
            drawPlots();
          });
        });

        lambdaInput.addEventListener("input", () => {
          if (getParamMode() === "rate") {
            const lam = parseFloat(lambdaInput.value);
            if (lam > 0) meanInput.value = (1 / lam).toFixed(4);
            compute();
            drawPlots();
          }
        });

        meanInput.addEventListener("input", () => {
          if (getParamMode() === "mean") {
            const mu = parseFloat(meanInput.value);
            if (mu > 0) lambdaInput.value = (1 / mu).toFixed(4);
            compute();
            drawPlots();
          }
        });

        [xInput, pInput, aInput, bInput, tInput].forEach((el) => {
          el.addEventListener("input", compute);
        });

        computeBtn.addEventListener("click", compute);
        resetBtn.addEventListener("click", reset);

        // Simple plotting without external libraries
        function clearCanvas(ctx, canvas) {
          ctx.clearRect(0, 0, canvas.width, canvas.height);
        }

        function drawAxes(ctx, canvas, xMax, yMax, isCDF) {
          const w = canvas.width;
          const h = canvas.height;
          ctx.strokeStyle = "#d1d5db";
          ctx.lineWidth = 1;

          // x-axis
          ctx.beginPath();
          ctx.moveTo(40, h - 30);
          ctx.lineTo(w - 10, h - 30);
          ctx.stroke();

          // y-axis
          ctx.beginPath();
          ctx.moveTo(40, 10);
          ctx.lineTo(40, h - 30);
          ctx.stroke();

          ctx.fillStyle = "#6b7280";
          ctx.font = "10px Inter";

          // x labels (0, xMax/2, xMax)
          const xLabels = [0, xMax / 2, xMax];
          xLabels.forEach((val) => {
            const xPix = 40 + (w - 50) * (val / xMax);
            ctx.beginPath();
            ctx.moveTo(xPix, h - 30);
            ctx.lineTo(xPix, h - 26);
            ctx.stroke();
            ctx.fillText(val.toFixed(1), xPix - 6, h - 14);
          });

          // y labels
          const yLabels = isCDF ? [0, 0.5, 1] : [0, yMax / 2, yMax];
          yLabels.forEach((val) => {
            const yPix = 10 + (h - 40) * (1 - val / (isCDF ? 1 : yMax));
            ctx.beginPath();
            ctx.moveTo(36, yPix);
            ctx.lineTo(40, yPix);
            ctx.stroke();
            ctx.fillText(val.toFixed(isCDF ? 1 : 2), 10, yPix + 3);
          });
        }

        function drawPDF(lambda) {
          const canvas = pdfCanvas;
          const ctx = canvas.getContext("2d");
          const w = (canvas.width = canvas.clientWidth);
          const h = (canvas.height = canvas.clientHeight);
          clearCanvas(ctx, canvas);

          const xMax = Math.max(5 / lambda, 5); // show up to ~5 mean times
          // max of pdf is at x=0: f(0) = lambda
          const yMax = lambda * 1.1;

          drawAxes(ctx, canvas, xMax, yMax, false);

          ctx.strokeStyle = "#2563eb";
          ctx.lineWidth = 2;
          ctx.beginPath();
          const steps = 200;
          for (let i = 0; i <= steps; i++) {
            const x = (xMax * i) / steps;
            const y = lambda * Math.exp(-lambda * x);
            const xPix = 40 + (w - 50) * (x / xMax);
            const yPix = 10 + (h - 40) * (1 - y / yMax);
            if (i === 0) ctx.moveTo(xPix, yPix);
            else ctx.lineTo(xPix, yPix);
          }
          ctx.stroke();
        }

        function drawCDF(lambda) {
          const canvas = cdfCanvas;
          const ctx = canvas.getContext("2d");
          const w = (canvas.width = canvas.clientWidth);
          const h = (canvas.height = canvas.clientHeight);
          clearCanvas(ctx, canvas);

          const xMax = Math.max(5 / lambda, 5);
          drawAxes(ctx, canvas, xMax, 1, true);

          ctx.strokeStyle = "#16a34a";
          ctx.lineWidth = 2;
          ctx.beginPath();
          const steps = 200;
          for (let i = 0; i <= steps; i++) {
            const x = (xMax * i) / steps;
            const y = 1 - Math.exp(-lambda * x);
            const xPix = 40 + (w - 50) * (x / xMax);
            const yPix = 10 + (h - 40) * (1 - y);
            if (i === 0) ctx.moveTo(xPix, yPix);
            else ctx.lineTo(xPix, yPix);
          }
          ctx.stroke();
        }

        function drawPlots() {
          const lam = parseFloat(plotLambdaSlider.value);
          plotLambdaLabel.textContent = lam.toFixed(1);
          drawPDF(lam);
          drawCDF(lam);
        }

        plotLambdaSlider.addEventListener("input", drawPlots);

        // FAQ accordion (simple)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.nextElementSibling;
              const icon = btn.querySelector("svg");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll("#faq-container .faq-question svg")
                .forEach((i) => i.classList.remove("rotate-180"));
              if (isHidden) {
                answer.classList.remove("hidden");
                icon.classList.add("rotate-180");
              }
            });
          });

        // Initial compute & plot
        compute();
        drawPlots();
      })();
    </script>
  
