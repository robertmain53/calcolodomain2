    <script>
      // Utility conversions
      function toMeters(value, unit) {
        if (unit === "m") return value;
        if (unit === "ft") return value * 0.3048;
        return value;
      }
      function fromM3hToLps(q) {
        return q / 3.6;
      }
      function fromM3hToGpm(q) {
        return q * 4.402867;
      } // 1 m3/h ≈ 4.4029 gpm
      function perWellCapacityToM3h(value, unit) {
        if (unit === "m3h") return value;
        if (unit === "gpm") return value / 4.402867;
        return value;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const soilTypeEl = document.getElementById("soil-type");
        const customKWrapper = document.getElementById("custom-k-wrapper");
        const hydraulicKEl = document.getElementById("hydraulic-conductivity");

        const tabWellpoints = document.getElementById("tab-wellpoints");
        const tabDeepwells = document.getElementById("tab-deepwells");
        const methodSelect = document.getElementById("method-select");

        function updateCustomKVisibility() {
          if (soilTypeEl.value === "custom") {
            customKWrapper.classList.remove("hidden");
          } else {
            customKWrapper.classList.add("hidden");
          }
        }

        soilTypeEl.addEventListener("change", function () {
          // Set typical K when selecting a predefined soil
          switch (soilTypeEl.value) {
            case "sand":
              hydraulicKEl.value = 1e-4;
              break;
            case "gravel":
              hydraulicKEl.value = 1e-3;
              break;
            case "sandy-gravel":
              hydraulicKEl.value = 5e-4;
              break;
            case "silty-sand":
              hydraulicKEl.value = 1e-5;
              break;
            case "silt":
              hydraulicKEl.value = 1e-6;
              break;
            case "clay":
              hydraulicKEl.value = 1e-8;
              break;
          }
          updateCustomKVisibility();
        });
        updateCustomKVisibility();

        // Tabs just pre-select method and tweak per-well capacity
        tabWellpoints.addEventListener("click", function () {
          tabWellpoints.classList.add("tab-active");
          tabWellpoints.classList.remove("text-gray-500");
          tabDeepwells.classList.remove("tab-active");
          tabDeepwells.classList.add("text-gray-500");
          methodSelect.value = "wellpoints";
          document.getElementById("per-well-capacity").value = 15;
          document.getElementById("per-well-capacity-unit").value = "m3h";
        });

        tabDeepwells.addEventListener("click", function () {
          tabDeepwells.classList.add("tab-active");
          tabDeepwells.classList.remove("text-gray-500");
          tabWellpoints.classList.remove("tab-active");
          tabWellpoints.classList.add("text-gray-500");
          methodSelect.value = "deepwells";
          document.getElementById("per-well-capacity").value = 40;
          document.getElementById("per-well-capacity-unit").value = "m3h";
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", function () {
            const errorEl = document.getElementById("calc-error");
            errorEl.classList.add("hidden");

            // Read geometry
            const L = parseFloat(
              document.getElementById("excavation-length").value
            );
            const W = parseFloat(
              document.getElementById("excavation-width").value
            );
            const Lunit = document.getElementById("length-unit").value;
            const Wunit = document.getElementById("width-unit").value;
            const sVal = parseFloat(document.getElementById("drawdown").value);
            const sUnit = document.getElementById("drawdown-unit").value;
            const bVal = parseFloat(
              document.getElementById("sat-thickness").value
            );
            const bUnit = document.getElementById("sat-thickness-unit").value;
            const timeDaysVal = parseFloat(
              document.getElementById("time-days").value
            );

            if ([L, W, sVal, bVal].some((v) => isNaN(v) || v <= 0)) {
              errorEl.textContent =
                "Please enter positive values for length, width, drawdown and saturated thickness.";
              errorEl.classList.remove("hidden");
              return;
            }

            // Convert to SI
            const Lm = toMeters(L, Lunit);
            const Wm = toMeters(W, Wunit);
            const sm = toMeters(sVal, sUnit);
            const bm = toMeters(bVal, bUnit);

            // Soil / K
            let K;
            if (soilTypeEl.value === "custom") {
              K = parseFloat(hydraulicKEl.value);
            } else {
              switch (soilTypeEl.value) {
                case "sand":
                  K = 1e-4;
                  break;
                case "gravel":
                  K = 1e-3;
                  break;
                case "sandy-gravel":
                  K = 5e-4;
                  break;
                case "silty-sand":
                  K = 1e-5;
                  break;
                case "silt":
                  K = 1e-6;
                  break;
                case "clay":
                  K = 1e-8;
                  break;
                default:
                  K = 1e-4;
              }
            }
            if (isNaN(K) || K <= 0) {
              errorEl.textContent =
                "Please enter a valid positive hydraulic conductivity K.";
              errorEl.classList.remove("hidden");
              return;
            }

            // Method and per-well capacity
            const method = methodSelect.value;
            const perWellVal = parseFloat(
              document.getElementById("per-well-capacity").value
            );
            const perWellUnit = document.getElementById(
              "per-well-capacity-unit"
            ).value;
            if (isNaN(perWellVal) || perWellVal <= 0) {
              errorEl.textContent =
                "Please enter a positive capacity per well.";
              errorEl.classList.remove("hidden");
              return;
            }
            const qWellM3h = perWellCapacityToM3h(perWellVal, perWellUnit);

            // Perimeter and characteristic flow length
            const perimeter = 2 * (Lm + Wm); // m
            let Lf;
            if (method === "sump") {
              // For sump, assume flow length is half the smaller dimension
              Lf = Math.min(Lm, Wm) / 2;
            } else {
              // For ring of wells, use half the width as characteristic
              Lf = Wm / 2;
            }
            if (Lf <= 0) Lf = 1;

            // Hydraulic gradient
            const i = sm / Lf;

            // Flow area
            const A = perimeter * bm; // m2

            // Darcy flow Q (m3/s)
            let Qs = K * i * A;

            // Apply simple safety factor (e.g., 1.3) to account for uncertainty
            const safetyFactor = 1.3;
            Qs *= safetyFactor;

            if (!isFinite(Qs) || Qs <= 0) {
              errorEl.textContent =
                "Calculation failed due to invalid combination of inputs. Please review values.";
              errorEl.classList.remove("hidden");
              return;
            }

            // Convert to m3/h
            const Qm3h = Qs * 3600;

            // Number of wells
            let Nwells = Math.ceil(Qm3h / qWellM3h);
            if (!isFinite(Nwells) || Nwells < 1) Nwells = 1;

            // Spacing
            const spacing = perimeter / Nwells; // m

            // Excavation volume (for reference)
            const excavationVolume = Lm * Wm * sm; // m3

            // Pumped volume over time
            let pumpedVolume = null;
            if (!isNaN(timeDaysVal) && timeDaysVal > 0) {
              const hours = timeDaysVal * 24;
              pumpedVolume = Qm3h * hours; // m3
            }

            // Populate results
            document.getElementById("result-total-flow-m3h").textContent =
              Qm3h.toFixed(1);
            document.getElementById("result-total-flow-lps").textContent =
              fromM3hToLps(Qm3h).toFixed(2);
            document.getElementById("result-total-flow-gpm").textContent =
              fromM3hToGpm(Qm3h).toFixed(1);

            document.getElementById("result-num-wells").textContent =
              Nwells.toString();
            document.getElementById("result-spacing").textContent =
              spacing.toFixed(1) +
              " m (" +
              (spacing / 0.3048).toFixed(1) +
              " ft)";

            document.getElementById("result-drawdown").textContent =
              sm.toFixed(2) + " m (" + (sm / 0.3048).toFixed(2) + " ft)";
            document.getElementById("result-gradient").textContent =
              i.toFixed(3) + " (dimensionless)";

            document.getElementById("result-excavation-volume").textContent =
              excavationVolume.toFixed(0) + " m³";

            if (pumpedVolume !== null) {
              document.getElementById("result-time-days").textContent =
                timeDaysVal.toFixed(1) + " days";
              document.getElementById("result-pumped-volume").textContent =
                pumpedVolume.toFixed(0) + " m³";
            } else {
              document.getElementById("result-time-days").textContent = "N/A";
              document.getElementById("result-pumped-volume").textContent =
                "N/A";
            }

            document
              .getElementById("results-section")
              .classList.remove("hidden");
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("excavation-length").value = 30;
            document.getElementById("length-unit").value = "m";
            document.getElementById("excavation-width").value = 20;
            document.getElementById("width-unit").value = "m";
            document.getElementById("drawdown").value = 3;
            document.getElementById("drawdown-unit").value = "m";
            document.getElementById("time-days").value = 3;
            document.getElementById("soil-type").value = "sand";
            hydraulicKEl.value = 1e-4;
            document.getElementById("sat-thickness").value = 8;
            document.getElementById("sat-thickness-unit").value = "m";
            document.getElementById("method-select").value = "wellpoints";
            document.getElementById("per-well-capacity").value = 15;
            document.getElementById("per-well-capacity-unit").value = "m3h";
            document.getElementById("results-section").classList.add("hidden");
            document.getElementById("calc-error").classList.add("hidden");
            tabWellpoints.click();
            updateCustomKVisibility();
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
