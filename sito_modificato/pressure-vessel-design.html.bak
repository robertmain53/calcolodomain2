    <script>
      // Unit labels update
      const unitSystemEl = document.getElementById("unitSystem");
      const labelDiameterUnit = document.getElementById("labelDiameterUnit");
      const labelPressureUnit = document.getElementById("labelPressureUnit");
      const labelThicknessUnit = document.getElementById("labelThicknessUnit");
      const labelStressUnit = document.getElementById("labelStressUnit");
      const labelCorrosionUnit = document.getElementById("labelCorrosionUnit");

      function updateUnitLabels() {
        const isMetric = unitSystemEl.value === "metric";
        labelDiameterUnit.textContent = isMetric ? "(mm)" : "(in)";
        labelThicknessUnit.textContent = isMetric ? "(mm)" : "(in)";
        labelCorrosionUnit.textContent = isMetric ? "(mm)" : "(in)";
        labelPressureUnit.textContent = isMetric ? "(MPa)" : "(psi)";
        labelStressUnit.textContent = isMetric ? "(MPa)" : "(psi)";
      }

      unitSystemEl.addEventListener("change", updateUnitLabels);
      updateUnitLabels();

      // Toggle design mode groups
      const designModeEl = document.getElementById("designMode");
      const groupDesignPressure = document.getElementById(
        "groupDesignPressure"
      );
      const groupThickness = document.getElementById("groupThickness");

      function updateDesignMode() {
        if (designModeEl.value === "thickness") {
          groupDesignPressure.classList.remove("hidden");
          groupThickness.classList.add("hidden");
        } else {
          groupDesignPressure.classList.add("hidden");
          groupThickness.classList.remove("hidden");
        }
      }

      designModeEl.addEventListener("change", updateDesignMode);
      updateDesignMode();

      // Core calculation
      const calcButton = document.getElementById("calcButton");
      const resetButton = document.getElementById("resetButton");
      const inputError = document.getElementById("inputError");

      const vesselTypeEl = document.getElementById("vesselType");
      const innerDiameterEl = document.getElementById("innerDiameter");
      const designPressureEl = document.getElementById("designPressure");
      const wallThicknessEl = document.getElementById("wallThickness");
      const allowableStressEl = document.getElementById("allowableStress");
      const efficiencyEl = document.getElementById("efficiency");
      const corrosionAllowanceEl =
        document.getElementById("corrosionAllowance");

      const primaryResults = document.getElementById("primaryResults");
      const primaryResultText = document.getElementById("primaryResultText");
      const stressResults = document.getElementById("stressResults");
      const stressList = document.getElementById("stressList");
      const stressCheck = document.getElementById("stressCheck");
      const thinWallCheck = document.getElementById("thinWallCheck");
      const thinWallText = document.getElementById("thinWallText");

      function parsePositive(value) {
        const v = parseFloat(value);
        return isFinite(v) && v > 0 ? v : null;
      }

      function calculate() {
        inputError.textContent = "";
        primaryResults.classList.add("hidden");
        stressResults.classList.add("hidden");
        thinWallCheck.classList.add("hidden");

        const mode = designModeEl.value;
        const type = vesselTypeEl.value;
        const isMetric = unitSystemEl.value === "metric";

        const D = parsePositive(innerDiameterEl.value);
        const P =
          mode === "thickness" ? parsePositive(designPressureEl.value) : null;
        const tInput =
          mode === "mawp" ? parsePositive(wallThicknessEl.value) : null;
        const S = parsePositive(allowableStressEl.value);
        const E = parseFloat(efficiencyEl.value);
        const c = parseFloat(corrosionAllowanceEl.value || "0");

        if (
          !D ||
          !S ||
          isNaN(E) ||
          E <= 0 ||
          E > 1 ||
          isNaN(c) ||
          c < 0 ||
          (mode === "thickness" && !P) ||
          (mode === "mawp" && !tInput)
        ) {
          inputError.textContent =
            "Please enter valid positive values for all required fields. Joint efficiency must be between 0 and 1.";
          return;
        }

        const R = D / 2; // inner radius

        let tRequired = null;
        let tNominal = null;
        let Pmawp = null;
        let Pdesign = null;

        if (mode === "thickness") {
          // Required thickness for given P
          if (type === "cylinder") {
            const denom = S * E - 0.6 * P;
            if (denom <= 0) {
              inputError.textContent =
                "Invalid combination of pressure, allowable stress, and efficiency (denominator ≤ 0). Reduce P or increase S/E.";
              return;
            }
            tRequired = (P * R) / denom;
          } else {
            // sphere
            const denom = 2 * S * E - 0.2 * P;
            if (denom <= 0) {
              inputError.textContent =
                "Invalid combination of pressure, allowable stress, and efficiency (denominator ≤ 0). Reduce P or increase S/E.";
              return;
            }
            tRequired = (P * R) / denom;
          }
          tNominal = tRequired + c;
          Pdesign = P;
        } else {
          // MAWP for given thickness
          const t = tInput - c;
          if (t <= 0) {
            inputError.textContent =
              "Net thickness after corrosion allowance must be positive.";
            return;
          }
          if (type === "cylinder") {
            Pmawp = (S * E * t) / (R + 0.6 * t);
          } else {
            Pmawp = (2 * S * E * t) / (R + 0.2 * t);
          }
          Pdesign = Pmawp;
          tRequired = t;
          tNominal = tInput;
        }

        // Stresses
        let hoop = null;
        let longitudinal = null;
        let sphereStress = null;

        if (type === "cylinder") {
          hoop = (Pdesign * R) / tRequired;
          longitudinal = (Pdesign * R) / (2 * tRequired);
        } else {
          sphereStress = (Pdesign * R) / (2 * tRequired);
        }

        // Thin-wall check
        const tOverR = tRequired / R;

        // Format units
        const lengthUnit = isMetric ? "mm" : "in";
        const pressureUnit = isMetric ? "MPa" : "psi";
        const stressUnit = pressureUnit;

        // Primary result text
        let primaryText = "";
        if (mode === "thickness") {
          primaryText += `Required structural thickness: ${tRequired.toFixed(
            3
          )} ${lengthUnit}`;
          if (c > 0) {
            primaryText += ` (excluding corrosion allowance).\nNominal thickness with corrosion allowance: ${tNominal.toFixed(
              3
            )} ${lengthUnit}.`;
          }
        } else {
          primaryText += `Maximum allowable working pressure (MAWP): ${Pmawp.toFixed(
            3
          )} ${pressureUnit} (based on net thickness).`;
          if (c > 0) {
            primaryText += `\nInput nominal thickness: ${tNominal.toFixed(
              3
            )} ${lengthUnit}, net thickness after corrosion: ${tRequired.toFixed(
              3
            )} ${lengthUnit}.`;
          } else {
            primaryText += `\nNet thickness used in calculation: ${tRequired.toFixed(
              3
            )} ${lengthUnit}.`;
          }
        }

        primaryResultText.textContent = primaryText;
        primaryResults.classList.remove("hidden");

        // Stress results
        stressList.innerHTML = "";
        if (type === "cylinder") {
          const li1 = document.createElement("li");
          li1.textContent = `Hoop (circumferential) stress σh ≈ ${hoop.toFixed(
            2
          )} ${stressUnit}`;
          const li2 = document.createElement("li");
          li2.textContent = `Longitudinal stress σl ≈ ${longitudinal.toFixed(
            2
          )} ${stressUnit}`;
          stressList.appendChild(li1);
          stressList.appendChild(li2);

          const governing = Math.max(hoop, longitudinal);
          if (governing <= S) {
            stressCheck.textContent = `Governing membrane stress (${governing.toFixed(
              2
            )} ${stressUnit}) is ≤ allowable stress S = ${S.toFixed(
              2
            )} ${stressUnit}.`;
            stressCheck.className = "mt-2 text-sm text-green-700";
          } else {
            stressCheck.textContent = `Warning: Governing membrane stress (${governing.toFixed(
              2
            )} ${stressUnit}) exceeds allowable stress S = ${S.toFixed(
              2
            )} ${stressUnit}. Consider increasing thickness, reducing pressure, or using a higher-strength material.`;
            stressCheck.className = "mt-2 text-sm text-red-700";
          }
        } else {
          const li1 = document.createElement("li");
          li1.textContent = `Membrane stress σ ≈ ${sphereStress.toFixed(
            2
          )} ${stressUnit}`;
          stressList.appendChild(li1);

          if (sphereStress <= S) {
            stressCheck.textContent = `Membrane stress (${sphereStress.toFixed(
              2
            )} ${stressUnit}) is ≤ allowable stress S = ${S.toFixed(
              2
            )} ${stressUnit}.`;
            stressCheck.className = "mt-2 text-sm text-green-700";
          } else {
            stressCheck.textContent = `Warning: Membrane stress (${sphereStress.toFixed(
              2
            )} ${stressUnit}) exceeds allowable stress S = ${S.toFixed(
              2
            )} ${stressUnit}. Consider increasing thickness, reducing pressure, or using a higher-strength material.`;
            stressCheck.className = "mt-2 text-sm text-red-700";
          }
        }
        stressResults.classList.remove("hidden");

        // Thin-wall validity
        thinWallText.textContent =
          `t/R = ${tOverR.toFixed(3)}. ` +
          (tOverR <= 0.1
            ? "This is within the typical thin-wall range (t/R ≤ 0.1), so thin-wall formulas are reasonably applicable."
            : "This exceeds the typical thin-wall limit (t/R ≤ 0.1). Consider using thick-wall theory or detailed ASME rules.");
        thinWallCheck.classList.remove("hidden");
      }

      calcButton.addEventListener("click", calculate);

      resetButton.addEventListener("click", () => {
        innerDiameterEl.value = "";
        designPressureEl.value = "";
        wallThicknessEl.value = "";
        allowableStressEl.value = "";
        efficiencyEl.value = "1";
        corrosionAllowanceEl.value = "0";
        inputError.textContent = "";
        primaryResults.classList.add("hidden");
        stressResults.classList.add("hidden");
        thinWallCheck.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
