    <script>
      // Tab switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modeSections = document.querySelectorAll(".mode-section");
      let currentMode = "sdof";

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.getAttribute("data-mode");
          currentMode = mode;
          modeTabs.forEach((t) => {
            t.classList.remove("bg-white", "text-blue-600", "border-gray-200");
            t.classList.add("text-gray-600");
            if (t !== tab) t.classList.add("border-transparent");
          });
          tab.classList.add("bg-white", "text-blue-600", "border-gray-200");
          tab.classList.remove("text-gray-600", "border-transparent");

          modeSections.forEach((sec) => sec.classList.add("hidden"));
          document.getElementById("mode-" + mode).classList.remove("hidden");

          document.getElementById("nf-results").classList.add("hidden");
          document.getElementById("nf-error").classList.add("hidden");
        });
      });

      function parseOrNull(v) {
        const x = parseFloat(v);
        return isNaN(x) ? null : x;
      }

      function roundSmart(val, digits = 4) {
        if (!isFinite(val)) return "–";
        if (val === 0) return "0";
        const abs = Math.abs(val);
        if (abs >= 1000 || abs < 0.001) {
          return val.toExponential(3);
        }
        return val
          .toFixed(digits)
          .replace(/\.0+$/, "")
          .replace(/(\.\d*?)0+$/, "$1");
      }

      function unitToSI(value, unit) {
        const v = value;
        switch (unit) {
          case "kg":
            return v;
          case "g":
            return v / 1000;
          case "lb":
            return v * 0.45359237;
          case "slug":
            return v * 14.59390294;

          case "N/m":
            return v;
          case "kN/m":
            return v * 1000;
          case "lbf/in":
            return v * 175.126835; // 1 lbf/in = 175.126835 N/m

          case "N·s/m":
            return v;
          case "kN·s/m":
            return v * 1000;
          case "lbf·s/in":
            return v * 175.126835;

          case "Pa":
            return v;
          case "MPa":
            return v * 1e6;
          case "GPa":
            return v * 1e9;
          case "psi":
            return v * 6894.75729;

          case "m4":
            return v;
          case "cm4":
            return v * 1e-8;
          case "in4":
            return v * 4.162314e-7;

          case "kg/m3":
            return v;
          case "g/cm3":
            return v * 1000;
          case "lb/ft3":
            return v * 16.018463;

          case "m2":
            return v;
          case "cm2":
            return v * 1e-4;
          case "in2":
            return v * 0.00064516;

          case "m":
            return v;
          case "cm":
            return v / 100;
          case "mm":
            return v / 1000;
          case "in":
            return v * 0.0254;
          case "ft":
            return v * 0.3048;

          case "kg·m2":
            return v;
          case "lb·ft2":
            return v * 0.04214011;
          case "lb·in2":
            return v * 0.0002926397;

          default:
            return v;
        }
      }

      document.getElementById("nf-calc-btn").addEventListener("click", () => {
        const errEl = document.getElementById("nf-error");
        const resEl = document.getElementById("nf-results");
        errEl.classList.add("hidden");
        errEl.textContent = "";
        resEl.classList.add("hidden");

        let omegaN = null;
        let zeta = 0;

        try {
          if (currentMode === "sdof") {
            const mVal = parseOrNull(
              document.getElementById("sdof-mass").value
            );
            const kVal = parseOrNull(document.getElementById("sdof-k").value);
            if (mVal === null || kVal === null || mVal <= 0 || kVal <= 0) {
              throw new Error(
                "Please enter positive mass and stiffness values."
              );
            }
            const m = unitToSI(
              mVal,
              document.getElementById("sdof-mass-unit").value
            );
            const k = unitToSI(
              kVal,
              document.getElementById("sdof-k-unit").value
            );

            omegaN = Math.sqrt(k / m);

            const cVal = parseOrNull(document.getElementById("sdof-c").value);
            const zetaInput = parseOrNull(
              document.getElementById("sdof-zeta").value
            );

            if (cVal !== null && cVal > 0) {
              const c = unitToSI(
                cVal,
                document.getElementById("sdof-c-unit").value
              );
              zeta = c / (2 * Math.sqrt(k * m));
            } else if (zetaInput !== null && zetaInput >= 0) {
              zeta = zetaInput;
            } else {
              zeta = 0;
            }
          } else if (currentMode === "beam") {
            const eVal = parseOrNull(document.getElementById("beam-e").value);
            const iVal = parseOrNull(document.getElementById("beam-i").value);
            const rhoVal = parseOrNull(
              document.getElementById("beam-rho").value
            );
            const aVal = parseOrNull(document.getElementById("beam-a").value);
            const lVal = parseOrNull(document.getElementById("beam-l").value);
            if (
              [eVal, iVal, rhoVal, aVal, lVal].some((v) => v === null || v <= 0)
            ) {
              throw new Error(
                "Please enter positive values for E, I, ρ, A, and L."
              );
            }
            const E = unitToSI(
              eVal,
              document.getElementById("beam-e-unit").value
            );
            const I = unitToSI(
              iVal,
              document.getElementById("beam-i-unit").value
            );
            const rho = unitToSI(
              rhoVal,
              document.getElementById("beam-rho-unit").value
            );
            const A = unitToSI(
              aVal,
              document.getElementById("beam-a-unit").value
            );
            const L = unitToSI(
              lVal,
              document.getElementById("beam-l-unit").value
            );

            const beta1 = 1.875;
            omegaN =
              Math.pow(beta1, 2) *
              Math.sqrt((E * I) / (rho * A * Math.pow(L, 4)));
            zeta = 0;
          } else if (currentMode === "torsion") {
            const gVal = parseOrNull(document.getElementById("tor-g").value);
            const jVal = parseOrNull(document.getElementById("tor-j").value);
            const lVal = parseOrNull(document.getElementById("tor-l").value);
            const jpVal = parseOrNull(document.getElementById("tor-jp").value);
            if ([gVal, jVal, lVal, jpVal].some((v) => v === null || v <= 0)) {
              throw new Error(
                "Please enter positive values for G, J, L, and J\u209A."
              );
            }
            const G = unitToSI(
              gVal,
              document.getElementById("tor-g-unit").value
            );
            const J = unitToSI(
              jVal,
              document.getElementById("tor-j-unit").value
            );
            const L = unitToSI(
              lVal,
              document.getElementById("tor-l-unit").value
            );
            const Jp = unitToSI(
              jpVal,
              document.getElementById("tor-jp-unit").value
            );

            const kt = (G * J) / L;
            omegaN = Math.sqrt(kt / Jp);
            zeta = 0;
          }

          if (!isFinite(omegaN) || omegaN <= 0) {
            throw new Error(
              "Input combination produced an invalid natural frequency. Please check your values and units."
            );
          }

          const fn = omegaN / (2 * Math.PI);
          let omegaD = omegaN;
          let fd = fn;
          let note = "";

          if (zeta > 0) {
            if (zeta < 1) {
              omegaD = omegaN * Math.sqrt(1 - zeta * zeta);
              fd = omegaD / (2 * Math.PI);
              note =
                "System is underdamped (0 < ζ < 1). Damped natural frequency is slightly lower than the undamped value.";
            } else if (zeta === 1) {
              omegaD = 0;
              fd = 0;
              note =
                "System is critically damped (ζ = 1). No oscillation; response returns to equilibrium without overshoot.";
            } else {
              omegaD = 0;
              fd = 0;
              note =
                "System is overdamped (ζ > 1). No oscillation; response decays slowly without crossing equilibrium.";
            }
          } else {
            note =
              "No damping specified (ζ = 0). Damped and undamped natural frequencies are identical.";
          }

          document.getElementById("res-omega-n").textContent =
            roundSmart(omegaN);
          document.getElementById("res-fn").textContent = roundSmart(fn);
          document.getElementById("res-omega-d").textContent =
            roundSmart(omegaD);
          document.getElementById("res-fd").textContent = roundSmart(fd);
          document.getElementById("res-zeta").textContent = roundSmart(zeta, 4);
          document.getElementById("nf-note").textContent = note;

          resEl.classList.remove("hidden");
        } catch (e) {
          errEl.textContent = e.message;
          errEl.classList.remove("hidden");
        }
      });

      document.getElementById("nf-reset-btn").addEventListener("click", () => {
        document
          .querySelectorAll("#nf-form input")
          .forEach((inp) => (inp.value = ""));
        document.getElementById("nf-results").classList.add("hidden");
        const errEl = document.getElementById("nf-error");
        errEl.classList.add("hidden");
        errEl.textContent = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
