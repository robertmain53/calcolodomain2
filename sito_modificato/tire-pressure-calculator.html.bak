    <script>
      // Utility conversions
      function lbToKg(lb) {
        return lb * 0.45359237;
      }
      function kgToLb(kg) {
        return kg / 0.45359237;
      }
      function cToK(c) {
        return c + 273.15;
      }
      function fToK(f) {
        return ((f - 32) * 5) / 9 + 273.15;
      }

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function computePressures() {
        const vehicleType = document.getElementById("vehicleType").value;
        const tireConstruction =
          document.getElementById("tireConstruction").value;

        let riderWeight =
          parseFloat(document.getElementById("riderWeight").value) || 0;
        let bikeWeight =
          parseFloat(document.getElementById("bikeWeight").value) || 0;
        let extraLoad =
          parseFloat(document.getElementById("extraLoad").value) || 0;

        const weightUnit = document.getElementById("weightUnit").value;
        const bikeWeightUnit = document.getElementById("bikeWeightUnit").value;
        const extraLoadUnit = document.getElementById("extraLoadUnit").value;

        if (weightUnit === "lb") riderWeight = lbToKg(riderWeight);
        if (bikeWeightUnit === "lb") bikeWeight = lbToKg(bikeWeight);
        if (extraLoadUnit === "lb") extraLoad = lbToKg(extraLoad);

        let totalMass = riderWeight + bikeWeight + extraLoad;
        if (!isFinite(totalMass) || totalMass <= 0) totalMass = 85; // fallback

        const frontLoadPct =
          parseFloat(document.getElementById("frontLoadPct").value) || 45;
        const frontFrac = clamp(frontLoadPct / 100, 0.35, 0.6);
        const rearFrac = 1 - frontFrac;

        const tireWidthUnit = document.getElementById("tireWidthUnit").value;
        let tireWidth =
          parseFloat(document.getElementById("tireWidth").value) || 28;
        let widthMm;

        if (vehicleType === "car" || tireWidthUnit === "inch") {
          // car: width is in inches, convert to mm approx (section width not exact but ok for scaling)
          widthMm = tireWidth * 25.4;
        } else {
          widthMm = tireWidth;
        }

        const terrain = document.getElementById("terrain").value;
        const condition = document.getElementById("condition").value;
        const maxTirePsi =
          parseFloat(document.getElementById("maxTirePsi").value) || null;

        let temp = parseFloat(document.getElementById("temp").value);
        const tempUnit = document.getElementById("tempUnit").value;
        let tempK = null;
        if (!isNaN(temp)) {
          tempK = tempUnit === "C" ? cToK(temp) : fToK(temp);
        }

        // Reference values
        let refMass, refPsiRear, refWidthMm;
        if (vehicleType === "car") {
          refMass = 1500; // kg total vehicle + occupants
          refPsiRear = 35;
          refWidthMm = 205;
        } else {
          refMass = 85; // rider + bike
          refPsiRear = 75;
          refWidthMm = 28;
        }

        const g = 9.81;
        const refLoadPerWheel = (refMass * g) / 2;
        const totalLoad = totalMass * g;
        const frontLoad = totalLoad * frontFrac;
        const rearLoad = totalLoad * rearFrac;

        // Base rear pressure from load scaling
        let baseRearPsi = refPsiRear * Math.sqrt(rearLoad / refLoadPerWheel);
        let baseFrontPsi = baseRearPsi * Math.sqrt(frontLoad / rearLoad);

        // Width adjustment (inverse proportional)
        const widthFactor = clamp(refWidthMm / widthMm, 0.4, 2.5);
        baseFrontPsi *= widthFactor;
        baseRearPsi *= widthFactor;

        // Construction factor
        let constrFactor = 1.0;
        if (tireConstruction === "tubeless") constrFactor = 0.93;
        else if (tireConstruction === "tubeless-ready") constrFactor = 0.96;
        baseFrontPsi *= constrFactor;
        baseRearPsi *= constrFactor;

        // Terrain factor
        let terrainFactor = 1.0;
        if (terrain === "rough") terrainFactor = 0.95;
        else if (terrain === "gravel") terrainFactor = 0.9;
        else if (terrain === "loose") terrainFactor = 0.85;
        baseFrontPsi *= terrainFactor;
        baseRearPsi *= terrainFactor;

        // Condition factor
        let condFactor = 1.0;
        if (condition === "wet") {
          condFactor = vehicleType === "car" ? 1.0 : 0.95;
        } else if (condition === "comfort") {
          condFactor = 0.95;
        } else if (condition === "race") {
          condFactor = 1.05;
        }
        baseFrontPsi *= condFactor;
        baseRearPsi *= condFactor;

        // Temperature correction (optional, relative to 20°C)
        if (tempK) {
          const refK = cToK(20);
          const tempFactor = tempK / refK;
          baseFrontPsi *= tempFactor;
          baseRearPsi *= tempFactor;
        }

        // Clamp to reasonable ranges
        let minPsi, maxPsi;
        if (vehicleType === "car") {
          minPsi = 26;
          maxPsi = 50;
        } else if (vehicleType === "mtb") {
          minPsi = 15;
          maxPsi = 40;
        } else if (vehicleType === "gravel") {
          minPsi = 25;
          maxPsi = 65;
        } else if (vehicleType === "commuter") {
          minPsi = 35;
          maxPsi = 80;
        } else {
          // road
          minPsi = 50;
          maxPsi = 100;
        }

        if (maxTirePsi) {
          maxPsi = Math.min(maxPsi, maxTirePsi);
        }

        baseFrontPsi = clamp(baseFrontPsi, minPsi, maxPsi);
        baseRearPsi = clamp(baseRearPsi, minPsi, maxPsi);

        // Build ranges
        function buildRange(psi) {
          const span = Math.max(4, psi * 0.08); // ±8% or at least ±4 PSI
          const min = clamp(psi - span / 2, minPsi, maxPsi);
          const max = clamp(psi + span / 2, minPsi, maxPsi);
          const comfort = clamp(psi - span * 0.25, minPsi, maxPsi);
          const race = clamp(psi + span * 0.25, minPsi, maxPsi);
          return { main: psi, min, max, comfort, race };
        }

        const front = buildRange(baseFrontPsi);
        const rear = buildRange(baseRearPsi);

        // Update UI
        const f = document.getElementById.bind(document);

        f("frontPsiMain").textContent = front.main.toFixed(0);
        f("frontPsiMin").textContent = front.min.toFixed(0);
        f("frontPsiMax").textContent = front.max.toFixed(0);
        f("frontPsiComfort").textContent = front.comfort.toFixed(0);
        f("frontPsiRace").textContent = front.race.toFixed(0);

        f("rearPsiMain").textContent = rear.main.toFixed(0);
        f("rearPsiMin").textContent = rear.min.toFixed(0);
        f("rearPsiMax").textContent = rear.max.toFixed(0);
        f("rearPsiComfort").textContent = rear.comfort.toFixed(0);
        f("rearPsiRace").textContent = rear.race.toFixed(0);

        const warningEl = document.getElementById("warningText");
        warningEl.textContent = "";

        if (maxTirePsi && (front.main > maxTirePsi || rear.main > maxTirePsi)) {
          warningEl.textContent =
            "Warning: recommended pressure exceeds the max PSI you entered. Reduce load or use a higher-volume tire.";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const frontLoadInput = document.getElementById("frontLoadPct");
        const frontLoadLabel = document.getElementById("frontLoadLabel");
        frontLoadInput.addEventListener("input", () => {
          frontLoadLabel.textContent = frontLoadInput.value;
        });

        document
          .getElementById("calcBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            computePressures();
          });

        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("vehicleType").value = "road";
            document.getElementById("tireConstruction").value = "tubeless";
            document.getElementById("riderWeight").value = 75;
            document.getElementById("weightUnit").value = "kg";
            document.getElementById("bikeWeight").value = 10;
            document.getElementById("bikeWeightUnit").value = "kg";
            document.getElementById("extraLoad").value = 0;
            document.getElementById("extraLoadUnit").value = "kg";
            document.getElementById("tireWidth").value = 28;
            document.getElementById("tireWidthUnit").value = "mm";
            document.getElementById("wheelSize").value = "700c";
            document.getElementById("maxTirePsi").value = "";
            document.getElementById("terrain").value = "smooth";
            document.getElementById("condition").value = "normal";
            document.getElementById("temp").value = "";
            document.getElementById("tempUnit").value = "C";
            document.getElementById("frontLoadPct").value = 45;
            document.getElementById("frontLoadLabel").textContent = "45";
            document.getElementById("warningText").textContent = "";
            computePressures();
          });

        // Recompute on key parameter changes for responsiveness
        [
          "vehicleType",
          "tireConstruction",
          "riderWeight",
          "bikeWeight",
          "extraLoad",
          "weightUnit",
          "bikeWeightUnit",
          "extraLoadUnit",
          "tireWidth",
          "tireWidthUnit",
          "terrain",
          "condition",
          "frontLoadPct",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("change", computePressures);
          el.addEventListener("blur", computePressures);
        });

        computePressures();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
