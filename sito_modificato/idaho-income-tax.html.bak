    <script>
      // --- Idaho tax model (simplified & maintainable) ---
      // NOTE: Brackets and deductions are approximate and should be updated when Idaho law changes.
      const IDAHO_CONFIG = {
        2025: {
          standardDeduction: {
            single: 15000,
            mfs: 15000,
            mfj: 30000,
            hoh: 22500,
          },
          dependentCredit: 205, // per qualifying dependent (approximate)
          brackets: {
            // taxable income thresholds and marginal rates
            single: [
              { upTo: 3000, rate: 0.01 },
              { upTo: 6000, rate: 0.03 },
              { upTo: 9000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            mfs: [
              { upTo: 3000, rate: 0.01 },
              { upTo: 6000, rate: 0.03 },
              { upTo: 9000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            mfj: [
              { upTo: 6000, rate: 0.01 },
              { upTo: 12000, rate: 0.03 },
              { upTo: 18000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            hoh: [
              { upTo: 4500, rate: 0.01 },
              { upTo: 9000, rate: 0.03 },
              { upTo: 13500, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
          },
        },
        2024: {
          // For now, assume same as 2025; adjust when official 2024 rules differ.
          standardDeduction: {
            single: 15000,
            mfs: 15000,
            mfj: 30000,
            hoh: 22500,
          },
          dependentCredit: 205,
          brackets: {
            single: [
              { upTo: 3000, rate: 0.01 },
              { upTo: 6000, rate: 0.03 },
              { upTo: 9000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            mfs: [
              { upTo: 3000, rate: 0.01 },
              { upTo: 6000, rate: 0.03 },
              { upTo: 9000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            mfj: [
              { upTo: 6000, rate: 0.01 },
              { upTo: 12000, rate: 0.03 },
              { upTo: 18000, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
            hoh: [
              { upTo: 4500, rate: 0.01 },
              { upTo: 9000, rate: 0.03 },
              { upTo: 13500, rate: 0.045 },
              { upTo: Infinity, rate: 0.059 },
            ],
          },
        },
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function calculateBracketTax(taxableIncome, brackets) {
        let remaining = taxableIncome;
        let lastCap = 0;
        let tax = 0;

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const cap = bracket.upTo;
          const span = Math.min(remaining, cap - lastCap);
          if (span > 0) {
            tax += span * bracket.rate;
            remaining -= span;
          }
          lastCap = cap;
        }
        return tax;
      }

      function calculateIdahoTax() {
        const errorEl = document.getElementById("calc-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const year = parseInt(document.getElementById("tax-year").value, 10);
        const status = document.getElementById("filing-status").value;

        const wages = parseFloat(document.getElementById("wages").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("other-income").value) || 0;
        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        const itemized =
          parseFloat(document.getElementById("itemized-deductions").value) || 0;
        const dependents = Math.max(
          0,
          parseInt(document.getElementById("dependents").value, 10) || 0
        );
        const otherCredits = Math.max(
          0,
          parseFloat(document.getElementById("other-credits").value) || 0
        );
        const withholding = Math.max(
          0,
          parseFloat(document.getElementById("withholding").value) || 0
        );

        if (!IDAHO_CONFIG[year]) {
          errorEl.textContent = "Unsupported tax year selected.";
          errorEl.classList.remove("hidden");
          return;
        }

        const cfg = IDAHO_CONFIG[year];

        const grossIncome = Math.max(0, wages + otherIncome);

        let deduction =
          cfg.standardDeduction[status] || cfg.standardDeduction.single;
        if (deductionType === "itemized") {
          deduction = Math.max(0, itemized);
        }

        const taxableIncome = Math.max(0, grossIncome - deduction);

        const brackets = cfg.brackets[status] || cfg.brackets.single;
        let taxBeforeCredits = calculateBracketTax(taxableIncome, brackets);

        const dependentCredits = dependents * cfg.dependentCredit;
        const totalCredits = Math.min(
          taxBeforeCredits,
          dependentCredits + otherCredits
        );
        const taxAfterCredits = Math.max(0, taxBeforeCredits - totalCredits);

        const effectiveRate =
          grossIncome > 0 ? taxAfterCredits / grossIncome : 0;

        const net = withholding - taxAfterCredits;
        const isRefund = net >= 0;
        const refundAbs = Math.abs(net);

        // Update UI
        document.getElementById("result-gross-income").textContent =
          formatCurrency(grossIncome);
        document.getElementById("result-deduction").textContent =
          "-" + formatCurrency(deduction);
        document.getElementById("result-taxable").textContent =
          formatCurrency(taxableIncome);

        document.getElementById("result-dependent-credits").textContent =
          "-" + formatCurrency(dependentCredits);
        document.getElementById("result-other-credits").textContent =
          "-" + formatCurrency(otherCredits);
        document.getElementById("result-withheld").textContent =
          "-" + formatCurrency(withholding);

        document.getElementById("result-tax").textContent =
          formatCurrency(taxAfterCredits);
        document.getElementById("result-effective-rate").textContent =
          formatPercent(effectiveRate);

        document.getElementById("result-refund").textContent =
          formatCurrency(refundAbs);
        document.getElementById("result-refund-label").textContent = isRefund
          ? "Estimated refund"
          : "Amount you may owe";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const withholdingInput = document.getElementById("withholding");
        const withholdingDisplay = document.getElementById(
          "withholding-display"
        );
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        function updateWithholdingDisplay() {
          const val = parseFloat(withholdingInput.value) || 0;
          withholdingDisplay.textContent = formatCurrency(val);
        }

        withholdingInput.addEventListener("input", updateWithholdingDisplay);

        calcBtn.addEventListener("click", calculateIdahoTax);

        resetBtn.addEventListener("click", () => {
          document.getElementById("filing-status").value = "single";
          document.getElementById("tax-year").value = "2025";
          document.getElementById("wages").value = 65000;
          document.getElementById("other-income").value = 0;
          document.getElementById("deduction-standard").checked = true;
          document.getElementById("itemized-deductions").value = 0;
          document.getElementById("dependents").value = 0;
          document.getElementById("other-credits").value = 0;
          document.getElementById("withholding").value = 6000;
          updateWithholdingDisplay();
          calculateIdahoTax();
        });

        // Initial calculation
        updateWithholdingDisplay();
        calculateIdahoTax();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
