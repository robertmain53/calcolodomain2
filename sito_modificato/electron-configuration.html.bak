          symbol: "Nh",
          name: "Nihonium",
          period: 7,
          group: 13,
          block: "p",
        },
        {
          Z: 114,
          symbol: "Fl",
          name: "Flerovium",
          period: 7,
          group: 14,
          block: "p",
        },
        {
          Z: 115,
          symbol: "Mc",
          name: "Moscovium",
          period: 7,
          group: 15,
          block: "p",
        },
        {
          Z: 116,
          symbol: "Lv",
          name: "Livermorium",
          period: 7,
          group: 16,
          block: "p",
        },
        {
          Z: 117,
          symbol: "Ts",
          name: "Tennessine",
          period: 7,
          group: 17,
          block: "p",
        },
        {
          Z: 118,
          symbol: "Og",
          name: "Oganesson",
          period: 7,
          group: 18,
          block: "p",
        },
      ];

      // Noble gases for shorthand
      const NOBLE_GASES = [
        { Z: 2, symbol: "He" },
        { Z: 10, symbol: "Ne" },
        { Z: 18, symbol: "Ar" },
        { Z: 36, symbol: "Kr" },
        { Z: 54, symbol: "Xe" },
        { Z: 86, symbol: "Rn" },
        { Z: 118, symbol: "Og" },
      ];

      // Known configuration exceptions (ground state) as strings
      const EXCEPTIONS = {
        24: "1s2 2s2 2p6 3s2 3p6 3d5 4s1", // Cr
        29: "1s2 2s2 2p6 3s2 3p6 3d10 4s1", // Cu
        41: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d4 5s1", // Nb
        42: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d5 5s1", // Mo
        44: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d7 5s1", // Ru
        45: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d8 5s1", // Rh
        46: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10", // Pd
        47: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s1", // Ag
        57: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 5d1 6s2", // La
        58: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 4f1 5d1 6s2", // Ce
        64: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 4f7 5d1 6s2", // Gd
        78: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 4f14 5d9 6s1", // Pt
        79: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 4f14 5d10 6s1", // Au
        89: "1s2 2s2 2p6 3s2 3p6 3d10 4s2 4p6 4d10 5s2 5p6 4f14 5d10 6s2 6p6 6d1 7s2", // Ac
      };

      // Subshell capacities and l values
      const SUBSHELL_INFO = {
        s: { capacity: 2, l: 0 },
        p: { capacity: 6, l: 1 },
        d: { capacity: 10, l: 2 },
        f: { capacity: 14, l: 3 },
      };

      // Aufbau order up to 7p
      const AUFBAU_ORDER = [
        "1s",
        "2s",
        "2p",
        "3s",
        "3p",
        "4s",
        "3d",
        "4p",
        "5s",
        "4d",
        "5p",
        "6s",
        "4f",
        "5d",
        "6p",
        "7s",
        "5f",
        "6d",
        "7p",
      ];

      function parseSubshell(label) {
        const match = label.match(/^(\d)([spdf])$/);
        if (!match) return null;
        return { n: parseInt(match[1], 10), type: match[2] };
      }

      function buildAufbauConfig(Z) {
        let remaining = Z;
        const config = [];
        for (const sub of AUFBAU_ORDER) {
          if (remaining <= 0) break;
          const info = parseSubshell(sub);
          const cap = SUBSHELL_INFO[info.type].capacity;
          const electrons = Math.min(cap, remaining);
          config.push({ n: info.n, type: info.type, electrons });
          remaining -= electrons;
        }
        return config;
      }

      function configToString(config) {
        return config
          .map((s) => `${s.n}${s.type}<sup>${s.electrons}</sup>`)
          .join(" ");
      }

      function configToPlainString(config) {
        return config.map((s) => `${s.n}${s.type}${s.electrons}`).join(" ");
      }

      function parseConfigString(str) {
        const parts = str.trim().split(/\s+/);
        const config = [];
        for (const p of parts) {
          const m = p.match(/^(\d)([spdf])(\d{1,2})$/);
          if (!m) continue;
          config.push({
            n: parseInt(m[1], 10),
            type: m[2],
            electrons: parseInt(m[3], 10),
          });
        }
        return config;
      }

      function getElementByInput(input) {
        const val = input.trim();
        if (!val) return null;
        // numeric atomic number
        if (/^\d+$/.test(val)) {
          const Z = parseInt(val, 10);
          return ELEMENTS.find((e) => e.Z === Z) || null;
        }
        // symbol or name (case-insensitive)
        const lower = val.toLowerCase();
        return (
          ELEMENTS.find(
            (e) =>
              e.symbol.toLowerCase() === lower || e.name.toLowerCase() === lower
          ) || null
        );
      }

      function getNobleGasCore(Z) {
        let core = null;
        for (const ng of NOBLE_GASES) {
          if (ng.Z < Z && (!core || ng.Z > core.Z)) core = ng;
        }
        return core;
      }

      function subtractConfig(config, Zcore) {
        let remaining = Zcore;
        const result = [];
        for (const sub of config) {
          if (remaining <= 0) {
            result.push({ ...sub });
          } else if (remaining >= sub.electrons) {
            remaining -= sub.electrons;
          } else {
            result.push({
              n: sub.n,
              type: sub.type,
              electrons: sub.electrons - remaining,
            });
            remaining = 0;
          }
        }
        return result;
      }

      function getValenceInfo(config) {
        if (!config.length) return { shell: "–", valence: "–", highest: "–" };
        const maxN = Math.max(...config.map((s) => s.n));
        const valenceElectrons = config
          .filter((s) => s.n === maxN)
          .reduce((sum, s) => sum + s.electrons, 0);
        const highestSub = config[config.length - 1];
        return {
          shell: maxN,
          valence: valenceElectrons,
          highest: `${highestSub.n}${highestSub.type}<sup>${highestSub.electrons}</sup>`,
        };
      }

      function buildOrbitalDiagram(config) {
        // For each subshell, determine number of orbitals and distribute electrons with Hund's rule
        const container = document.getElementById("orbital-diagram");
        container.innerHTML = "";
        if (!config.length) {
          container.innerHTML =
            '<p class="text-gray-500">No configuration available.</p>';
          return;
        }

        const subshells = config.map((s) => ({
          label: `${s.n}${s.type}`,
          n: s.n,
          type: s.type,
          electrons: s.electrons,
          l: SUBSHELL_INFO[s.type].l,
          orbitals: SUBSHELL_INFO[s.type].capacity / 2,
        }));

        const wrapper = document.createElement("div");
        wrapper.className = "inline-flex flex-col space-y-2";

        subshells.forEach((sub) => {
          const row = document.createElement("div");
          row.className = "flex items-center space-x-2";

          const label = document.createElement("div");
          label.className = "w-10 text-right font-semibold";
          label.textContent = sub.label;
          row.appendChild(label);

          const orbitalsDiv = document.createElement("div");
          orbitalsDiv.className = "flex items-center space-x-1";

          // Hund's rule: first fill each orbital with one up-spin, then pair
          let remaining = sub.electrons;
          const electronsPerOrbital = new Array(sub.orbitals).fill(0);
          // first pass: one electron per orbital
          for (let i = 0; i < sub.orbitals && remaining > 0; i++) {
            electronsPerOrbital[i] = 1;
            remaining--;
          }
          // second pass: pair
          for (let i = 0; i < sub.orbitals && remaining > 0; i++) {
            electronsPerOrbital[i] = 2;
            remaining--;
          }

          electronsPerOrbital.forEach((count) => {
            const cell = document.createElement("div");
            cell.className =
              "orbital-cell border border-gray-400 flex items-center justify-center px-1";
            if (count === 0) {
              cell.innerHTML = "&nbsp;";
            } else if (count === 1) {
              cell.innerHTML =
                '<span class="orbital-arrow text-blue-700">↑</span>';
            } else {
              cell.innerHTML =
                '<span class="orbital-arrow text-blue-700 mr-0.5">↑</span><span class="orbital-arrow text-red-700">↓</span>';
            }
            orbitalsDiv.appendChild(cell);
          });

          row.appendChild(orbitalsDiv);
          wrapper.appendChild(row);
        });

        container.appendChild(wrapper);
      }

      function getLastElectronQuantumNumbers(config) {
        if (!config.length) return { n: "–", l: "–", ml: "–", ms: "–" };
        const last = config[config.length - 1];
        const l = SUBSHELL_INFO[last.type].l;
        const numOrbitals = SUBSHELL_INFO[last.type].capacity / 2;
        const mlValues = [];
        for (let m = -l; m <= l; m++) mlValues.push(m);

        // Determine which orbital and spin the last electron occupies using Hund's rule logic
        let electrons = last.electrons;
        let orbitalIndex = 0;
        let spin = "+1/2";

        if (electrons <= numOrbitals) {
          // only single occupancy, fill each orbital with up spin
          orbitalIndex = electrons - 1;
          spin = "+1/2";
        } else {
          // first each orbital has one up, remaining are down
          const remaining = electrons - numOrbitals;
          orbitalIndex = remaining - 1;
          spin = "-1/2";
        }

        if (electrons === 0) {
          return { n: last.n, l: l, ml: "–", ms: "–" };
        }

        const ml = mlValues[orbitalIndex] ?? mlValues[mlValues.length - 1];

        return {
          n: last.n,
          l: l,
          ml: ml,
          ms: spin,
        };
      }

      function updateUIForElement(element) {
        const errorEl = document.getElementById("error-message");
        errorEl.classList.add("hidden");
        if (!element) {
          errorEl.textContent =
            "Element not found. Please check the spelling, symbol, or atomic number.";
          errorEl.classList.remove("hidden");
          return;
        }

        const Z = element.Z;
        let config;
        if (EXCEPTIONS[Z]) {
          config = parseConfigString(EXCEPTIONS[Z]);
        } else {
          config = buildAufbauConfig(Z);
        }

        const fullConfigHTML = configToString(config);
        const core = getNobleGasCore(Z);
        let shorthandHTML = fullConfigHTML;
        if (core) {
          const coreConfig = buildAufbauConfig(core.Z);
          const remainder = subtractConfig(config, core.Z);
          shorthandHTML = `[${core.symbol}] ` + configToString(remainder);
        }

        const valence = getValenceInfo(config);
        const qn = getLastElectronQuantumNumbers(config);

        document.getElementById("res-element-name").textContent = element.name;
        document.getElementById("res-symbol").textContent = element.symbol;
        document.getElementById("res-atomic-number").textContent = element.Z;
        document.getElementById("res-period-group").textContent = `${
          element.period
        } / ${element.group || "–"}`;
        document.getElementById("res-block").textContent =
          element.block.toUpperCase();

        document.getElementById("res-full-config").innerHTML = fullConfigHTML;
        document.getElementById("res-shorthand-config").innerHTML =
          shorthandHTML;

        document.getElementById("res-valence-shell").textContent =
          valence.shell;
        document.getElementById("res-valence-electrons").textContent =
          valence.valence;
        document.getElementById("res-highest-subshell").innerHTML =
          valence.highest;

        document.getElementById("res-qn-n").textContent = qn.n;
        document.getElementById("res-qn-l").textContent = qn.l;
        document.getElementById("res-qn-ml").textContent = qn.ml;
        document.getElementById("res-qn-ms").textContent = qn.ms;

        if (document.getElementById("show-orbital-diagram-toggle").checked) {
          buildOrbitalDiagram(config);
          document
            .getElementById("orbital-diagram-section")
            .classList.remove("hidden");
        } else {
          document
            .getElementById("orbital-diagram-section")
            .classList.add("hidden");
        }
      }

      function clearResults() {
        document.getElementById("element-input").value = "";
        document.getElementById("element-select").value = "";
        document.getElementById("error-message").classList.add("hidden");

        document.getElementById("res-element-name").textContent = "–";
        document.getElementById("res-symbol").textContent = "–";
        document.getElementById("res-atomic-number").textContent = "–";
        document.getElementById("res-period-group").textContent = "–";
        document.getElementById("res-block").textContent = "–";
        document.getElementById("res-full-config").innerHTML = "–";
        document.getElementById("res-shorthand-config").innerHTML = "–";
        document.getElementById("res-valence-shell").textContent = "–";
        document.getElementById("res-valence-electrons").textContent = "–";
        document.getElementById("res-highest-subshell").innerHTML = "–";
        document.getElementById("res-qn-n").textContent = "–";
        document.getElementById("res-qn-l").textContent = "–";
        document.getElementById("res-qn-ml").textContent = "–";
        document.getElementById("res-qn-ms").textContent = "–";
        document.getElementById("orbital-diagram").innerHTML =
          '<p class="text-gray-500">No element selected yet.</p>';
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Populate select
        const select = document.getElementById("element-select");
        const defaultOpt = document.createElement("option");
        defaultOpt.value = "";
        defaultOpt.textContent = "Select element…";
        select.appendChild(defaultOpt);
        ELEMENTS.forEach((e) => {
          const opt = document.createElement("option");
          opt.value = e.Z;
          opt.textContent = `${e.Z} – ${e.name} (${e.symbol})`;
          select.appendChild(opt);
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", () => {
            const inputVal = document.getElementById("element-input").value;
            const element = getElementByInput(
              inputVal || document.getElementById("element-select").value
            );
            updateUIForElement(element);
          });

        document
          .getElementById("element-select")
          .addEventListener("change", (e) => {
            if (!e.target.value) return;
            const Z = parseInt(e.target.value, 10);
            const element = ELEMENTS.find((el) => el.Z === Z);
            document.getElementById("element-input").value = element
              ? element.symbol
              : "";
            updateUIForElement(element);
          });

        document
          .getElementById("element-input")
          .addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              document.getElementById("calculate-btn").click();
            }
          });

        document.getElementById("clear-btn").addEventListener("click", () => {
          clearResults();
        });

        document
          .getElementById("show-orbital-diagram-toggle")
          .addEventListener("change", (e) => {
            const section = document.getElementById("orbital-diagram-section");
            if (e.target.checked) {
              section.classList.remove("hidden");
            } else {
              section.classList.add("hidden");
            }
          });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const content = btn.parentElement.querySelector("div");
              content.classList.toggle("hidden");
            });
          });
      });
    </script>

