    <script>
      // Tabs
      const tabButtons = document.querySelectorAll('.tab-btn');
      const panels = {
        'pct-of': document.getElementById('panel-pct-of'),
        'what-pct': document.getElementById('panel-what-pct'),
        'pct-change': document.getElementById('panel-pct-change'),
        'add-sub': document.getElementById('panel-add-sub')
      };
      tabButtons.forEach(btn => {
        btn.addEventListener('click', () => {
          tabButtons.forEach(b => b.classList.remove('active'));
          btn.classList.add('active');
          const target = btn.dataset.tab;
          Object.keys(panels).forEach(k => panels[k].classList.add('hidden'));
          panels[target].classList.remove('hidden');
          // clear result? we keep last
        });
      });

      const resValue = document.getElementById('res-value');
      const resSteps = document.getElementById('res-steps');

      document.getElementById('btn-pct-of').addEventListener('click', () => {
        const pct = parseFloat(document.getElementById('pct-of-percent').value);
        const base = parseFloat(document.getElementById('pct-of-base').value);
        if (isNaN(pct) || isNaN(base)) {
          resValue.textContent = '—';
          resSteps.textContent = 'Please enter valid numbers.';
          return;
        }
        const result = base * pct / 100;
        resValue.textContent = result.toFixed(6).replace(/\.?0+$/,'');
        resSteps.textContent = `Formula: (percent ÷ 100) × base\n= (${pct} ÷ 100) × ${base}\n= ${pct/100} × ${base}\n= ${result}`;
      });

      document.getElementById('btn-what-pct').addEventListener('click', () => {
        const part = parseFloat(document.getElementById('what-pct-part').value);
        const whole = parseFloat(document.getElementById('what-pct-whole').value);
        if (isNaN(part) || isNaN(whole) || whole === 0) {
          resValue.textContent = '—';
          resSteps.textContent = 'Whole cannot be 0.';
          return;
        }
        const result = (part / whole) * 100;
        resValue.textContent = result.toFixed(6).replace(/\.?0+$/,'') + '%';
        resSteps.textContent = `Formula: (part ÷ whole) × 100\n= (${part} ÷ ${whole}) × 100\n= ${(part/whole).toFixed(6)} × 100\n= ${result}%`;
      });

      document.getElementById('btn-pct-change').addEventListener('click', () => {
        const oldVal = parseFloat(document.getElementById('pct-change-old').value);
        const newVal = parseFloat(document.getElementById('pct-change-new').value);
        if (isNaN(oldVal) || isNaN(newVal) || oldVal === 0) {
          resValue.textContent = '—';
          resSteps.textContent = 'Old value cannot be 0.';
          return;
        }
        const diff = newVal - oldVal;
        const pct = (diff / oldVal) * 100;
        const direction = pct > 0 ? 'increase' : (pct < 0 ? 'decrease' : 'no change');
        resValue.textContent = pct.toFixed(6).replace(/\.?0+$/,'') + '% ' + direction;
        resSteps.textContent = `Formula: ((new − old) ÷ old) × 100\n= ((${newVal} − ${oldVal}) ÷ ${oldVal}) × 100\n= (${diff} ÷ ${oldVal}) × 100\n= ${(diff/oldVal).toFixed(6)} × 100\n= ${pct}% (${direction})`;
      });

      document.getElementById('btn-add-sub').addEventListener('click', () => {
        const original = parseFloat(document.getElementById('add-sub-value').value);
        const pct = parseFloat(document.getElementById('add-sub-percent').value);
        const mode = document.querySelector('input[name="add-sub-mode"]:checked').value;
        if (isNaN(original) || isNaN(pct)) {
          resValue.textContent = '—';
          resSteps.textContent = 'Please enter valid numbers.';
          return;
        }
        let result;
        if (mode === 'add') {
          result = original * (1 + pct / 100);
          resValue.textContent = result.toFixed(6).replace(/\.?0+$/,'');
          resSteps.textContent = `Formula: original × (1 + pct/100)\n= ${original} × (1 + ${pct}/100)\n= ${original} × (1 + ${pct/100})\n= ${result}`;
        } else {
          result = original * (1 - pct / 100);
          resValue.textContent = result.toFixed(6).replace(/\.?0+$/,'');
          resSteps.textContent = `Formula: original × (1 − pct/100)\n= ${original} × (1 − ${pct}/100)\n= ${original} × (1 − ${pct/100})\n= ${result}`;
        }
      });
    </script>

