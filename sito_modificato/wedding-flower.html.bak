    <script>
      // Wedding Flower Calculator Logic
      (function () {
        const $ = (id) => document.getElementById(id);

        const pricePerStemBase = {
          budget: 1.0,
          standard: 2.0,
          premium: 3.5,
        };

        const seasonMultiplier = {
          inSeason: 1.0,
          shoulder: 1.15,
          offSeason: 1.3,
        };

        const densityMultiplier = {
          light: 0.8,
          normal: 1.0,
          full: 1.25,
        };

        const bouquetSizeMultiplier = {
          small: 0.8,
          medium: 1.0,
          large: 1.3,
        };

        const centerpieceBaseStems = {
          budvases: 10,
          low: 18,
          tall: 24,
        };

        function formatCurrency(value) {
          if (!isFinite(value)) return "–";
          const rounded = Math.round(value);
          return "$" + rounded.toLocaleString();
        }

        function calculate() {
          const guestCount = Math.max(
            0,
            parseInt($("guestCount").value || "0", 10)
          );
          const tableSize = Math.max(
            1,
            parseInt($("tableSize").value || "8", 10)
          );

          const includeBridal = $("includeBridal").checked;
          const includeBridesmaids = $("includeBridesmaids").checked;
          const includeBoutonnieres = $("includeBoutonnieres").checked;
          const includeCenterpieces = $("includeCenterpieces").checked;
          const includeCeremony = $("includeCeremony").checked;

          const style = $("style").value;
          const season = $("season").value;
          const priceTier = $("priceTier").value;
          const bouquetSize = $("bouquetSize").value;
          const density = $("density").value;
          const centerpieceType = $("centerpieceType").value;

          const tables = guestCount > 0 ? Math.ceil(guestCount / tableSize) : 0;

          // Base stems for a medium, classic bridal bouquet
          let bridalBaseStems = 24;
          if (style === "lush") bridalBaseStems *= 1.2;
          if (style === "airy") bridalBaseStems *= 0.85;

          bridalBaseStems *= bouquetSizeMultiplier[bouquetSize] || 1;
          bridalBaseStems *= densityMultiplier[density] || 1;

          const bridesmaidBaseStems = bridalBaseStems * 0.65;
          const boutonniereStems = 4; // average including greenery

          let centerpieceStems = centerpieceBaseStems[centerpieceType] || 18;
          centerpieceStems *= densityMultiplier[density] || 1;

          // Ceremony decor: treat as 4 units (arch, 2x large, aisle markers)
          let ceremonyUnitStems = 60;
          if (style === "lush") ceremonyUnitStems *= 1.25;
          if (style === "airy") ceremonyUnitStems *= 0.85;
          ceremonyUnitStems *= densityMultiplier[density] || 1;
          const ceremonyUnits = 4;

          // Quantities
          const qty = {
            bridal: includeBridal ? 1 : 0,
            bridesmaids: includeBridesmaids ? 4 : 0, // default 4 bridesmaids
            boutonnieres: includeBoutonnieres ? 8 : 0, // groom, groomsmen, parents
            centerpieces: includeCenterpieces ? tables : 0,
            ceremony: includeCeremony ? ceremonyUnits : 0,
          };

          // Stems per arrangement (rounded)
          const stemsEach = {
            bridal: Math.round(bridalBaseStems),
            bridesmaids: Math.round(bridesmaidBaseStems),
            boutonnieres: boutonniereStems,
            centerpieces: Math.round(centerpieceStems),
            ceremony: Math.round(ceremonyUnitStems),
          };

          const totalStemsByType = {
            bridal: qty.bridal * stemsEach.bridal,
            bridesmaids: qty.bridesmaids * stemsEach.bridesmaids,
            boutonnieres: qty.boutonnieres * stemsEach.boutonnieres,
            centerpieces: qty.centerpieces * stemsEach.centerpieces,
            ceremony: qty.ceremony * stemsEach.ceremony,
          };

          const totalStems = Object.values(totalStemsByType).reduce(
            (a, b) => a + b,
            0
          );

          // Pricing
          const baseStemPrice = pricePerStemBase[priceTier] || 2.0;
          const seasonMult = seasonMultiplier[season] || 1.0;
          const stemPrice = baseStemPrice * seasonMult;

          const totalCost = totalStems * stemPrice;
          const lowCost = totalCost * 0.8;
          const highCost = totalCost * 1.25;

          // Update summary
          $("totalStems").textContent = totalStems
            ? totalStems.toLocaleString() + " stems"
            : "–";
          $("centerpieceCount").textContent = qty.centerpieces
            ? qty.centerpieces.toString()
            : "0";
          $("totalBudget").textContent = totalStems
            ? formatCurrency(totalCost)
            : "–";
          $("budgetRange").textContent = totalStems
            ? `Low–high range: ${formatCurrency(lowCost)} – ${formatCurrency(
                highCost
              )}`
            : "Low–high range: –";

          // Breakdown table
          const breakdownBody = $("breakdownBody");
          breakdownBody.innerHTML = "";

          const rows = [
            { key: "bridal", label: "Bridal bouquet" },
            { key: "bridesmaids", label: "Bridesmaid bouquets" },
            { key: "boutonnieres", label: "Boutonnieres & corsages" },
            { key: "centerpieces", label: "Reception centerpieces" },
            { key: "ceremony", label: "Ceremony decor units" },
          ];

          rows.forEach((row) => {
            const q = qty[row.key];
            if (!q) return;
            const se = stemsEach[row.key];
            const ts = totalStemsByType[row.key];
            const cost = ts * stemPrice;

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${row.label}</td>
          <td class="px-3 py-2 text-right text-gray-700">${q}</td>
          <td class="px-3 py-2 text-right text-gray-700">${se}</td>
          <td class="px-3 py-2 text-right text-gray-700">${ts.toLocaleString()}</td>
          <td class="px-3 py-2 text-right text-gray-900 font-medium">${formatCurrency(
            cost
          )}</td>
        `;
            breakdownBody.appendChild(tr);
          });

          if (!breakdownBody.children.length) {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td colspan="5" class="px-3 py-3 text-center text-gray-500">
            Select at least one arrangement type to see a breakdown.
          </td>
        `;
            breakdownBody.appendChild(tr);
          }
        }

        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculate);

        // Auto-calc on load
        window.addEventListener("DOMContentLoaded", calculate);
      })();
    </script>

