  <script>
    const ftInput = document.getElementById('ft-input');
    const psiOutput = document.getElementById('psi-output');
    const psiInput = document.getElementById('psi-input');
    const ftOutput = document.getElementById('ft-output');
    const factorLabel = document.getElementById('factor-label');
    const inverseLabel = document.getElementById('inverse-label');
    const tempButtons = document.querySelectorAll('.mode-pill');

    const tbl1 = document.getElementById('tbl-1ft');
    const tbl5 = document.getElementById('tbl-5ft');
    const tbl10 = document.getElementById('tbl-10ft');
    const tbl25 = document.getElementById('tbl-25ft');
    const tbl50 = document.getElementById('tbl-50ft');

    let currentFactor = 0.4335;
    let isUpdating = false;

    function updateFromFeet() {
      if (isUpdating) return;
      isUpdating = true;
      const ft = parseFloat(ftInput.value || 0);
      const psi = ft * currentFactor;
      psiOutput.value = +psi.toFixed(4);
      isUpdating = false;
    }

    function updateFromPsiReverse() {
      if (isUpdating) return;
      isUpdating = true;
      const psi = parseFloat(psiInput.value || 0);
      const ft = currentFactor > 0 ? psi / currentFactor : 0;
      ftOutput.value = +ft.toFixed(2);
      isUpdating = false;
    }

    function updateLabels() {
      factorLabel.textContent = currentFactor.toFixed(4) + ' psi per ft';
      inverseLabel.textContent = (1 / currentFactor).toFixed(3) + ' ft per psi';
    }

    function updateTable() {
      tbl1.textContent = (1 * currentFactor).toFixed(4);
      tbl5.textContent = (5 * currentFactor).toFixed(4);
      tbl10.textContent = (10 * currentFactor).toFixed(4);
      tbl25.textContent = (25 * currentFactor).toFixed(4);
      tbl50.textContent = (50 * currentFactor).toFixed(4);
    }

    tempButtons.forEach(btn => {
      btn.addEventListener('click', () => {
        tempButtons.forEach(b => b.classList.remove('mode-pill-active'));
        btn.classList.add('mode-pill-active');
        currentFactor = parseFloat(btn.dataset.factor);
        updateLabels();
        updateFromFeet();
        updateFromPsiReverse();
        updateTable();
      });
    });

    ftInput.addEventListener('input', updateFromFeet);
    psiInput.addEventListener('input', updateFromPsiReverse);

    // init
    updateLabels();
    updateFromFeet();
    updateFromPsiReverse();
    updateTable();
  </script>

