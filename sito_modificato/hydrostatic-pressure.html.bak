    <script>
      // Unit conversion helpers
      const densityToSI = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "kg_m3":
            return v;
          case "g_cm3":
            return v * 1000; // 1 g/cm3 = 1000 kg/m3
          case "lb_ft3":
            return v * 16.018463; // 1 lb/ft3 ≈ 16.018463 kg/m3
          default:
            return v;
        }
      };

      const depthToSI = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m":
            return v;
          case "cm":
            return v / 100;
          case "ft":
            return v * 0.3048;
          default:
            return v;
        }
      };

      const pressureToSI = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "pa":
            return v;
          case "kpa":
            return v * 1e3;
          case "bar":
            return v * 1e5;
          case "atm":
            return v * 101325;
          case "psi":
            return v * 6894.757293;
          default:
            return v;
        }
      };

      const pressureFromSI = (pa, unit) => {
        switch (unit) {
          case "pa":
            return pa;
          case "kpa":
            return pa / 1e3;
          case "bar":
            return pa / 1e5;
          case "atm":
            return pa / 101325;
          case "psi":
            return pa / 6894.757293;
          default:
            return pa;
        }
      };

      const formatNumber = (x, digits = 3) => {
        if (!isFinite(x)) return "–";
        if (Math.abs(x) >= 1e4 || (Math.abs(x) < 1e-2 && x !== 0)) {
          return x.toExponential(digits - 1);
        }
        return Number(x.toFixed(digits)).toString();
      };

      // Elements
      const fluidSelect = document.getElementById("fluid-select");
      const densityInput = document.getElementById("density-input");
      const densityUnit = document.getElementById("density-unit");
      const depthInput = document.getElementById("depth-input");
      const depthUnit = document.getElementById("depth-unit");
      const gravityInput = document.getElementById("gravity-input");
      const gravityResetBtn = document.getElementById("gravity-reset");
      const surfacePressureInput = document.getElementById(
        "surface-pressure-input"
      );
      const surfacePressureUnit = document.getElementById(
        "surface-pressure-unit"
      );
      const calculateBtn = document.getElementById("calculate-btn");
      const swapDepthPressureBtn = document.getElementById(
        "swap-depth-pressure-btn"
      );
      const resultMain = document.getElementById("result-main");
      const resultSub = document.getElementById("result-sub");
      const resultList = document.getElementById("result-list");
      const formulaText = document.getElementById("formula-text");

      let mode = "pressure"; // 'pressure' or 'depth'

      const fluidPresets = {
        custom: null,
        water_fresh: 1000,
        seawater: 1025,
        mercury: 13600,
        oil: 850,
        concrete: 1000, // pore water approx
      };

      function applyFluidPreset() {
        const key = fluidSelect.value;
        const rho = fluidPresets[key];
        if (rho) {
          densityUnit.value = "kg_m3";
          densityInput.value = rho;
          densityInput.disabled = true;
          densityUnit.disabled = true;
        } else {
          densityInput.disabled = false;
          densityUnit.disabled = false;
        }
      }

      fluidSelect.addEventListener("change", () => {
        applyFluidPreset();
      });

      gravityResetBtn.addEventListener("click", () => {
        gravityInput.value = 9.80665;
      });

      // Preset buttons
      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const d = Number(btn.dataset.presetDepth);
          const fluid = btn.dataset.presetFluid;
          if (isFinite(d)) {
            depthUnit.value = "m";
            depthInput.value = d;
          }
          if (fluid) {
            fluidSelect.value = fluid;
            applyFluidPreset();
          }
          mode = "pressure";
          updateModeUI();
          calculate();
        });
      });

      function updateModeUI() {
        if (mode === "pressure") {
          swapDepthPressureBtn.textContent = "Given pressure → find depth";
          resultSub.textContent =
            "Calculating hydrostatic pressure from depth.";
        } else {
          swapDepthPressureBtn.textContent = "Given depth → find pressure";
          resultSub.textContent =
            "Calculating depth from hydrostatic pressure.";
        }
      }

      swapDepthPressureBtn.addEventListener("click", () => {
        mode = mode === "pressure" ? "depth" : "pressure";
        updateModeUI();
      });

      function calculate() {
        const rhoSI = densityToSI(densityInput.value, densityUnit.value);
        const g = Number(gravityInput.value);
        const hSI = depthToSI(depthInput.value, depthUnit.value);
        const pSurfaceSI = pressureToSI(
          surfacePressureInput.value,
          surfacePressureUnit.value
        );
        const pressureType =
          document.querySelector('input[name="pressure-type"]:checked')
            ?.value || "gauge";

        if (!isFinite(rhoSI) || rhoSI <= 0 || !isFinite(g) || g <= 0) {
          resultMain.textContent = "Check density and gravity inputs.";
          return;
        }

        if (mode === "pressure") {
          if (!isFinite(hSI) || hSI < 0) {
            resultMain.textContent = "Enter a valid depth.";
            return;
          }
          const pGauge = rhoSI * g * hSI;
          const pAbs = pSurfaceSI + pGauge;
          const pDisplay = pressureType === "gauge" ? pGauge : pAbs;

          resultMain.textContent = `Pressure: ${formatNumber(
            pDisplay,
            4
          )} Pa (${pressureType})`;
          const bar = pDisplay / 1e5;
          const psi = pDisplay / 6894.757293;
          const kPa = pDisplay / 1e3;
          const equivWaterDepth = pDisplay / (1000 * 9.80665); // m of water

          resultList.innerHTML = `
        <li>SI pressure: ${formatNumber(pDisplay, 4)} Pa (${formatNumber(
            kPa,
            4
          )} kPa)</li>
        <li>In bar: ${formatNumber(bar, 4)} bar</li>
        <li>In psi: ${formatNumber(psi, 4)} psi</li>
        <li>Equivalent water column: ${formatNumber(
          equivWaterDepth,
          4
        )} m H₂O</li>
      `;

          formulaText.textContent =
            pressureType === "gauge"
              ? "Gauge pressure: p = ρ g h. Absolute pressure: p_abs = p_surface + ρ g h."
              : "Absolute pressure: p_abs = p_surface + ρ g h. Gauge pressure: p_gauge = ρ g h.";
        } else {
          // mode === 'depth', solve for h from given pressure difference
          const targetPressureSI = pressureToSI(
            depthInput.value,
            depthUnit.value
          ); // reuse depth input as pressure in chosen unit
          if (!isFinite(targetPressureSI)) {
            resultMain.textContent = "Enter a valid target pressure.";
            return;
          }

          let pGauge;
          if (pressureType === "gauge") {
            pGauge = targetPressureSI;
          } else {
            // absolute: subtract surface
            pGauge = targetPressureSI - pSurfaceSI;
          }
          if (pGauge <= 0) {
            resultMain.textContent =
              "Target pressure must exceed surface pressure.";
            return;
          }
          const h = pGauge / (rhoSI * g); // m
          const h_ft = h / 0.3048;

          resultMain.textContent = `Required depth: ${formatNumber(h, 4)} m`;
          resultList.innerHTML = `
        <li>Depth in meters: ${formatNumber(h, 4)} m</li>
        <li>Depth in feet: ${formatNumber(h_ft, 4)} ft</li>
        <li>Gauge pressure: ${formatNumber(pGauge, 4)} Pa (${formatNumber(
            pGauge / 1e3,
            4
          )} kPa)</li>
        <li>Absolute pressure: ${formatNumber(pSurfaceSI + pGauge, 4)} Pa</li>
      `;
          formulaText.textContent =
            "Depth from pressure: h = (p_target − p_surface) / (ρ g).";
        }
      }

      calculateBtn.addEventListener("click", calculate);

      // Initialize
      applyFluidPreset();
      updateModeUI();
    </script>

