    <script>
      // Tabs
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document
            .querySelectorAll(".tab-btn")
            .forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById(target).classList.remove("hidden");
        });
      });

      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? null : v;
      }

      function show(id) {
        document.getElementById(id).classList.remove("hidden");
      }

      function round(val, digits) {
        const f = Math.pow(10, digits);
        return Math.round(val * f) / f;
      }

      // Torque & RPM
      document
        .getElementById("btn-calc-torque")
        .addEventListener("click", () => {
          const torque = toNumber("torque-value");
          const rpm = toNumber("rpm-value");
          if (torque === null || rpm === null || rpm <= 0) {
            alert("Please enter valid torque and RPM values.");
            return;
          }
          const unit = document.getElementById("torque-unit").value;
          let hp;
          if (unit === "lbft") {
            hp = (torque * rpm) / 5252;
          } else {
            hp = (torque * rpm) / 7127;
          }
          const loss = toNumber("drivetrain-loss");
          let whp = hp;
          let bhp = hp;
          if (loss !== null && loss > 0 && loss < 40) {
            // assume entered torque is at wheels, so hp is WHP
            whp = hp;
            bhp = whp / (1 - loss / 100);
          }
          document.getElementById("torque-whp").textContent = round(whp, 1);
          document.getElementById("torque-whp-kw").textContent = round(
            whp * 0.7457,
            1
          );
          document.getElementById("torque-bhp").textContent = round(bhp, 1);
          document.getElementById("torque-bhp-kw").textContent = round(
            bhp * 0.7457,
            1
          );
          show("torque-results");
        });

      // ET
      document.getElementById("btn-calc-et").addEventListener("click", () => {
        let et = toNumber("et-time");
        let weight = toNumber("et-weight");
        if (et === null || et <= 0 || weight === null || weight <= 0) {
          alert("Please enter valid ET and weight.");
          return;
        }
        const dist = document.getElementById("et-distance").value;
        const wUnit = document.getElementById("et-weight-unit").value;
        if (dist === "eighth") {
          // rough convert 1/8 ET to 1/4 ET
          et = et * 1.57;
        }
        if (wUnit === "kg") {
          weight = weight * 2.20462;
        }
        const hp = weight / Math.pow(et / 5.825, 3);
        document.getElementById("et-hp").textContent = round(hp, 1);
        document.getElementById("et-kw").textContent = round(hp * 0.7457, 1);
        show("et-results");
      });

      // Trap speed
      document.getElementById("btn-calc-trap").addEventListener("click", () => {
        let v = toNumber("trap-speed");
        let weight = toNumber("trap-weight");
        if (v === null || v <= 0 || weight === null || weight <= 0) {
          alert("Please enter valid trap speed and weight.");
          return;
        }
        const vUnit = document.getElementById("trap-speed-unit").value;
        const wUnit = document.getElementById("trap-weight-unit").value;
        if (vUnit === "kph") {
          v = v / 1.60934;
        }
        if (wUnit === "kg") {
          weight = weight * 2.20462;
        }
        const hp = weight * Math.pow(v / 234, 3);
        document.getElementById("trap-hp").textContent = round(hp, 1);
        document.getElementById("trap-kw").textContent = round(hp * 0.7457, 1);
        show("trap-results");
      });

      // Boost & displacement (very rough)
      document
        .getElementById("btn-calc-boost")
        .addEventListener("click", () => {
          let disp = toNumber("disp-value");
          const boost = toNumber("boost-value");
          const ve = toNumber("ve-value");
          const rpm = toNumber("rpm-boost");
          if (
            disp === null ||
            disp <= 0 ||
            boost === null ||
            rpm === null ||
            rpm <= 0 ||
            ve === null
          ) {
            alert("Please enter displacement, boost, VE, and RPM.");
            return;
          }
          const dispUnit = document.getElementById("disp-unit").value;
          const boostUnit = document.getElementById("boost-unit").value;

          if (dispUnit === "ci") {
            disp = disp * 0.0163871; // cubic inches to liters
          }
          let boostPsi = boost;
          if (boostUnit === "bar") {
            boostPsi = boost * 14.5038;
          }
          const atmPsi = 14.7;
          const pr = 1 + boostPsi / atmPsi;
          const veFrac = ve / 100;

          // very rough: base NA hp per liter per 1000 rpm at 100% VE
          const baseHpPerLPerKrpm = 1.6; // tuned gasoline engine rough factor
          const naHp = disp * baseHpPerLPerKrpm * (rpm / 1000) * veFrac;
          const boostedHp = naHp * pr * 0.9; // 0.9 fudge for heat/inefficiency

          const hp = boostedHp;
          document.getElementById("boost-hp").textContent = round(hp, 1);
          document.getElementById("boost-kw").textContent = round(
            hp * 0.7457,
            1
          );
          show("boost-results");
        });

      // HP <-> kW
      document.getElementById("btn-hp-to-kw").addEventListener("click", () => {
        const hp = toNumber("conv-hp");
        if (hp === null) {
          alert("Enter horsepower to convert.");
          return;
        }
        document.getElementById("conv-kw").value = round(hp * 0.7457, 3);
      });

      document.getElementById("btn-kw-to-hp").addEventListener("click", () => {
        const kw = toNumber("conv-kw");
        if (kw === null) {
          alert("Enter kilowatts to convert.");
          return;
        }
        document.getElementById("conv-hp").value = round(kw / 0.7457, 3);
      });

      // Reset handlers
      document.querySelectorAll("button[data-reset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const scope = btn.getAttribute("data-reset");
          if (scope === "torque") {
            ["torque-value", "rpm-value", "drivetrain-loss"].forEach(
              (id) => (document.getElementById(id).value = "")
            );
            document.getElementById("torque-results").classList.add("hidden");
          } else if (scope === "et") {
            ["et-time", "et-weight"].forEach(
              (id) => (document.getElementById(id).value = "")
            );
            document.getElementById("et-results").classList.add("hidden");
          } else if (scope === "trap") {
            ["trap-speed", "trap-weight"].forEach(
              (id) => (document.getElementById(id).value = "")
            );
            document.getElementById("trap-results").classList.add("hidden");
          } else if (scope === "boost") {
            ["disp-value", "boost-value", "ve-value", "rpm-boost"].forEach(
              (id) => (document.getElementById(id).value = "")
            );
            document.getElementById("boost-results").classList.add("hidden");
          } else if (scope === "convert") {
            ["conv-hp", "conv-kw"].forEach(
              (id) => (document.getElementById(id).value = "")
            );
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
