    <script>
      // Tabs
      const tabButtons = document.querySelectorAll(".tab-btn");
      const tabPanels = document.querySelectorAll(".tab-panel");
      function activateTab(id) {
        tabButtons.forEach((btn) => {
          if (btn.dataset.tab === id) {
            btn.classList.add("tab-active", "border-blue-600", "text-blue-700");
          } else {
            btn.classList.remove(
              "tab-active",
              "border-blue-600",
              "text-blue-700"
            );
          }
        });
        tabPanels.forEach((panel) => {
          panel.classList.toggle("hidden", panel.id !== id);
        });
      }
      if (tabButtons.length) {
        activateTab("tab-time-temp");
        tabButtons.forEach((btn) => {
          btn.addEventListener("click", () => activateTab(btn.dataset.tab));
        });
      }

      // Helpers
      function cToK(c) {
        return c + 273.15;
      }
      function fToK(f) {
        return ((f - 32) * 5) / 9 + 273.15;
      }
      function kFromValue(v, unit) {
        return unit === "C" ? cToK(v) : fToK(v);
      }
      function clampMinutes(m) {
        return Math.max(1, Math.round(m));
      }

      // Time & temperature
      document.getElementById("btnTimeTemp").addEventListener("click", () => {
        const origTemp = parseFloat(document.getElementById("origTemp").value);
        const newTemp = parseFloat(document.getElementById("newTemp").value);
        const origUnit = document.getElementById("origTempUnit").value;
        const newUnit = document.getElementById("newTempUnit").value;
        const h = parseFloat(document.getElementById("origTimeH").value) || 0;
        const m = parseFloat(document.getElementById("origTimeM").value) || 0;
        const totalMin = h * 60 + m;
        const resultBox = document.getElementById("timeTempResult");

        if (!origTemp || !newTemp || totalMin <= 0) {
          alert(
            "Please enter original temperature, time, and new temperature."
          );
          return;
        }
        const TorigK = kFromValue(origTemp, origUnit);
        const TnewK = kFromValue(newTemp, newUnit);
        if (TorigK <= 0 || TnewK <= 0) {
          alert("Temperatures must be above absolute zero.");
          return;
        }
        const exponent = 1.5;
        const newMin = totalMin * Math.pow(TorigK / TnewK, exponent);
        const clamped = clampMinutes(newMin);
        const resH = Math.floor(clamped / 60);
        const resM = clamped % 60;
        document.getElementById("timeTempH").textContent = resH;
        document.getElementById("timeTempM").textContent = resM;
        document.getElementById("timeTempEarly").textContent = Math.max(
          3,
          Math.round(clamped * 0.1)
        );
        resultBox.classList.remove("hidden");
      });

      // Recipe scaling
      document
        .getElementById("btnScaleFromServings")
        .addEventListener("click", () => {
          const o = parseFloat(document.getElementById("origServings").value);
          const n = parseFloat(document.getElementById("newServings").value);
          if (!o || !n || o <= 0) {
            alert("Enter both original and new servings.");
            return;
          }
          const factor = n / o;
          document.getElementById("scaleFactor").value = factor.toFixed(2);
        });

      document
        .getElementById("btnScaleRecipe")
        .addEventListener("click", () => {
          const factor = parseFloat(
            document.getElementById("scaleFactor").value
          );
          const input = document.getElementById("ingredientsInput").value;
          if (!factor || factor <= 0) {
            alert("Enter a valid scale factor (or compute it from servings).");
            return;
          }
          if (!input.trim()) {
            alert("Paste or type your ingredient list first.");
            return;
          }
          const lines = input.split(/\r?\n/);
          const scaled = lines.map((line) => {
            const match = line.match(/^(\s*)([0-9]*\.?[0-9]+)(\s*)(.*)$/);
            if (!match) return line;
            const prefix = match[1] || "";
            const num = parseFloat(match[2]);
            const space = match[3] || "";
            const rest = match[4] || "";
            if (isNaN(num)) return line;
            const newNum = +(num * factor).toFixed(2);
            return prefix + newNum + space + rest;
          });
          document.getElementById("scaledRecipeOutput").value =
            scaled.join("\n");
          document
            .getElementById("scaledRecipeWrapper")
            .classList.remove("hidden");
        });

      // Cups <-> grams
      const densities = {
        flour: 120,
        sugar: 200,
        brown_sugar: 220,
        butter: 227,
        rice: 195,
        water: 240,
      };
      function volToMl(value, unit) {
        switch (unit) {
          case "cup":
            return value * 240;
          case "tbsp":
            return value * 15;
          case "tsp":
            return value * 5;
          case "ml":
          default:
            return value;
        }
      }
      function mlToVol(ml, unit) {
        switch (unit) {
          case "cup":
            return ml / 240;
          case "tbsp":
            return ml / 15;
          case "tsp":
            return ml / 5;
          case "ml":
          default:
            return ml;
        }
      }

      document
        .getElementById("convertDirection")
        .addEventListener("change", (e) => {
          const dir = e.target.value;
          const volGroup = document.getElementById("volInputGroup");
          const massGroup = document.getElementById("massInputGroup");
          if (dir === "volToMass") {
            volGroup.classList.remove("hidden");
            massGroup.classList.add("hidden");
          } else {
            volGroup.classList.add("hidden");
            massGroup.classList.remove("hidden");
          }
          document.getElementById("convertResult").classList.add("hidden");
        });

      document.getElementById("btnConvert").addEventListener("click", () => {
        const ingredient = document.getElementById("ingredientSelect").value;
        const dir = document.getElementById("convertDirection").value;
        const density = densities[ingredient];
        const resultBox = document.getElementById("convertResult");
        const resultText = document.getElementById("convertResultText");

        if (!density) {
          alert("Unknown ingredient density.");
          return;
        }

        if (dir === "volToMass") {
          const v = parseFloat(document.getElementById("volValue").value);
          const vUnit = document.getElementById("volUnit").value;
          if (!v || v <= 0) {
            alert("Enter a valid volume.");
            return;
          }
          const ml = volToMl(v, vUnit);
          const cups = mlToVol(ml, "cup");
          const grams = cups * density;
          const oz = grams / 28.3495;
          resultText.textContent = `${v} ${vUnit} ≈ ${grams.toFixed(
            1
          )} g (${oz.toFixed(2)} oz) of this ingredient.`;
        } else {
          const m = parseFloat(document.getElementById("massValue").value);
          const mUnit = document.getElementById("massUnit").value;
          if (!m || m <= 0) {
            alert("Enter a valid weight.");
            return;
          }
          const grams = mUnit === "g" ? m : m * 28.3495;
          const cups = grams / density;
          const ml = cups * 240;
          const tbsp = mlToVol(ml, "tbsp");
          resultText.textContent = `${m} ${mUnit} ≈ ${cups.toFixed(
            2
          )} cup(s) ≈ ${tbsp.toFixed(1)} tbsp of this ingredient.`;
        }
        resultBox.classList.remove("hidden");
      });

      // Roast helper
      document.getElementById("btnRoast").addEventListener("click", () => {
        const adults =
          parseFloat(document.getElementById("roastAdults").value) || 0;
        const children =
          parseFloat(document.getElementById("roastChildren").value) || 0;
        const leftovers = document.getElementById("roastLeftovers").value;
        const type = document.getElementById("roastType").value;
        const weightUnit = document.getElementById("roastWeightUnit").value;
        let ovenTemp = parseFloat(document.getElementById("roastTemp").value);
        const ovenUnit = document.getElementById("roastTempUnit").value;

        if (adults <= 0 && children <= 0) {
          alert("Enter at least one adult or child.");
          return;
        }

        // Portion per adult (kg)
        let perAdultKg;
        if (type === "turkey") {
          perAdultKg = leftovers === "yes" ? 0.68 : 0.45;
        } else {
          // chicken
          perAdultKg = 0.4;
        }
        const perChildKg = perAdultKg * 0.5;
        let totalKg = adults * perAdultKg + children * perChildKg;

        // Round to nearest 0.25 kg
        totalKg = Math.max(1, Math.round(totalKg * 4) / 4);

        // Default oven temps if none entered
        if (!ovenTemp || ovenTemp <= 0) {
          if (type === "turkey") {
            ovenTemp = ovenUnit === "C" ? 180 : 350;
          } else {
            ovenTemp = ovenUnit === "C" ? 190 : 375;
          }
          document.getElementById("roastTemp").value = ovenTemp;
        }

        // Base time per kg at reference temp (180C / 350F)
        const baseMinPerKg = type === "turkey" ? 29 : 25;
        const refK = cToK(180);
        const ovenK = kFromValue(ovenTemp, ovenUnit);
        const exponent = 1.3;
        let totalMin =
          totalKg * baseMinPerKg * Math.pow(refK / ovenK, exponent);
        totalMin = clampMinutes(totalMin);
        const h = Math.floor(totalMin / 60);
        const m = totalMin % 60;

        const sizeText =
          weightUnit === "kg"
            ? `${totalKg.toFixed(2)} kg (${(totalKg * 2.20462).toFixed(1)} lb)`
            : `${(totalKg * 2.20462).toFixed(1)} lb (${totalKg.toFixed(2)} kg)`;

        document.getElementById("roastSize").textContent = sizeText;
        document.getElementById(
          "roastTime"
        ).textContent = `${h} hours ${m} minutes (approx.)`;
        document.getElementById("roastResult").classList.remove("hidden");
      });

      // Mobile menu toggle (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
