    <script>
      // Utility functions
      function fmtMoney(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "–";
        return (
          value.toLocaleString(undefined, {
            minimumFractionDigits: 1,
            maximumFractionDigits: 1,
          }) + "%"
        );
      }

      function calcPayment(principal, annualRate, months) {
        if (principal <= 0 || months <= 0)
          return { payment: 0, interest: 0, total: 0 };
        const r = annualRate / 100 / 12;
        if (r === 0) {
          const payment = principal / months;
          return { payment, interest: 0, total: principal };
        }
        const factor = Math.pow(1 + r, months);
        const payment = (principal * r * factor) / (factor - 1);
        const total = payment * months;
        const interest = total - principal;
        return { payment, interest, total };
      }

      function calcDTI(income, existingDebts, newPayment) {
        if (income <= 0) return NaN;
        return ((existingDebts + newPayment) / income) * 100;
      }

      // Sync term years <-> months
      const termYearsEl = document.getElementById("loanTermYears");
      const termMonthsEl = document.getElementById("loanTermMonths");

      function syncFromYears() {
        const years = parseFloat(termYearsEl.value);
        if (isFinite(years) && years > 0) {
          termMonthsEl.value = Math.round(years * 12);
        }
      }
      function syncFromMonths() {
        const months = parseFloat(termMonthsEl.value);
        if (isFinite(months) && months > 0) {
          termYearsEl.value = (months / 12).toFixed(2).replace(/\.00$/, "");
        }
      }
      termYearsEl.addEventListener("input", syncFromYears);
      termMonthsEl.addEventListener("input", syncFromMonths);

      // Quick amount buttons
      document.querySelectorAll(".quick-amount").forEach((btn) => {
        btn.addEventListener("click", () => {
          const amt = parseFloat(btn.dataset.amount || "0");
          if (amt > 0) {
            document.getElementById("loanAmount").value = amt;
          }
        });
      });

      // Main calculation
      function runCalculation() {
        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        const amount =
          parseFloat(document.getElementById("loanAmount").value) || 0;
        const fees = parseFloat(document.getElementById("fees").value) || 0;
        const months =
          parseFloat(document.getElementById("loanTermMonths").value) || 0;
        const rateSolo =
          parseFloat(document.getElementById("rateSolo").value) || 0;
        const rateCosign =
          parseFloat(document.getElementById("rateCosign").value) || 0;

        if (amount <= 0 || months <= 0) {
          errorEl.textContent = "Please enter a positive loan amount and term.";
          return;
        }
        if (rateSolo < 0 || rateCosign < 0) {
          errorEl.textContent = "Interest rates cannot be negative.";
          return;
        }

        const principal = amount + Math.max(0, fees);

        const solo = calcPayment(principal, rateSolo, months);
        const cosign = calcPayment(principal, rateCosign, months);

        const borrowerIncome =
          parseFloat(document.getElementById("borrowerIncome").value) || 0;
        const cosignerIncome =
          parseFloat(document.getElementById("cosignerIncome").value) || 0;
        const borrowerDebts =
          parseFloat(document.getElementById("borrowerDebts").value) || 0;
        const cosignerDebts =
          parseFloat(document.getElementById("cosignerDebts").value) || 0;

        const soloDTIVal = calcDTI(borrowerIncome, borrowerDebts, solo.payment);
        const cosignDTIVal = calcDTI(
          borrowerIncome + cosignerIncome,
          borrowerDebts + cosignerDebts,
          cosign.payment
        );

        // Summary cards
        document.getElementById("soloPayment").textContent = fmtMoney(
          solo.payment
        );
        document.getElementById("soloInterest").textContent = fmtMoney(
          solo.interest
        );
        document.getElementById("soloTotal").textContent = fmtMoney(solo.total);
        document.getElementById("soloDTI").textContent = isNaN(soloDTIVal)
          ? "–"
          : fmtPercent(soloDTIVal);

        document.getElementById("cosignPayment").textContent = fmtMoney(
          cosign.payment
        );
        document.getElementById("cosignInterest").textContent = fmtMoney(
          cosign.interest
        );
        document.getElementById("cosignTotal").textContent = fmtMoney(
          cosign.total
        );
        document.getElementById("cosignDTI").textContent = isNaN(cosignDTIVal)
          ? "–"
          : fmtPercent(cosignDTIVal);

        // Comparison deltas
        const deltaPay = cosign.payment - solo.payment;
        const deltaInt = cosign.interest - solo.interest;
        const deltaDTIVal = cosignDTIVal - soloDTIVal;

        const deltaPayEl = document.getElementById("deltaPayment");
        const deltaIntEl = document.getElementById("deltaInterest");
        const deltaDTIEl = document.getElementById("deltaDTI");

        if (isFinite(deltaPay) && deltaPay !== 0) {
          const label = deltaPay < 0 ? "Lower by " : "Higher by ";
          deltaPayEl.textContent = label + fmtMoney(Math.abs(deltaPay));
        } else {
          deltaPayEl.textContent = "No change";
        }

        if (isFinite(deltaInt) && deltaInt !== 0) {
          const label = deltaInt < 0 ? "Save " : "Pay ";
          deltaIntEl.textContent = label + fmtMoney(Math.abs(deltaInt));
        } else {
          deltaIntEl.textContent = "No change";
        }

        if (isFinite(deltaDTIVal) && deltaDTIVal !== 0) {
          const label = deltaDTIVal < 0 ? "Improves by " : "Worsens by ";
          deltaDTIEl.textContent = label + fmtPercent(Math.abs(deltaDTIVal));
        } else {
          deltaDTIEl.textContent = "No change";
        }

        // Table
        document.getElementById("soloRateCell").textContent =
          fmtPercent(rateSolo);
        document.getElementById("soloPaymentCell").textContent = fmtMoney(
          solo.payment
        );
        document.getElementById("soloInterestCell").textContent = fmtMoney(
          solo.interest
        );
        document.getElementById("soloTotalCell").textContent = fmtMoney(
          solo.total
        );
        document.getElementById("soloDTICell").textContent = isNaN(soloDTIVal)
          ? "–"
          : fmtPercent(soloDTIVal);

        document.getElementById("cosignRateCell").textContent =
          fmtPercent(rateCosign);
        document.getElementById("cosignPaymentCell").textContent = fmtMoney(
          cosign.payment
        );
        document.getElementById("cosignInterestCell").textContent = fmtMoney(
          cosign.interest
        );
        document.getElementById("cosignTotalCell").textContent = fmtMoney(
          cosign.total
        );
        document.getElementById("cosignDTICell").textContent = isNaN(
          cosignDTIVal
        )
          ? "–"
          : fmtPercent(cosignDTIVal);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", runCalculation);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("loanAmount").value = 20000;
        document.getElementById("fees").value = 0;
        document.getElementById("loanTermYears").value = 5;
        document.getElementById("loanTermMonths").value = 60;
        document.getElementById("rateSolo").value = 12;
        document.getElementById("rateCosign").value = 8;
        document.getElementById("borrowerIncome").value = 3500;
        document.getElementById("cosignerIncome").value = 4500;
        document.getElementById("borrowerDebts").value = 600;
        document.getElementById("cosignerDebts").value = 400;
        document.getElementById("errorMsg").textContent = "";
        runCalculation();
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const content = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = content.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              content.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      // Initial calculation on load
      document.addEventListener("DOMContentLoaded", runCalculation);
    </script>

