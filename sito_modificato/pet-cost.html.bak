    <script>
      // Utility: format currency based on selected code
      function formatCurrency(value, currencyCode) {
        if (!isFinite(value)) return "-";
        try {
          return new Intl.NumberFormat(undefined, {
            style: "currency",
            currency: currencyCode,
            maximumFractionDigits: 0,
          }).format(value);
        } catch (e) {
          // Fallback
          const symbol =
            currencyCode === "EUR" ? "€" : currencyCode === "GBP" ? "£" : "$";
          return symbol + value.toFixed(0);
        }
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value.replace(",", "."));
        return isNaN(v) || v < 0 ? 0 : v;
      }

      function loadTypicalCosts() {
        const type = document.getElementById("pet-type").value;
        const size = document.getElementById("pet-size").value;

        // Defaults based on broad survey ranges (USD-equivalent)
        let food = 60,
          insurance = 35,
          routineVet = 25,
          grooming = 20,
          preventative = 20,
          meds = 0,
          boarding = 30,
          toys = 20;

        if (type === "dog") {
          if (size === "small") {
            food = 50;
            insurance = 35;
            routineVet = 25;
            grooming = 30;
            preventative = 20;
            boarding = 35;
            toys = 20;
          } else if (size === "medium") {
            food = 70;
            insurance = 45;
            routineVet = 30;
            grooming = 35;
            preventative = 25;
            boarding = 40;
            toys = 25;
          } else if (size === "large" || size === "giant") {
            food = 90;
            insurance = 55;
            routineVet = 35;
            grooming = 40;
            preventative = 30;
            boarding = 50;
            toys = 30;
          }
        } else if (type === "cat") {
          food = 45;
          insurance = 30;
          routineVet = 25;
          grooming = 10;
          preventative = 15;
          boarding = 25;
          toys = 15;
        } else if (type === "small") {
          food = 25;
          insurance = 15;
          routineVet = 15;
          grooming = 5;
          preventative = 10;
          boarding = 15;
          toys = 10;
        } else if (type === "bird") {
          food = 20;
          insurance = 0;
          routineVet = 15;
          grooming = 0;
          preventative = 5;
          boarding = 15;
          toys = 10;
        }

        document.getElementById("food").value = food;
        document.getElementById("insurance").value = insurance;
        document.getElementById("routine-vet").value = routineVet;
        document.getElementById("grooming").value = grooming;
        document.getElementById("preventative").value = preventative;
        document.getElementById("meds").value = meds;
        document.getElementById("boarding").value = boarding;
        document.getElementById("toys").value = toys;
      }

      function calculatePetCost() {
        const currency = document.getElementById("currency").value;
        const yearsRaw = parseFloat(
          document.getElementById("years").value.replace(",", ".")
        );
        const years =
          isNaN(yearsRaw) || yearsRaw <= 0 ? 1 : Math.min(yearsRaw, 30);

        // One-off
        const purchase = getNumber("purchase-cost");
        const initialVet = getNumber("initial-vet");
        const spayNeuter = getNumber("spay-neuter");
        const microchip = getNumber("microchip");
        const supplies = getNumber("supplies");
        const trainingSetup = getNumber("training-setup");

        const oneOffTotal =
          purchase +
          initialVet +
          spayNeuter +
          microchip +
          supplies +
          trainingSetup;

        // Monthly recurring
        const food = getNumber("food");
        const insurance = getNumber("insurance");
        const routineVet = getNumber("routine-vet");
        const grooming = getNumber("grooming");
        const preventative = getNumber("preventative");
        const meds = getNumber("meds");
        const boarding = getNumber("boarding");
        const toys = getNumber("toys");

        const monthlyRecurring =
          food +
          insurance +
          routineVet +
          grooming +
          preventative +
          meds +
          boarding +
          toys;

        // Annual extras
        const emergencyAnnual = getNumber("emergency-annual");
        const travelAnnual = getNumber("travel-annual");
        const annualExtras = emergencyAnnual + travelAnnual;
        const monthlyExtras = annualExtras / 12;

        const monthlyTotal = monthlyRecurring + monthlyExtras;
        const yearlyTotal = monthlyTotal * 12;
        const lifetimeTotal = yearlyTotal * years + oneOffTotal;

        // Update summary
        document.getElementById("result-monthly").textContent = formatCurrency(
          monthlyTotal,
          currency
        );
        document.getElementById("result-yearly").textContent = formatCurrency(
          yearlyTotal,
          currency
        );
        document.getElementById("result-lifetime").textContent = formatCurrency(
          lifetimeTotal,
          currency
        );

        // Breakdown table
        const breakdown = [
          {
            label: "One‑off setup costs (total)",
            monthly: oneOffTotal / (years * 12),
            yearly: oneOffTotal / years,
            lifetime: oneOffTotal,
          },
          {
            label: "Food",
            monthly: food,
            yearly: food * 12,
            lifetime: food * 12 * years,
          },
          {
            label: "Insurance",
            monthly: insurance,
            yearly: insurance * 12,
            lifetime: insurance * 12 * years,
          },
          {
            label: "Routine vet care",
            monthly: routineVet,
            yearly: routineVet * 12,
            lifetime: routineVet * 12 * years,
          },
          {
            label: "Grooming",
            monthly: grooming,
            yearly: grooming * 12,
            lifetime: grooming * 12 * years,
          },
          {
            label: "Preventative treatments",
            monthly: preventative,
            yearly: preventative * 12,
            lifetime: preventative * 12 * years,
          },
          {
            label: "Medications / supplements",
            monthly: meds,
            yearly: meds * 12,
            lifetime: meds * 12 * years,
          },
          {
            label: "Boarding / pet sitting / dog walker",
            monthly: boarding,
            yearly: boarding * 12,
            lifetime: boarding * 12 * years,
          },
          {
            label: "Toys, treats & misc.",
            monthly: toys,
            yearly: toys * 12,
            lifetime: toys * 12 * years,
          },
          {
            label: "Emergency fund & travel (annual extras)",
            monthly: monthlyExtras,
            yearly: annualExtras,
            lifetime: annualExtras * years,
          },
        ];

        const tbody = document.getElementById("breakdown-body");
        tbody.innerHTML = "";
        breakdown.forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-700">${row.label}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          row.monthly,
          currency
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          row.yearly,
          currency
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          row.lifetime,
          currency
        )}</td>
      `;
          tbody.appendChild(tr);
        });

        document.getElementById("results-note").textContent =
          "Note: One‑off setup costs are shown as a separate line but also included in the lifetime total. " +
          "All values are estimates based on the numbers you entered.";

        document.getElementById("results-section").classList.remove("hidden");
      }

      function resetCalculator() {
        document.getElementById("currency").value = "USD";
        document.getElementById("pet-type").value = "dog";
        document.getElementById("pet-size").value = "small";
        document.getElementById("years").value = 12;

        document.getElementById("purchase-cost").value = 300;
        document.getElementById("initial-vet").value = 250;
        document.getElementById("spay-neuter").value = 300;
        document.getElementById("microchip").value = 60;
        document.getElementById("supplies").value = 250;
        document.getElementById("training-setup").value = 150;

        document.getElementById("food").value = 70;
        document.getElementById("insurance").value = 45;
        document.getElementById("routine-vet").value = 30;
        document.getElementById("grooming").value = 35;
        document.getElementById("preventative").value = 25;
        document.getElementById("meds").value = 0;
        document.getElementById("boarding").value = 40;
        document.getElementById("toys").value = 25;

        document.getElementById("emergency-annual").value = 300;
        document.getElementById("travel-annual").value = 150;

        document.getElementById("results-section").classList.add("hidden");
      }

      document
        .getElementById("load-typical")
        .addEventListener("click", loadTypicalCosts);
      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculatePetCost);
      document
        .getElementById("reset-btn")
        .addEventListener("click", resetCalculator);
    </script>

