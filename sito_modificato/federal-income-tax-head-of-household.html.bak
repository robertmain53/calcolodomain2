    <script>
      // --- Tax data (simplified, based on IRS publications; update as needed) ---
      const TAX_DATA = {
        2024: {
          standardDeduction: {
            HOH: 22000,
            SINGLE: 14600,
            MFS: 14600,
            MFJ: 29200,
          },
          brackets: {
            HOH: [
              { rate: 0.1, upTo: 16750 },
              { rate: 0.12, upTo: 63950 },
              { rate: 0.22, upTo: 102900 },
              { rate: 0.24, upTo: 197300 },
              { rate: 0.32, upTo: 250525 },
              { rate: 0.35, upTo: 600150 },
              { rate: 0.37, upTo: Infinity },
            ],
            SINGLE: [
              { rate: 0.1, upTo: 11600 },
              { rate: 0.12, upTo: 47150 },
              { rate: 0.22, upTo: 100525 },
              { rate: 0.24, upTo: 191950 },
              { rate: 0.32, upTo: 243725 },
              { rate: 0.35, upTo: 609350 },
              { rate: 0.37, upTo: Infinity },
            ],
            MFJ: [
              { rate: 0.1, upTo: 23200 },
              { rate: 0.12, upTo: 94300 },
              { rate: 0.22, upTo: 201050 },
              { rate: 0.24, upTo: 383900 },
              { rate: 0.32, upTo: 487450 },
              { rate: 0.35, upTo: 731200 },
              { rate: 0.37, upTo: Infinity },
            ],
          },
          childTaxCreditPerChild: 2000,
          ctcPhaseoutStartHOH: 200000,
        },
        2025: {
          // Placeholder: assume modest inflation adjustments; replace with official IRS numbers when available
          standardDeduction: {
            HOH: 22500,
            SINGLE: 15000,
            MFS: 15000,
            MFJ: 30000,
          },
          brackets: {
            HOH: [
              { rate: 0.1, upTo: 17200 },
              { rate: 0.12, upTo: 65500 },
              { rate: 0.22, upTo: 105500 },
              { rate: 0.24, upTo: 202000 },
              { rate: 0.32, upTo: 256000 },
              { rate: 0.35, upTo: 614000 },
              { rate: 0.37, upTo: Infinity },
            ],
            SINGLE: [
              { rate: 0.1, upTo: 11900 },
              { rate: 0.12, upTo: 48500 },
              { rate: 0.22, upTo: 103000 },
              { rate: 0.24, upTo: 197000 },
              { rate: 0.32, upTo: 250000 },
              { rate: 0.35, upTo: 625000 },
              { rate: 0.37, upTo: Infinity },
            ],
            MFJ: [
              { rate: 0.1, upTo: 23800 },
              { rate: 0.12, upTo: 97000 },
              { rate: 0.22, upTo: 206000 },
              { rate: 0.24, upTo: 394000 },
              { rate: 0.32, upTo: 500000 },
              { rate: 0.35, upTo: 750000 },
              { rate: 0.37, upTo: Infinity },
            ],
          },
          childTaxCreditPerChild: 2000,
          ctcPhaseoutStartHOH: 200000,
        },
      };

      function formatCurrency(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function computeTaxFromBrackets(taxableIncome, brackets) {
        let remaining = taxableIncome;
        let lastLimit = 0;
        let totalTax = 0;
        const breakdown = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const upper = bracket.upTo;
          const amountInBracket = Math.max(
            0,
            Math.min(remaining, upper - lastLimit)
          );
          const taxForBracket = amountInBracket * bracket.rate;
          if (amountInBracket > 0) {
            breakdown.push({
              from: lastLimit,
              to: upper,
              amount: amountInBracket,
              rate: bracket.rate,
              tax: taxForBracket,
            });
          }
          totalTax += taxForBracket;
          remaining -= amountInBracket;
          lastLimit = upper;
        }

        const marginalBracket = breakdown.length
          ? breakdown[breakdown.length - 1]
          : null;
        return {
          totalTax,
          breakdown,
          marginalRate: marginalBracket ? marginalBracket.rate : 0,
        };
      }

      function estimateChildTaxCredit(
        year,
        agi,
        numChildren,
        taxBeforeCredits
      ) {
        const data = TAX_DATA[year];
        const perChild = data.childTaxCreditPerChild;
        const baseCredit = Math.max(0, numChildren) * perChild;

        if (baseCredit === 0) return 0;

        const phaseoutStart = data.ctcPhaseoutStartHOH;
        let credit = baseCredit;

        if (agi > phaseoutStart) {
          const excess = agi - phaseoutStart;
          const reduction = Math.floor(excess / 1000) * 50; // $50 per $1,000 over threshold
          credit = Math.max(0, baseCredit - reduction);
        }

        // Non-refundable portion only
        return Math.min(credit, taxBeforeCredits);
      }

      function calculate() {
        const year = parseInt(document.getElementById("tax-year").value, 10);
        const income = Math.max(
          0,
          Number(document.getElementById("income").value) || 0
        );
        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        const itemized = Math.max(
          0,
          Number(document.getElementById("itemized-deduction").value) || 0
        );
        const numChildren = Math.max(
          0,
          Math.floor(Number(document.getElementById("num-children").value) || 0)
        );
        const otherCreditsInput = Math.max(
          0,
          Number(document.getElementById("other-credits").value) || 0
        );

        const data = TAX_DATA[year];
        const stdDeduction = data.standardDeduction.HOH;

        const agi = income;
        const deductionsUsed =
          deductionType === "itemized" ? itemized : stdDeduction;
        const taxableIncome = Math.max(0, agi - deductionsUsed);

        const {
          totalTax: taxBeforeCredits,
          breakdown,
          marginalRate,
        } = computeTaxFromBrackets(taxableIncome, data.brackets.HOH);

        const ctc = estimateChildTaxCredit(
          year,
          agi,
          numChildren,
          taxBeforeCredits
        );
        const remainingTaxAfterCTC = Math.max(0, taxBeforeCredits - ctc);
        const otherCredits = Math.min(otherCreditsInput, remainingTaxAfterCTC);
        const finalTax = Math.max(0, remainingTaxAfterCTC - otherCredits);

        const effectiveRate = agi > 0 ? finalTax / agi : 0;

        // Update outputs
        document.getElementById("agi-output").textContent = formatCurrency(agi);
        document.getElementById("deduction-output").textContent =
          formatCurrency(deductionsUsed);
        document.getElementById("taxable-output").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("tax-before-credits-output").textContent =
          formatCurrency(Math.round(taxBeforeCredits));
        document.getElementById("ctc-output").textContent =
          "-" + formatCurrency(Math.round(ctc));
        document.getElementById("other-credits-output").textContent =
          "-" + formatCurrency(Math.round(otherCredits));
        document.getElementById("tax-owed-output").textContent = formatCurrency(
          Math.round(finalTax)
        );
        document.getElementById("effective-rate-output").textContent =
          formatPercent(effectiveRate);
        document.getElementById("marginal-bracket-output").textContent =
          marginalRate ? Math.round(marginalRate * 100) + "%" : "–";

        // Bracket breakdown table
        const tbody = document.getElementById("bracket-breakdown-body");
        tbody.innerHTML = "";
        if (breakdown.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="4" class="px-3 py-2 text-center text-gray-500 text-sm">No taxable income.</td>';
          tbody.appendChild(tr);
        } else {
          breakdown.forEach((row) => {
            const tr = document.createElement("tr");
            tr.className = "tax-bracket-row";
            const fromLabel = formatCurrency(row.from);
            const toLabel =
              row.to === Infinity ? "and up" : formatCurrency(row.to);
            tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap">${fromLabel} – ${toLabel}</td>
          <td class="px-3 py-2 text-right">${formatCurrency(
            Math.round(row.amount)
          )}</td>
          <td class="px-3 py-2 text-right">${Math.round(row.rate * 100)}%</td>
          <td class="px-3 py-2 text-right">${formatCurrency(
            Math.round(row.tax)
          )}</td>
        `;
            tbody.appendChild(tr);
          });
        }

        // Status comparison (using same taxable income)
        updateStatusComparison(year, taxableIncome);
      }

      function updateStatusComparison(year, taxableIncome) {
        const data = TAX_DATA[year];
        const statuses = [
          { code: "HOH", label: "Head of Household" },
          { code: "SINGLE", label: "Single" },
          { code: "MFJ", label: "Married Filing Jointly" },
        ];
        const tbody = document.getElementById("status-comparison-body");
        tbody.innerHTML = "";

        statuses.forEach((status) => {
          const brackets = data.brackets[status.code];
          if (!brackets) return;
          const { totalTax } = computeTaxFromBrackets(taxableIncome, brackets);
          const effective = taxableIncome > 0 ? totalTax / taxableIncome : 0;

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">${status.label}</td>
        <td class="px-3 py-2 text-right">${formatCurrency(
          Math.round(taxableIncome)
        )}</td>
        <td class="px-3 py-2 text-right">${formatCurrency(
          Math.round(totalTax)
        )}</td>
        <td class="px-3 py-2 text-right">${formatPercent(effective)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function handleDeductionToggle() {
        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        const itemizedWrapper = document.getElementById("itemized-wrapper");
        if (deductionType === "itemized") {
          itemizedWrapper.classList.remove("hidden");
        } else {
          itemizedWrapper.classList.add("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("calculate-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            calculate();
          });

        document
          .querySelectorAll('input[name="deduction-type"]')
          .forEach((radio) => {
            radio.addEventListener("change", () => {
              handleDeductionToggle();
              calculate();
            });
          });

        [
          "tax-year",
          "income",
          "itemized-deduction",
          "num-children",
          "other-credits",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              if (
                id === "income" ||
                id === "itemized-deduction" ||
                id === "other-credits"
              ) {
                // Debounce numeric inputs slightly
                clearTimeout(el._debounce);
                el._debounce = setTimeout(calculate, 200);
              } else {
                calculate();
              }
            });
          }
        });

        handleDeductionToggle();
        calculate();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
