    <script>
      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function clampAngle(a) {
        if (isNaN(a)) return NaN;
        // keep as entered but limit to 0-360 for safety
        if (a < 0) a = 0;
        if (a > 360) a = 360;
        return a;
      }

      const modeEl = document.getElementById("mode");
      const arcInputWrap = document.getElementById("input-arc");
      const centralInputWrap = document.getElementById("input-central");
      const inscribedInputWrap = document.getElementById("input-inscribed");
      const arcEl = document.getElementById("arcAngle");
      const centralEl = document.getElementById("centralAngle");
      const inscribedEl = document.getElementById("inscribedAngleInput");
      const radiusEl = document.getElementById("radius");
      const errorMsg = document.getElementById("errorMsg");

      const resInscribed = document.getElementById("resInscribed");
      const resCentral = document.getElementById("resCentral");
      const resArc = document.getElementById("resArc");
      const stepExplanation = document.getElementById("stepExplanation");

      const canvas = document.getElementById("circleCanvas");
      const ctx = canvas.getContext("2d");

      function updateVisibility() {
        const mode = modeEl.value;
        arcInputWrap.classList.add("hidden");
        centralInputWrap.classList.add("hidden");
        inscribedInputWrap.classList.add("hidden");

        if (mode === "inscribed-from-arc") {
          arcInputWrap.classList.remove("hidden");
        } else if (mode === "inscribed-from-central") {
          centralInputWrap.classList.remove("hidden");
        } else if (mode === "arc-from-inscribed") {
          inscribedInputWrap.classList.remove("hidden");
        } else if (mode === "central-from-inscribed") {
          inscribedInputWrap.classList.remove("hidden");
        }
      }

      function compute() {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        const mode = modeEl.value;
        let arc = NaN,
          central = NaN,
          inscribed = NaN;

        if (mode === "inscribed-from-arc") {
          arc = clampAngle(parseFloat(arcEl.value));
          if (isNaN(arc)) return showError("Please enter a valid arc measure.");
          inscribed = arc / 2;
          central = arc; // central angle equals arc measure
        } else if (mode === "inscribed-from-central") {
          central = clampAngle(parseFloat(centralEl.value));
          if (isNaN(central))
            return showError("Please enter a valid central angle.");
          inscribed = central / 2;
          arc = central;
        } else if (mode === "arc-from-inscribed") {
          inscribed = clampAngle(parseFloat(inscribedEl.value));
          if (isNaN(inscribed))
            return showError("Please enter a valid inscribed angle.");
          arc = 2 * inscribed;
          central = arc;
        } else if (mode === "central-from-inscribed") {
          inscribed = clampAngle(parseFloat(inscribedEl.value));
          if (isNaN(inscribed))
            return showError("Please enter a valid inscribed angle.");
          central = 2 * inscribed;
          arc = central;
        }

        // Clamp derived values to 0-360 for display
        arc = clampAngle(arc);
        central = clampAngle(central);
        inscribed = clampAngle(inscribed);

        resInscribed.innerHTML = isNaN(inscribed)
          ? "–"
          : inscribed.toFixed(2) + "&deg;";
        resCentral.innerHTML = isNaN(central)
          ? "–"
          : central.toFixed(2) + "&deg;";
        resArc.innerHTML = isNaN(arc) ? "–" : arc.toFixed(2) + "&deg;";

        updateSteps(mode, arc, central, inscribed);
        drawDiagram(arc, inscribed);
      }

      function showError(msg) {
        errorMsg.textContent = msg;
        errorMsg.classList.remove("hidden");
      }

      function updateSteps(mode, arc, central, inscribed) {
        let html = "";
        if (mode === "inscribed-from-arc") {
          html += `<p>1. You entered an intercepted arc of <strong>${arc.toFixed(
            2
          )}°</strong>.</p>`;
          html += `<p>2. The inscribed angle is half the arc: θ<sub>inscribed</sub> = ½ × ${arc.toFixed(
            2
          )}°.</p>`;
          html += `<p>3. So θ<sub>inscribed</sub> = <strong>${inscribed.toFixed(
            2
          )}°</strong>.</p>`;
        } else if (mode === "inscribed-from-central") {
          html += `<p>1. You entered a central angle of <strong>${central.toFixed(
            2
          )}°</strong>.</p>`;
          html += `<p>2. The inscribed angle intercepting the same arc is half the central angle.</p>`;
          html += `<p>3. θ<sub>inscribed</sub> = ½ × ${central.toFixed(
            2
          )}° = <strong>${inscribed.toFixed(2)}°</strong>.</p>`;
        } else if (mode === "arc-from-inscribed") {
          html += `<p>1. You entered an inscribed angle of <strong>${inscribed.toFixed(
            2
          )}°</strong>.</p>`;
          html += `<p>2. The intercepted arc is twice the inscribed angle: arc = 2 × θ<sub>inscribed</sub>.</p>`;
          html += `<p>3. arc = 2 × ${inscribed.toFixed(
            2
          )}° = <strong>${arc.toFixed(2)}°</strong>.</p>`;
        } else if (mode === "central-from-inscribed") {
          html += `<p>1. You entered an inscribed angle of <strong>${inscribed.toFixed(
            2
          )}°</strong>.</p>`;
          html += `<p>2. The central angle intercepting the same arc is twice the inscribed angle.</p>`;
          html += `<p>3. θ<sub>central</sub> = 2 × ${inscribed.toFixed(
            2
          )}° = <strong>${central.toFixed(2)}°</strong>.</p>`;
        }
        stepExplanation.innerHTML = html;
      }

      function drawDiagram(arcDeg, inscribedDeg) {
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        const R = Math.max(
          40,
          Math.min(160, parseFloat(radiusEl.value) || 120)
        );
        const cx = w / 2;
        const cy = h / 2;

        // Draw circle
        ctx.beginPath();
        ctx.arc(cx, cy, R, 0, 2 * Math.PI);
        ctx.strokeStyle = "#4B5563";
        ctx.lineWidth = 1.5;
        ctx.stroke();

        // If no valid arc, stop
        if (isNaN(arcDeg) || arcDeg <= 0) return;

        // Define arc from angleStart to angleEnd (in radians)
        const arcRad = (arcDeg * Math.PI) / 180;
        const angleStart = -Math.PI / 2 - arcRad / 2; // symmetric around top
        const angleEnd = angleStart + arcRad;

        // Points B and C on circle
        const B = {
          x: cx + R * Math.cos(angleStart),
          y: cy + R * Math.sin(angleStart),
        };
        const C = {
          x: cx + R * Math.cos(angleEnd),
          y: cy + R * Math.sin(angleEnd),
        };

        // Place A on circle roughly opposite the arc midpoint
        const midAngle = (angleStart + angleEnd) / 2;
        const Aangle = midAngle + Math.PI; // opposite side
        const A = {
          x: cx + R * Math.cos(Aangle),
          y: cy + R * Math.sin(Aangle),
        };

        // Draw arc BC
        ctx.beginPath();
        ctx.arc(cx, cy, R, angleStart, angleEnd);
        ctx.strokeStyle = "#EF4444";
        ctx.lineWidth = 2;
        ctx.stroke();

        // Draw chords AB and AC
        ctx.beginPath();
        ctx.moveTo(A.x, A.y);
        ctx.lineTo(B.x, B.y);
        ctx.moveTo(A.x, A.y);
        ctx.lineTo(C.x, C.y);
        ctx.strokeStyle = "#2563EB";
        ctx.lineWidth = 2;
        ctx.stroke();

        // Draw central angle lines
        ctx.beginPath();
        ctx.moveTo(cx, cy);
        ctx.lineTo(B.x, B.y);
        ctx.moveTo(cx, cy);
        ctx.lineTo(C.x, C.y);
        ctx.strokeStyle = "#10B981";
        ctx.lineWidth = 1.5;
        ctx.stroke();

        // Draw points
        function drawPoint(p, label) {
          ctx.beginPath();
          ctx.arc(p.x, p.y, 4, 0, 2 * Math.PI);
          ctx.fillStyle = "#111827";
          ctx.fill();
          ctx.font = "12px Inter, system-ui, sans-serif";
          ctx.fillText(label, p.x + 6, p.y - 6);
        }
        drawPoint({ x: cx, y: cy }, "O");
        drawPoint(A, "A");
        drawPoint(B, "B");
        drawPoint(C, "C");

        // Draw small inscribed angle arc at A
        const vAB = Math.atan2(B.y - A.y, B.x - A.x);
        const vAC = Math.atan2(C.y - A.y, C.x - A.x);
        ctx.beginPath();
        ctx.arc(A.x, A.y, 20, vAB, vAC, false);
        ctx.strokeStyle = "#F59E0B";
        ctx.lineWidth = 1.5;
        ctx.stroke();

        // Label inscribed angle value
        if (!isNaN(inscribedDeg)) {
          ctx.font = "12px Inter, system-ui, sans-serif";
          ctx.fillStyle = "#F59E0B";
          const labelAngle = (vAB + vAC) / 2;
          const lx = A.x + 26 * Math.cos(labelAngle);
          const ly = A.y + 26 * Math.sin(labelAngle);
          ctx.fillText(inscribedDeg.toFixed(1) + "°", lx, ly);
        }
      }

      modeEl.addEventListener("change", () => {
        updateVisibility();
        compute();
      });

      document.getElementById("calcBtn").addEventListener("click", compute);
      [arcEl, centralEl, inscribedEl, radiusEl].forEach((el) => {
        el.addEventListener("input", () => {
          // live update but debounce errors
          compute();
        });
      });

      // Initial setup
      updateVisibility();
      compute();
    </script>

