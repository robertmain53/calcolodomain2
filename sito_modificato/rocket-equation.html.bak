    <script>
      // Utility helpers
      function toNumber(v) {
        if (v === "" || v === null || v === undefined) return null;
        const n = Number(v);
        return isNaN(n) ? null : n;
      }

      function formatNumber(n, decimals = 3) {
        if (!isFinite(n)) return "—";
        if (Math.abs(n) >= 1e4 || (Math.abs(n) < 1e-2 && n !== 0)) {
          return n.toExponential(3);
        }
        return n.toFixed(decimals);
      }

      // g0 selector
      const g0Select = document.getElementById("g0");
      const g0Custom = document.getElementById("g0-custom");

      g0Select.addEventListener("change", () => {
        if (g0Select.value === "custom") {
          g0Custom.classList.remove("hidden");
        } else {
          g0Custom.classList.add("hidden");
        }
      });

      function getG0() {
        if (g0Select.value === "custom") {
          const g = toNumber(g0Custom.value);
          return g && g > 0 ? g : 9.80665;
        }
        return Number(g0Select.value);
      }

      // Presets
      const presetButtons = document.querySelectorAll("[data-preset]");
      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          const ispEl = document.getElementById("isp");
          const dvEl = document.getElementById("deltaV");
          const dvUnit = document.getElementById("deltaV-unit");

          if (preset === "leo") {
            dvEl.value = 9.4;
            dvUnit.value = "kmps";
            ispEl.value = 320;
          } else if (preset === "gto") {
            dvEl.value = 12;
            dvUnit.value = "kmps";
            ispEl.value = 340;
          } else if (preset === "escape") {
            dvEl.value = 15;
            dvUnit.value = "kmps";
            ispEl.value = 450;
          }
        });
      });

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";

        const solveFor = document.getElementById("solve-for").value;
        const g0 = getG0();

        // Masses
        let m0 = toNumber(document.getElementById("m0").value);
        let mf = toNumber(document.getElementById("mf").value);
        let mp = toNumber(document.getElementById("mp").value);

        const m0Unit = document.getElementById("m0-unit").value;
        const mfUnit = document.getElementById("mf-unit").value;
        const mpUnit = document.getElementById("mp-unit").value;

        if (m0 != null && m0Unit === "tonne") m0 *= 1000;
        if (mf != null && mfUnit === "tonne") mf *= 1000;
        if (mp != null && mpUnit === "tonne") mp *= 1000;

        // Infer missing mass if possible
        if (m0 != null && mf != null && mp == null) {
          mp = m0 - mf;
        } else if (m0 != null && mp != null && mf == null) {
          mf = m0 - mp;
        } else if (mf != null && mp != null && m0 == null) {
          m0 = mf + mp;
        }

        // Performance
        let dv = toNumber(document.getElementById("deltaV").value);
        const dvUnit = document.getElementById("deltaV-unit").value;
        if (dv != null && dvUnit === "kmps") dv *= 1000;

        let ve = toNumber(document.getElementById("ve").value);
        const veUnit = document.getElementById("ve-unit").value;
        if (ve != null && veUnit === "kmps") ve *= 1000;

        let isp = toNumber(document.getElementById("isp").value);

        // Thrust / burn
        let thrust = toNumber(document.getElementById("thrust").value);
        const thrustUnit = document.getElementById("thrust-unit").value;
        if (thrust != null && thrustUnit === "kN") thrust *= 1000;

        let mdot = toNumber(document.getElementById("mdot").value);
        const mdotUnit = document.getElementById("mdot-unit").value;
        if (mdot != null && mdotUnit === "tonneps") mdot *= 1000;

        let burn = toNumber(document.getElementById("burn-time").value);

        // Basic validation
        if (
          (m0 != null && m0 <= 0) ||
          (mf != null && mf <= 0) ||
          (mp != null && mp < 0)
        ) {
          errorEl.textContent = "Masses must be positive and consistent.";
          return;
        }

        // Derive ve / isp if possible
        if (ve == null && isp != null && isp > 0) {
          ve = isp * g0;
        } else if (isp == null && ve != null && ve > 0) {
          isp = ve / g0;
        }

        // Solve rocket equation depending on mode
        let massRatio = null;

        try {
          if (solveFor === "deltaV" || solveFor === "auto") {
            if (
              dv == null &&
              ve != null &&
              m0 != null &&
              mf != null &&
              m0 > mf
            ) {
              massRatio = m0 / mf;
              dv = ve * Math.log(massRatio);
            }
          }

          if (
            (solveFor === "massRatio" || solveFor === "auto") &&
            massRatio == null
          ) {
            if (m0 != null && mf != null && m0 > mf) {
              massRatio = m0 / mf;
            } else if (dv != null && ve != null && ve > 0) {
              massRatio = Math.exp(dv / ve);
            }
          }

          if (
            (solveFor === "masses" || solveFor === "auto") &&
            massRatio == null &&
            dv != null &&
            ve != null &&
            ve > 0
          ) {
            massRatio = Math.exp(dv / ve);
          }

          // If we have mass ratio and one mass, infer others
          if (massRatio != null) {
            if (m0 != null && mf == null) {
              mf = m0 / massRatio;
              mp = m0 - mf;
            } else if (mf != null && m0 == null) {
              m0 = mf * massRatio;
              mp = m0 - mf;
            } else if (m0 != null && mf != null && mp == null) {
              mp = m0 - mf;
            }
          }

          // If thrust and ve known, infer mdot
          if (thrust != null && ve != null && ve > 0 && mdot == null) {
            mdot = thrust / ve;
          }

          // If propellant mass and mdot known, infer burn
          if (
            mp != null &&
            mp > 0 &&
            mdot != null &&
            mdot > 0 &&
            burn == null
          ) {
            burn = mp / mdot;
          }

          // If burn and mp known but mdot unknown
          if (mp != null && burn != null && burn > 0 && mdot == null) {
            mdot = mp / burn;
          }

          // If thrust unknown but mdot and ve known
          if (thrust == null && mdot != null && ve != null) {
            thrust = mdot * ve;
          }
        } catch (e) {
          errorEl.textContent =
            "Unable to solve with the provided inputs. Please check values.";
          return;
        }

        // Final sanity checks
        if (dv == null && ve == null && (m0 == null || mf == null)) {
          errorEl.textContent =
            "Please provide enough information: typically masses plus Isp/ve, or delta-v plus Isp/ve.";
          return;
        }

        // Compute mass ratio and propellant fraction if still missing
        if (m0 != null && mf != null && m0 > mf) {
          massRatio = m0 / mf;
        }
        const propFrac = m0 != null && mp != null && m0 > 0 ? mp / m0 : null;

        // Populate results
        const resSection = document.getElementById("results-section");
        resSection.classList.remove("hidden");

        document.getElementById("res-dv").textContent =
          dv != null
            ? `${formatNumber(dv)} m/s (${formatNumber(dv / 1000, 3)} km/s)`
            : "—";
        document.getElementById("res-mr").textContent =
          massRatio != null ? formatNumber(massRatio, 4) : "—";
        document.getElementById("res-pf").textContent =
          propFrac != null ? `${formatNumber(propFrac * 100, 2)} %` : "—";
        document.getElementById("res-m0").textContent =
          m0 != null
            ? `${formatNumber(m0)} kg (${formatNumber(m0 / 1000, 3)} t)`
            : "—";
        document.getElementById("res-mf").textContent =
          mf != null
            ? `${formatNumber(mf)} kg (${formatNumber(mf / 1000, 3)} t)`
            : "—";
        document.getElementById("res-mp").textContent =
          mp != null
            ? `${formatNumber(mp)} kg (${formatNumber(mp / 1000, 3)} t)`
            : "—";

        document.getElementById("res-ve").textContent =
          ve != null
            ? `${formatNumber(ve)} m/s (${formatNumber(ve / 1000, 3)} km/s)`
            : "—";
        document.getElementById("res-isp").textContent =
          isp != null ? `${formatNumber(isp, 3)} s` : "—";
        document.getElementById("res-thrust").textContent =
          thrust != null
            ? `${formatNumber(thrust)} N (${formatNumber(thrust / 1000, 3)} kN)`
            : "—";
        document.getElementById("res-mdot").textContent =
          mdot != null
            ? `${formatNumber(mdot)} kg/s (${formatNumber(mdot / 1000, 3)} t/s)`
            : "—";
        document.getElementById("res-burn").textContent =
          burn != null
            ? `${formatNumber(burn)} s (${formatNumber(burn / 60, 3)} min)`
            : "—";
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("rocket-form").reset();
        document.getElementById("error-msg").textContent = "";
        document.getElementById("results-section").classList.add("hidden");
        g0Custom.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
