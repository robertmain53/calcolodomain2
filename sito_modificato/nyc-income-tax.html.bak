    <script>
      // Simplified NY State and NYC tax model (illustrative, not official)
      const NY_STANDARD_DEDUCTION = {
        single: 8000,
        mfj: 16050,
        mfs: 8000,
        hoh: 11200,
      };

      // NY State brackets: [upperLimit, rate]
      const NY_BRACKETS = [
        [8500, 0.04],
        [11700, 0.045],
        [13900, 0.0525],
        [80650, 0.059],
        [215400, 0.0621],
        [1077550, 0.0649],
        [Infinity, 0.0685],
      ];

      // NYC brackets (single-like, we reuse for all statuses for simplicity)
      const NYC_BRACKETS = [
        [12000, 0.03078],
        [25000, 0.03762],
        [50000, 0.03819],
        [Infinity, 0.03876],
      ];

      function applyBrackets(income, brackets) {
        let tax = 0;
        let lastLimit = 0;
        let marginalRate = 0;

        for (const [limit, rate] of brackets) {
          if (income <= lastLimit) break;
          const taxableAtThisRate = Math.min(income, limit) - lastLimit;
          tax += taxableAtThisRate * rate;
          marginalRate = rate;
          lastLimit = limit;
          if (income <= limit) break;
        }

        return { tax, marginalRate };
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function getSelectedFilingStatus() {
        return document.getElementById("filing-status").value;
      }

      function getStandardDeduction(status) {
        return NY_STANDARD_DEDUCTION[status] || NY_STANDARD_DEDUCTION.single;
      }

      function calculateTaxes() {
        const status = getSelectedFilingStatus();
        const residentType = document.getElementById("nyc-resident").value;
        const residentPercent =
          parseFloat(
            document.getElementById("nyc-resident-percent").value || "100"
          ) / 100;

        const income = parseFloat(
          document.getElementById("annual-income").value || "0"
        );
        const otherIncome = parseFloat(
          document.getElementById("other-income").value || "0"
        );
        const credits = parseFloat(
          document.getElementById("credits").value || "0"
        );

        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        const itemized = parseFloat(
          document.getElementById("itemized-deductions").value || "0"
        );

        const totalIncome = Math.max(0, income + otherIncome);
        const deduction =
          deductionType === "standard"
            ? getStandardDeduction(status)
            : itemized;
        const taxableIncome = Math.max(0, totalIncome - deduction);

        const nyResult = applyBrackets(taxableIncome, NY_BRACKETS);
        let nyTax = nyResult.tax;

        let nycTax = 0;
        let nycMarginalRate = 0;
        if (residentType !== "no") {
          const nycResult = applyBrackets(taxableIncome, NYC_BRACKETS);
          nycTax =
            nycResult.tax * (residentType === "partial" ? residentPercent : 1);
          nycMarginalRate = nycResult.marginalRate;
        }

        let totalTax = Math.max(0, nyTax + nycTax - credits);
        const effectiveRate = totalIncome > 0 ? totalTax / totalIncome : 0;
        const takeHome = totalIncome - totalTax;

        // Update summary
        document.getElementById("summary-taxable-income").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("summary-ny-tax").textContent =
          formatCurrency(nyTax);
        document.getElementById("summary-nyc-tax").textContent =
          formatCurrency(nycTax);
        document.getElementById("summary-total-tax").textContent =
          formatCurrency(totalTax);
        document.getElementById("summary-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("summary-takehome").textContent =
          formatCurrency(takeHome);

        // Update marginal rates
        document.getElementById("ny-marginal-rate").textContent =
          taxableIncome > 0 ? formatPercent(nyResult.marginalRate) : "–";
        document.getElementById("nyc-marginal-rate").textContent =
          residentType !== "no" && taxableIncome > 0
            ? formatPercent(nycMarginalRate)
            : "–";
        const combinedMarginal =
          taxableIncome > 0
            ? nyResult.marginalRate +
              (residentType !== "no" ? nycMarginalRate : 0)
            : 0;
        document.getElementById("combined-marginal-rate").textContent =
          taxableIncome > 0 ? formatPercent(combinedMarginal) : "–";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const nycResidentSelect = document.getElementById("nyc-resident");
        const nycResidentPercentWrapper = document.getElementById(
          "nyc-resident-percent-wrapper"
        );
        const nycResidentPercent = document.getElementById(
          "nyc-resident-percent"
        );
        const nycResidentPercentOutput = document.getElementById(
          "nyc-resident-percent-output"
        );

        nycResidentSelect.addEventListener("change", () => {
          if (nycResidentSelect.value === "partial") {
            nycResidentPercentWrapper.classList.remove("hidden");
          } else {
            nycResidentPercentWrapper.classList.add("hidden");
          }
          calculateTaxes();
        });

        nycResidentPercent.addEventListener("input", () => {
          nycResidentPercentOutput.textContent = nycResidentPercent.value + "%";
          calculateTaxes();
        });

        const deductionRadios = document.querySelectorAll(
          'input[name="deduction-type"]'
        );
        const itemizedWrapper = document.getElementById(
          "itemized-deductions-wrapper"
        );
        deductionRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.value === "itemized" && radio.checked) {
              itemizedWrapper.classList.remove("hidden");
            } else if (radio.value === "standard" && radio.checked) {
              itemizedWrapper.classList.add("hidden");
            }
            calculateTaxes();
          });
        });

        [
          "filing-status",
          "annual-income",
          "other-income",
          "itemized-deductions",
          "credits",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", () => {
            if (el.type === "number") {
              const v = parseFloat(el.value);
              if (isNaN(v) || v < 0) el.value = "0";
            }
            calculateTaxes();
          });
          if (el.tagName === "SELECT") {
            el.addEventListener("change", calculateTaxes);
          }
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            calculateTaxes();
          });

        // Initial calculation
        calculateTaxes();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
