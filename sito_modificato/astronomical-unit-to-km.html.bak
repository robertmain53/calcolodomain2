    <script>
      (function () {
        const AU_TO_KM = 149597870.7; // exact IAU definition

        const modeAuToKmBtn = document.getElementById("mode-au-to-km");
        const modeKmToAuBtn = document.getElementById("mode-km-to-au");
        const inputValueEl = document.getElementById("input-value");
        const outputValueEl = document.getElementById("output-value");
        const inputLabelEl = document.getElementById("input-label");
        const outputLabelEl = document.getElementById("output-label");
        const precisionRange = document.getElementById("precision");
        const precisionDisplay = document.getElementById("precision-display");
        const copyBtn = document.getElementById("copy-result");
        const swapBtn = document.getElementById("swap-direction");
        const presetButtons = document.querySelectorAll(".preset-btn");

        let mode = "AU_TO_KM";

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          if (value === 0) return "0";
          const abs = Math.abs(value);
          if (abs >= 1e9 || abs < 1e-3) {
            return value.toExponential(decimals);
          }
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function convert() {
          const raw = inputValueEl.value.trim();
          if (raw === "") {
            outputValueEl.value = "";
            return;
          }
          const x = Number(raw);
          if (!isFinite(x)) {
            outputValueEl.value = "Invalid input";
            return;
          }
          const decimals = parseInt(precisionRange.value, 10);
          let result;
          if (mode === "AU_TO_KM") {
            result = x * AU_TO_KM;
          } else {
            result = x / AU_TO_KM;
          }
          outputValueEl.value = formatNumber(result, decimals);
        }

        function setMode(newMode) {
          mode = newMode;
          const auPressed = newMode === "AU_TO_KM";
          modeAuToKmBtn.setAttribute(
            "aria-pressed",
            auPressed ? "true" : "false"
          );
          modeKmToAuBtn.setAttribute(
            "aria-pressed",
            auPressed ? "false" : "true"
          );

          if (auPressed) {
            inputLabelEl.textContent = "Astronomical units (AU)";
            outputLabelEl.textContent = "Kilometers (km)";
            inputValueEl.placeholder = "e.g. 1";
          } else {
            inputLabelEl.textContent = "Kilometers (km)";
            outputLabelEl.textContent = "Astronomical units (AU)";
            inputValueEl.placeholder = "e.g. 149597870.7";
          }
          convert();
        }

        modeAuToKmBtn.addEventListener("click", () => setMode("AU_TO_KM"));
        modeKmToAuBtn.addEventListener("click", () => setMode("KM_TO_AU"));

        precisionRange.addEventListener("input", () => {
          precisionDisplay.textContent = precisionRange.value;
          convert();
        });

        inputValueEl.addEventListener("input", convert);

        swapBtn.addEventListener("click", () => {
          const currentInput = inputValueEl.value;
          const currentOutput = outputValueEl.value.replace(/,/g, "");
          // swap mode
          setMode(mode === "AU_TO_KM" ? "KM_TO_AU" : "AU_TO_KM");
          // use previous output as new input if valid
          if (currentOutput && isFinite(Number(currentOutput))) {
            inputValueEl.value = currentOutput;
          } else {
            inputValueEl.value = currentInput;
          }
          convert();
        });

        copyBtn.addEventListener("click", () => {
          if (!outputValueEl.value) return;
          navigator.clipboard
            .writeText(outputValueEl.value)
            .then(() => {
              copyBtn.textContent = "Copied!";
              setTimeout(() => {
                copyBtn.textContent = "Copy result";
              }, 1500);
            })
            .catch(() => {});
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const auValue = parseFloat(btn.getAttribute("data-au"));
            if (mode !== "AU_TO_KM") {
              setMode("AU_TO_KM");
            }
            inputValueEl.value = auValue;
            convert();
          });
        });

        // initial calculation
        convert();
      })();
    </script>

