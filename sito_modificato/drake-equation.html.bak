            modePointBtn.classList.add(
              "bg-blue-500",
              "text-white",
              "border-blue-500"
            );
            modePointBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            modeRangeBtn.classList.remove(
              "bg-blue-500",
              "text-white",
              "border-blue-500"
            );
            modeRangeBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            rangeWrappers.forEach((w) => w.classList.add("hidden"));
          } else {
            modeRangeBtn.classList.add(
              "bg-blue-500",
              "text-white",
              "border-blue-500"
            );
            modeRangeBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            modePointBtn.classList.remove(
              "bg-blue-500",
              "text-white",
              "border-blue-500"
            );
            modePointBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            rangeWrappers.forEach((w) => w.classList.remove("hidden"));
          }
          updateResultsPlaceholder();
        }

        function updateResultsPlaceholder() {
          if (mode === "point") {
            NRangeOutput.textContent = "Min – Max";
            NRangeInterp.textContent =
              "Switch to “Uncertainty ranges” mode to see pessimistic vs. optimistic bounds.";
          } else {
            NRangeInterp.textContent =
              "Enter min and max for each factor, then click “Calculate N” to see the range.";
          }
        }

        function applyPreset(name) {
          const p = presets[name];
          if (!p) return;
          factorIds.forEach((id) => {
            const input = document.getElementById(id);
            if (input && typeof p[id] !== "undefined") {
              input.value = p[id];
            }
            const minInput = document.getElementById(id + "-min");
            const maxInput = document.getElementById(id + "-max");
            const rangeInput = document.getElementById(id + "-range");
            if (p.ranges && p.ranges[id]) {
              const [minVal, maxVal] = p.ranges[id];
              if (minInput) minInput.value = minVal;
              if (maxInput) maxInput.value = maxVal;
              if (rangeInput) {
                const mid = (minVal + maxVal) / 2;
                rangeInput.value = mid;
              }
            }
          });
          updateResultsPlaceholder();
          NOutput.textContent = "–";
          NInterp.textContent =
            "Preset applied. Click “Calculate N” to see the updated estimate.";
          NRangeOutput.textContent =
            mode === "range" ? "Min – Max" : "Min – Max";
        }

        function sanitizeNumber(value, fallback) {
          const n = parseFloat(value);
          if (isNaN(n) || !isFinite(n)) return fallback;
          return n;
        }

        function formatN(n) {
          if (!isFinite(n)) return "∞";
          if (n === 0) return "0";
          if (n < 0.001) return n.toExponential(2);
          if (n < 1) return n.toPrecision(2);
          if (n < 10000)
            return n.toLocaleString(undefined, { maximumFractionDigits: 2 });
          return n.toExponential(2);
        }

        function calculatePoint() {
          let product = 1;
          const values = {};
          for (const id of factorIds) {
            const el = document.getElementById(id);
            const val = sanitizeNumber(el.value, 0);
            values[id] = val;
            product *= val;
          }
          const N = product;
          NOutput.textContent = formatN(N);
          let interp;
          if (N < 0.1) {
            interp =
              "Your assumptions imply it is very unlikely that any other communicative civilization exists in the Milky Way right now.";
          } else if (N < 1) {
            interp =
              "On average there is less than one communicative civilization at a time, so we may well be alone at the moment.";
          } else if (N < 1000) {
            interp =
              "Your estimate suggests there are likely " +
              formatN(N) +
              " communicative civilizations in the Milky Way.";
          } else {
            interp =
              "Your assumptions are very optimistic, yielding " +
              formatN(N) +
              " civilizations or more.";
          }
          NInterp.textContent = interp;
          updateBreakdown(values);
          return { N, values };
        }

        function calculateRange() {
          const minVals = {};
          const maxVals = {};
          factorIds.forEach((id) => {
            const minEl = document.getElementById(id + "-min");
            const maxEl = document.getElementById(id + "-max");
            minVals[id] = sanitizeNumber(minEl ? minEl.value : 0, 0);
            maxVals[id] = sanitizeNumber(maxEl ? maxEl.value : 0, 0);
          });
          let Nmin = 1;
          let Nmax = 1;
          factorIds.forEach((id) => {
            Nmin *= Math.max(0, minVals[id]);
            Nmax *= Math.max(0, maxVals[id]);
          });
          NRangeOutput.textContent = formatN(Nmin) + " – " + formatN(Nmax);
          let interp;
          if (Nmax < 1) {
            interp =
              "Even under optimistic assumptions within your ranges, fewer than one communicative civilization is expected.";
          } else if (Nmin < 1 && Nmax >= 1) {
            interp =
              "Your ranges span both “we are likely alone” and “many civilizations exist”, highlighting our large uncertainty.";
          } else {
            interp =
              "Across your ranges, the galaxy is likely to host multiple communicative civilizations.";
          }
          NRangeInterp.textContent = interp;
          // For breakdown, use geometric mean of min and max as a representative value
          const geoValues = {};
          factorIds.forEach((id) => {
            const a = Math.max(0, minVals[id]);
            const b = Math.max(0, maxVals[id]);
            geoValues[id] = a > 0 && b > 0 ? Math.sqrt(a * b) : 0;
          });
          updateBreakdown(geoValues);
          return { Nmin, Nmax, minVals, maxVals };
        }

        function updateBreakdown(values) {
          factorBreakdown.innerHTML = "";
          const labels = {
            Rstar: "R* (star formation)",
            fp: "f\u209a (stars with planets)",
            ne: "n\u2091 (habitable planets)",
            fl: "f\u2097 (life develops)",
            fi: "f\u1d62 (intelligent life)",
            fc: "f\u1d9c (communicative)",
            L: "L (lifetime)",
          };
          factorIds.forEach((id) => {
            const val = values[id];
            const div = document.createElement("div");
            div.className = "flex justify-between";
            const label = labels[id] || id;
            div.innerHTML =
              "<span>" +
              label +
              '</span><span class="range-label font-mono">' +
              formatN(val) +
              "</span>";
            factorBreakdown.appendChild(div);
          });
        }

        // Sync range sliders with min/max inputs
        factorIds.forEach((id) => {
          const rangeEl = document.getElementById(id + "-range");
          const minEl = document.getElementById(id + "-min");
          const maxEl = document.getElementById(id + "-max");
          if (rangeEl && minEl && maxEl) {
            rangeEl.addEventListener("input", () => {
              const minVal = sanitizeNumber(minEl.value, 0);
              const maxVal = sanitizeNumber(maxEl.value, 0);
              const mid = sanitizeNumber(rangeEl.value, (minVal + maxVal) / 2);
              // Move closer to mid but keep min <= mid <= max
              const span = (maxVal - minVal) / 2;
              if (span > 0) {
                minEl.value = Math.max(0, mid - span);
                maxEl.value = mid + span;
              }
            });
            minEl.addEventListener("change", () => {
              const minVal = sanitizeNumber(minEl.value, 0);
              const maxVal = sanitizeNumber(maxEl.value, 0);
              if (minVal > maxVal) maxEl.value = minVal;
              rangeEl.value = (minVal + maxVal) / 2;
            });
            maxEl.addEventListener("change", () => {
              const minVal = sanitizeNumber(minEl.value, 0);
              const maxVal = sanitizeNumber(maxEl.value, 0);
              if (maxVal < minVal) minEl.value = maxVal;
              rangeEl.value = (minVal + maxVal) / 2;
            });
          }
        });

        // Event listeners
        modePointBtn.addEventListener("click", () => setMode("point"));
        modeRangeBtn.addEventListener("click", () => setMode("range"));

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const presetName = btn.getAttribute("data-preset");
            applyPreset(presetName);
          });
        });

        calculateBtn.addEventListener("click", () => {
          if (mode === "point") {
            calculatePoint();
          } else {
            calculateRange();
          }
        });

        resetBtn.addEventListener("click", () => {
          // Small timeout so browser resets form first
          setTimeout(() => {
            applyPreset("classic");
            setMode("point");
            NOutput.textContent = "–";
            NInterp.textContent =
              "Adjust the sliders and click “Calculate N” to see your estimate.";
            NRangeOutput.textContent = "Min – Max";
            updateResultsPlaceholder();
          }, 0);
        });

        // Initialize defaults
        applyPreset("classic");
        setMode("point");
      })();
    </script>

