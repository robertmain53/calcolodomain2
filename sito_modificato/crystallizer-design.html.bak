    <script>
      // Tab switching
      const tabBatch = document.getElementById("tab-batch");
      const tabMsmpr = document.getElementById("tab-msmpr");
      const panelBatch = document.getElementById("panel-batch");
      const panelMsmpr = document.getElementById("panel-msmpr");

      function activateTab(tab) {
        if (tab === "batch") {
          tabBatch.classList.add("border-blue-600", "text-blue-600");
          tabBatch.classList.remove("border-transparent", "text-gray-600");
          tabMsmpr.classList.add("border-transparent", "text-gray-600");
          tabMsmpr.classList.remove("border-blue-600", "text-blue-600");
          panelBatch.classList.remove("hidden");
          panelMsmpr.classList.add("hidden");
        } else {
          tabMsmpr.classList.add("border-blue-600", "text-blue-600");
          tabMsmpr.classList.remove("border-transparent", "text-gray-600");
          tabBatch.classList.add("border-transparent", "text-gray-600");
          tabBatch.classList.remove("border-blue-600", "text-blue-600");
          panelMsmpr.classList.remove("hidden");
          panelBatch.classList.add("hidden");
        }
      }

      tabBatch.addEventListener("click", () => activateTab("batch"));
      tabMsmpr.addEventListener("click", () => activateTab("msmpr"));

      // Helper: format numbers
      function fmt(value, unit) {
        if (!isFinite(value)) return "–";
        let abs = Math.abs(value);
        let digits = abs === 0 ? 2 : abs < 0.01 || abs > 1e4 ? 3 : 2;
        return value.toExponential
          ? value.toExponential(3) + (unit ? " " + unit : "")
          : value.toFixed(digits) + (unit ? " " + unit : "");
      }

      // Batch calculations
      document
        .getElementById("btn-batch-calc")
        .addEventListener("click", () => {
          const V = parseFloat(document.getElementById("batch-volume").value);
          const rho = parseFloat(
            document.getElementById("batch-density").value
          );
          const C0 = parseFloat(document.getElementById("batch-c0").value);
          const Ceq = parseFloat(document.getElementById("batch-ce").value);
          const M = parseFloat(document.getElementById("batch-ms").value);
          const mSeed =
            parseFloat(document.getElementById("batch-seed").value) || 0;

          const errEl = document.getElementById("batch-error");
          errEl.classList.add("hidden");
          errEl.textContent = "";

          if ([V, rho, C0, Ceq, M].some((v) => !isFinite(v) || v <= 0)) {
            errEl.textContent =
              "Please enter positive values for volume, density, concentrations, and molecular weight.";
            errEl.classList.remove("hidden");
            return;
          }
          if (C0 <= Ceq) {
            errEl.textContent =
              "Initial concentration must be greater than equilibrium solubility to generate crystals.";
            errEl.classList.remove("hidden");
            return;
          }

          const ms = (rho * V) / (1 + C0); // solvent mass
          const m0 = C0 * ms;
          const mdFinal = Ceq * ms;
          const mCryst = Math.max(m0 - mdFinal, 0);
          const yieldMass = mCryst / m0;

          const n0 = m0 / M;
          const nCryst = mCryst / M;
          const seedFrac = mSeed > 0 ? mSeed / (mSeed + mCryst) : 0;

          document.getElementById("batch-S").textContent = fmt(C0 / Ceq, "");
          document.getElementById("batch-mcryst").textContent = fmt(
            mCryst,
            "kg"
          );
          document.getElementById("batch-cf").textContent = fmt(
            mdFinal / ms,
            "kg/kg"
          );
          document.getElementById("batch-yield").textContent =
            (yieldMass * 100).toFixed(1) + " %";

          document.getElementById("batch-n0").textContent = fmt(n0, "kmol");
          document.getElementById("batch-ncryst").textContent = fmt(
            nCryst,
            "kmol"
          );
          document.getElementById("batch-seed-frac").textContent =
            mSeed > 0 ? (seedFrac * 100).toFixed(1) + " %" : "0 %";
        });

      // MSMPR calculations
      document
        .getElementById("btn-msmpr-calc")
        .addEventListener("click", () => {
          const Q = parseFloat(document.getElementById("msmpr-Q").value); // m3/h
          const Cf = parseFloat(document.getElementById("msmpr-Cf").value); // kg/kg
          const Ceq = parseFloat(document.getElementById("msmpr-Ceq").value); // kg/kg
          const rho = parseFloat(document.getElementById("msmpr-rho").value); // kg/m3
          const G = parseFloat(document.getElementById("msmpr-G").value); // m/s
          const B0 = parseFloat(document.getElementById("msmpr-B0").value); // #/m3 s
          const Ltarget_um = parseFloat(
            document.getElementById("msmpr-Ltarget").value
          ); // µm
          const phi = parseFloat(document.getElementById("msmpr-phi").value); // mass fraction

          const errEl = document.getElementById("msmpr-error");
          errEl.classList.add("hidden");
          errEl.textContent = "";

          if (
            [Q, Cf, Ceq, rho, G, B0, Ltarget_um, phi].some(
              (v) => !isFinite(v) || v <= 0
            )
          ) {
            errEl.textContent =
              "Please enter positive values for all MSMPR inputs.";
            errEl.classList.remove("hidden");
            return;
          }
          if (Cf <= Ceq) {
            errEl.textContent =
              "Feed concentration must be greater than equilibrium solubility to sustain crystallization.";
            errEl.classList.remove("hidden");
            return;
          }
          if (phi >= 0.5) {
            errEl.textContent =
              "Crystal mass fraction ϕ should typically be below 0.4 for practical operation.";
            errEl.classList.remove("hidden");
            return;
          }

          const S = Cf / Ceq;
          const Y = (Cf - Ceq) / Cf;

          const Ltarget_m = Ltarget_um * 1e-6;
          const tau_s = Ltarget_m / (3 * G); // s
          const tau_h = tau_s / 3600; // h
          const V = Q * tau_h; // m3

          const mSlurryDot = rho * Q; // kg/h
          const mCrystDot = phi * mSlurryDot;

          // Estimated mean size from MSMPR correlation (should match target if consistent)
          const Lbar_m = 3 * G * tau_s;
          const Lbar_um = Lbar_m * 1e6;

          document.getElementById("msmpr-S").textContent = fmt(S, "");
          document.getElementById("msmpr-yield").textContent =
            (Y * 100).toFixed(1) + " %";
          document.getElementById("msmpr-mdot").textContent = fmt(
            mCrystDot,
            "kg/h"
          );

          document.getElementById("msmpr-tau").textContent = fmt(tau_h, "h");
          document.getElementById("msmpr-V").textContent = fmt(V, "m³");
          document.getElementById("msmpr-Lbar").textContent = fmt(
            Lbar_um,
            "µm"
          );
        });

      // Initialize with batch tab active
      activateTab("batch");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
